	//存储默认参数
	var defaultOptions = {
	        resize: {
	            standard: 1023
	            , medium: 768
	            , small: 767
	        }
	        , dataAttr: 'spice-data-image-src'
	        , dataLazyAttr: 'spice-data-lazy-src'
	    }

	var defaultFunc = {
	    
	}

	var AutoReplaceImageSrc = function(self, option){
	    var $self  = this
	    $self.opt = $.extend({}, defaultOptions, option)
	    $self.self = self
	    $self.init()
	}

	AutoReplaceImageSrc.prototype = {
	    init: function(){
	        var self = this

	        $(window)
	            .off('resize.spice-autoReplaceImageSrc')
	            .on('resize.spice-autoReplaceImageSrc', function(){
	                self.elem = $( self.self )
	                self.resizeScreenDebounce(  )
	            })
	            .trigger('resize.spice-autoReplaceImageSrc')
	    }
	    , replaceImg: function(media){
	        var self = this
	            , dataLazyAttr = self.opt.dataLazyAttr
	            , dataAttr = self.opt.dataAttr

	        $.each( self.elem, function(i, elem){
	            var $elem = $(elem)
	                , $parent = $elem.parent()
	                , lazySrc = $elem.attr( dataLazyAttr )
	                , isLazy = !!lazySrc
	                , elemSrc = $elem.attr('src')
	                , srcObj = $.parseJSON( $elem.attr(dataAttr) )

	            elemSrc = lazySrc || elemSrc

	            // 当现有图片和 src 相同时不替换图片
	            // 防止重复替换，影响效率
	            src = srcObj[media]
	            if( elemSrc != src ){
	                // 图片切换完成之前触发回调事件
	                $elem.trigger('spice.autoReplaceImageSrc-before');
	                var attrObj = {}
	                // 如果存在延迟加载效果，就直接赋值给延迟的属性
	                // 如果没有延迟效果，则需要将图片整个替换，避免之前的图片缓存导致图片不能及时切换
	                if(isLazy){
	                    attrObj[ dataLazyAttr ] = src
	                    $elem.attr( attrObj )
	                }else{
	                    var $img = $elem.clone(true)
	                    attrObj[ 'src' ] = src
	                    $img.attr( attrObj )
	                    $elem.replaceWith($img)
	                }
	                // 图片切换完成之后触发回调事件
	                $parent.trigger('spice.autoReplaceImageSrc-after');
	            }

	        } )
	        
	    }
	    , resizeScreenDebounce: function(){
	        var self = this
	            /*, srcObj = self.srcObj*/;
	        return spice.resizeScreenDebounce({
	                        resize: self.opt.resize
	                        , callBack: function(media){
	                            self.replaceImg( media )
	                        }
	                    })();
	    }
	}

	module.exports = AutoReplaceImageSrc

