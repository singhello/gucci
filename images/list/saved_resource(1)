	S.isFlashEnabled = function()
	{
	    var hasFlash = false;
	    try
	    {
	        var fo = new ActiveXObject('ShockwaveFlash.ShockwaveFlash');
	        if(fo) hasFlash = true;
	    }
	    catch(e)
	    {
	        if(navigator.mimeTypes ["application/x-shockwave-flash"] != undefined) hasFlash = true;
	    }
	    return hasFlash;
	}
	S.hasFlash = S.isFlashEnabled()

	S.setTel = function(elem,tel){
		if ( $(window).width() <= 767 ) {
			$(elem).attr('href', 'tel:'+ tel)
		}
	}

	S.launchFullscreen = function(element) {
		//此方法不可以在異步任務中執行，否則火狐無法全屏
		if (element.requestFullscreen) {
			element.requestFullscreen();
		} else if (element.mozRequestFullScreen) {
			element.mozRequestFullScreen();
		} else if (element.msRequestFullscreen) {
			element.msRequestFullscreen();
		} else if (element.oRequestFullscreen) {
			element.oRequestFullscreen();
		} else if (element.webkitRequestFullscreen) {
			element.webkitRequestFullScreen();
		} else if (element.webkitEnterFullscreen) {
			element.webkitEnterFullscreen();
		} else {

			// var docHtml = document.documentElement;
			// var docBody = document.body;
			// var videobox = document.getElementById('videobox');
			// var cssText = 'width:100%;height:100%;overflow:hidden;';
			// docHtml.style.cssText = cssText;
			// docBody.style.cssText = cssText;
			// videobox.style.cssText = cssText + ';' + 'margin:0px;padding:0px;';
			document.IsFullScreen = true;

		}
	}
	S.setFullscreenEvents = function(element, fn) {
		element.addEventListener("fullscreenchange", function(e) {
			fn()
		})
		element.addEventListener("mozfullscreenchange", function(e) {
			fn()
		})
		element.addEventListener("webkitfullscreenchange", function(e) {
			fn()
		})
		element.addEventListener("msfullscreenchange", function(e) {
			fn()
		})
	}

	//编辑发送请求的url
	S.rewriteUrl = function(url){
		if ( S.isLocalhost ) {
			return '/data/language/' + S.language + url + '.json'
		} else{
			return url
		}
	}
	S.rewriteJson = function(key,url){
		var o = {}
		if ( key ) {
			o = $.parseJSON( key )
		}else{
			if (url) {
				o = spice.syncXhrGet( url, {}, {dataType: 'json'})
			} else{
				o = ''
			}
		}
		return o
	}
	S.parseJSON = function(data){
		if (!data) {return}
		if ( $.isPlainObject(data) ) {
			return data
		}else{
			return $.parseJSON(data)
		}
	}
	S.addSlot = function(data, slot){
		if ( !data || !slot ) {return false}
		if ( $.isArray(data) ) {
			for (var i = 0; i < data.length; i++) {
				data[i].slot = slot
			}
		} else if( $.isPlainObject(data) ) {
			data.slot = slot
		}
	}

	//ajax方法
	//异步
	S.asyncXhrGet = function(url, data, fn){
		spice.asyncXhrGet( S.base + url , data, {
		    success: function(data, textStatus){
				fn(data, textStatus)
		    }
		    , error: function(XMLHttpRequest, textStatus, errorThrown){
				fn(XMLHttpRequest, textStatus, errorThrown)
		    }
		    , dataType: 'json'
		})
	}
	S.asyncXhrPost = function(url, data, fn){
		spice.asyncXhrPost( S.base + url , data, {
		    success: function(data, textStatus){
				fn(data, textStatus)
		    }
		    , error: function(XMLHttpRequest, textStatus, errorThrown){
				fn(XMLHttpRequest, textStatus, errorThrown)
		    }
		    , dataType: 'json'
		})
	}
	//同步
	S.syncXhrGet = function(url, data, fn){
		var o = spice.syncXhrGet( S.base + url , data, {dataType: 'json'})
		fn(o)
	}
	S.syncXhrPost = function(url, data, fn){
		var o = spice.syncXhrPost( S.base + url , data, {dataType: 'json'})
		fn(o)
	}

	S.lightBox = function (options) {
