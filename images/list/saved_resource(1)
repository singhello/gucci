	S.setMultiLanguage = function(arr){
		if ( !$.isArray(arr) ) {return false}
		var obj = {}
			, commonObj = {}
			, page = "page"
		for (var i = 0; i < arr.length; i++) {
			var key = arr[i]
				// , dami = spice.syncXhr( S.staticbase + '/data/multiLanguage/'+ S.locale + '/' +arr[i]+'.json', {}, {dataType: 'json'})
				// , dami = spice.syncXhr( '/' +arr[i]+'.json', {}, {dataType: 'json'})//無用了
