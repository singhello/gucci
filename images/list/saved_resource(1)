		, 'swipe', 'swipeleft', 'swiperight', 'swipeup', 'swipedown'
		, 'press', 'pressup'
		, 'pinch', 'pinchstart', 'pinchmove', 'pinchend', 'pinchcancel', 'pinchin', 'pinchout'
		, 'rotate', 'rotatestart', 'rotatemove', 'rotateend', 'rotatecancel']
		, function(i, v){
