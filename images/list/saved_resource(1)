/*jshint browser: true, strict: true, immed: true, latedef: true, undef: true, regexdash: false */
(function (undefined) {
"use strict";

var Hex = {},
    decoder;

Hex.decode = function(a) {
    var i;
    if (decoder === undefined) {
        var hex = "0123456789ABCDEF",
            ignore = " \f\n\r\t\u00A0\u2028\u2029";
        decoder = [];
        for (i = 0; i < 16; ++i)
            decoder[hex.charAt(i)] = i;
        hex = hex.toLowerCase();
        for (i = 10; i < 16; ++i)
            decoder[hex.charAt(i)] = i;
        for (i = 0; i < ignore.length; ++i)
            decoder[ignore.charAt(i)] = -1;
    }
    var out = [],
        bits = 0,
        char_count = 0;
    for (i = 0; i < a.length; ++i) {
        var c = a.charAt(i);
        if (c == '=')
            break;
        c = decoder[c];
        if (c == -1)
            continue;
        if (c === undefined)
            throw 'Illegal character at offset ' + i;
        bits |= c;
        if (++char_count >= 2) {
            out[out.length] = bits;
            bits = 0;
            char_count = 0;
        } else {
            bits <<= 4;
        }
    }
    if (char_count)
        throw "Hex encoding incomplete: 4 bits missing";
    return out;
};

// export globals
window.Hex = Hex;
})();// Base64 JavaScript decoder
// Copyright (c) 2008-2013 Lapo Luchini <lapo@lapo.it>

// Permission to use, copy, modify, and/or distribute this software for any
// purpose with or without fee is hereby granted, provided that the above
// copyright notice and this permission notice appear in all copies.
// 
// THE SOFTWARE IS PROVIDED "AS IS" AND THE AUTHOR DISCLAIMS ALL WARRANTIES
// WITH REGARD TO THIS SOFTWARE INCLUDING ALL IMPLIED WARRANTIES OF
// MERCHANTABILITY AND FITNESS. IN NO EVENT SHALL THE AUTHOR BE LIABLE FOR
// ANY SPECIAL, DIRECT, INDIRECT, OR CONSEQUENTIAL DAMAGES OR ANY DAMAGES
// WHATSOEVER RESULTING FROM LOSS OF USE, DATA OR PROFITS, WHETHER IN AN
// ACTION OF CONTRACT, NEGLIGENCE OR OTHER TORTIOUS ACTION, ARISING OUT OF
// OR IN CONNECTION WITH THE USE OR PERFORMANCE OF THIS SOFTWARE.

/*jshint browser: true, strict: true, immed: true, latedef: true, undef: true, regexdash: false */
(function (undefined) {
"use strict";

var Base64 = {},
    decoder;

Base64.decode = function (a) {
    var i;
    if (decoder === undefined) {
        var b64 = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/",
            ignore = "= \f\n\r\t\u00A0\u2028\u2029";
        decoder = [];
        for (i = 0; i < 64; ++i)
            decoder[b64.charAt(i)] = i;
        for (i = 0; i < ignore.length; ++i)
            decoder[ignore.charAt(i)] = -1;
    }
    var out = [];
    var bits = 0, char_count = 0;
    for (i = 0; i < a.length; ++i) {
        var c = a.charAt(i);
        if (c == '=')
            break;
        c = decoder[c];
        if (c == -1)
            continue;
        if (c === undefined)
            throw 'Illegal character at offset ' + i;
        bits |= c;
        if (++char_count >= 4) {
            out[out.length] = (bits >> 16);
            out[out.length] = (bits >> 8) & 0xFF;
            out[out.length] = bits & 0xFF;
            bits = 0;
            char_count = 0;
        } else {
            bits <<= 6;
        }
    }
    switch (char_count) {
      case 1:
        throw "Base64 encoding incomplete: at least 2 bits missing";
      case 2:
        out[out.length] = (bits >> 10);
        break;
      case 3:
        out[out.length] = (bits >> 16);
        out[out.length] = (bits >> 8) & 0xFF;
        break;
    }
    return out;
};

Base64.re = /-----BEGIN [^-]+-----([A-Za-z0-9+\/=\s]+)-----END [^-]+-----|begin-base64[^\n]+\n([A-Za-z0-9+\/=\s]+)====/;
Base64.unarmor = function (a) {
    var m = Base64.re.exec(a);
    if (m) {
        if (m[1])
            a = m[1];
        else if (m[2])
            a = m[2];
        else
            throw "RegExp out of sync";
    }
    return Base64.decode(a);
};

// export globals
window.Base64 = Base64;
})();// ASN.1 JavaScript decoder
// Copyright (c) 2008-2013 Lapo Luchini <lapo@lapo.it>

// Permission to use, copy, modify, and/or distribute this software for any
// purpose with or without fee is hereby granted, provided that the above
// copyright notice and this permission notice appear in all copies.
// 
// THE SOFTWARE IS PROVIDED "AS IS" AND THE AUTHOR DISCLAIMS ALL WARRANTIES
// WITH REGARD TO THIS SOFTWARE INCLUDING ALL IMPLIED WARRANTIES OF
// MERCHANTABILITY AND FITNESS. IN NO EVENT SHALL THE AUTHOR BE LIABLE FOR
// ANY SPECIAL, DIRECT, INDIRECT, OR CONSEQUENTIAL DAMAGES OR ANY DAMAGES
// WHATSOEVER RESULTING FROM LOSS OF USE, DATA OR PROFITS, WHETHER IN AN
// ACTION OF CONTRACT, NEGLIGENCE OR OTHER TORTIOUS ACTION, ARISING OUT OF
// OR IN CONNECTION WITH THE USE OR PERFORMANCE OF THIS SOFTWARE.

/*jshint browser: true, strict: true, immed: true, latedef: true, undef: true, regexdash: false */
/*global oids */
(function (undefined) {
"use strict";

var hardLimit = 100,
    ellipsis = "\u2026",
    DOM = {
        tag: function (tagName, className) {
            var t = document.createElement(tagName);
            t.className = className;
            return t;
        },
        text: function (str) {
            return document.createTextNode(str);
        }
    };

function Stream(enc, pos) {
    if (enc instanceof Stream) {
        this.enc = enc.enc;
        this.pos = enc.pos;
    } else {
        this.enc = enc;
        this.pos = pos;
    }
}
Stream.prototype.get = function (pos) {
    if (pos === undefined)
        pos = this.pos++;
    if (pos >= this.enc.length)
        throw 'Requesting byte offset ' + pos + ' on a stream of length ' + this.enc.length;
    return this.enc[pos];
};
Stream.prototype.hexDigits = "0123456789ABCDEF";
Stream.prototype.hexByte = function (b) {
    return this.hexDigits.charAt((b >> 4) & 0xF) + this.hexDigits.charAt(b & 0xF);
};
Stream.prototype.hexDump = function (start, end, raw) {
    var s = "";
    for (var i = start; i < end; ++i) {
        s += this.hexByte(this.get(i));
        if (raw !== true)
            switch (i & 0xF) {
            case 0x7: s += "  "; break;
            case 0xF: s += "\n"; break;
            default:  s += " ";
            }
    }
    return s;
};
Stream.prototype.parseStringISO = function (start, end) {
    var s = "";
    for (var i = start; i < end; ++i)
        s += String.fromCharCode(this.get(i));
    return s;
};
Stream.prototype.parseStringUTF = function (start, end) {
    var s = "";
    for (var i = start; i < end; ) {
        var c = this.get(i++);
        if (c < 128)
            s += String.fromCharCode(c);
        else if ((c > 191) && (c < 224))
            s += String.fromCharCode(((c & 0x1F) << 6) | (this.get(i++) & 0x3F));
        else
            s += String.fromCharCode(((c & 0x0F) << 12) | ((this.get(i++) & 0x3F) << 6) | (this.get(i++) & 0x3F));
    }
    return s;
};
Stream.prototype.parseStringBMP = function (start, end) {
    var str = ""
    for (var i = start; i < end; i += 2) {
        var high_byte = this.get(i);
        var low_byte = this.get(i + 1);
        str += String.fromCharCode( (high_byte << 8) + low_byte );
    }

    return str;
};
Stream.prototype.reTime = /^((?:1[89]|2\d)?\d\d)(0[1-9]|1[0-2])(0[1-9]|[12]\d|3[01])([01]\d|2[0-3])(?:([0-5]\d)(?:([0-5]\d)(?:[.,](\d{1,3}))?)?)?(Z|[-+](?:[0]\d|1[0-2])([0-5]\d)?)?$/;
Stream.prototype.parseTime = function (start, end) {
    var s = this.parseStringISO(start, end),
        m = this.reTime.exec(s);
    if (!m)
        return "Unrecognized time: " + s;
    s = m[1] + "-" + m[2] + "-" + m[3] + " " + m[4];
    if (m[5]) {
        s += ":" + m[5];
        if (m[6]) {
            s += ":" + m[6];
            if (m[7])
                s += "." + m[7];
        }
    }
    if (m[8]) {
        s += " UTC";
        if (m[8] != 'Z') {
            s += m[8];
            if (m[9])
                s += ":" + m[9];
        }
    }
    return s;
};
Stream.prototype.parseInteger = function (start, end) {
    //TODO support negative numbers
    var len = end - start;
    if (len > 4) {
        len <<= 3;
        var s = this.get(start);
        if (s === 0)
            len -= 8;
        else
            while (s < 128) {
                s <<= 1;
                --len;
            }
        return "(" + len + " bit)";
    }
    var n = 0;
    for (var i = start; i < end; ++i)
        n = (n << 8) | this.get(i);
    return n;
};
Stream.prototype.parseBitString = function (start, end) {
    var unusedBit = this.get(start),
        lenBit = ((end - start - 1) << 3) - unusedBit,
        s = "(" + lenBit + " bit)";
    if (lenBit <= 20) {
        var skip = unusedBit;
        s += " ";
        for (var i = end - 1; i > start; --i) {
            var b = this.get(i);
            for (var j = skip; j < 8; ++j)
                s += (b >> j) & 1 ? "1" : "0";
            skip = 0;
        }
    }
    return s;
};
Stream.prototype.parseOctetString = function (start, end) {
    var len = end - start,
        s = "(" + len + " byte) ";
    if (len > hardLimit)
        end = start + hardLimit;
    for (var i = start; i < end; ++i)
        s += this.hexByte(this.get(i)); //TODO: also try Latin1?
    if (len > hardLimit)
        s += ellipsis;
    return s;
};
Stream.prototype.parseOID = function (start, end) {
    var s = '',
        n = 0,
        bits = 0;
    for (var i = start; i < end; ++i) {
        var v = this.get(i);
        n = (n << 7) | (v & 0x7F);
        bits += 7;
        if (!(v & 0x80)) { // finished
            if (s === '') {
                var m = n < 80 ? n < 40 ? 0 : 1 : 2;
                s = m + "." + (n - m * 40);
            } else
                s += "." + ((bits >= 31) ? "bigint" : n);
            n = bits = 0;
        }
    }
    return s;
};

function ASN1(stream, header, length, tag, sub) {
    this.stream = stream;
    this.header = header;
    this.length = length;
    this.tag = tag;
    this.sub = sub;
}
ASN1.prototype.typeName = function () {
    if (this.tag === undefined)
        return "unknown";
    var tagClass = this.tag >> 6,
        tagConstructed = (this.tag >> 5) & 1,
        tagNumber = this.tag & 0x1F;
    switch (tagClass) {
    case 0: // universal
        switch (tagNumber) {
        case 0x00: return "EOC";
        case 0x01: return "BOOLEAN";
        case 0x02: return "INTEGER";
        case 0x03: return "BIT_STRING";
        case 0x04: return "OCTET_STRING";
        case 0x05: return "NULL";
        case 0x06: return "OBJECT_IDENTIFIER";
        case 0x07: return "ObjectDescriptor";
        case 0x08: return "EXTERNAL";
        case 0x09: return "REAL";
        case 0x0A: return "ENUMERATED";
        case 0x0B: return "EMBEDDED_PDV";
        case 0x0C: return "UTF8String";
        case 0x10: return "SEQUENCE";
        case 0x11: return "SET";
        case 0x12: return "NumericString";
        case 0x13: return "PrintableString"; // ASCII subset
        case 0x14: return "TeletexString"; // aka T61String
        case 0x15: return "VideotexString";
        case 0x16: return "IA5String"; // ASCII
        case 0x17: return "UTCTime";
        case 0x18: return "GeneralizedTime";
        case 0x19: return "GraphicString";
        case 0x1A: return "VisibleString"; // ASCII subset
        case 0x1B: return "GeneralString";
        case 0x1C: return "UniversalString";
        case 0x1E: return "BMPString";
        default:   return "Universal_" + tagNumber.toString(16);
        }
    case 1: return "Application_" + tagNumber.toString(16);
    case 2: return "[" + tagNumber + "]"; // Context
    case 3: return "Private_" + tagNumber.toString(16);
    }
};
ASN1.prototype.reSeemsASCII = /^[ -~]+$/;
ASN1.prototype.content = function () {
    if (this.tag === undefined)
        return null;
    var tagClass = this.tag >> 6,
        tagNumber = this.tag & 0x1F,
        content = this.posContent(),
        len = Math.abs(this.length);
    if (tagClass !== 0) { // universal
        if (this.sub !== null)
            return "(" + this.sub.length + " elem)";
        //TODO: TRY TO PARSE ASCII STRING
        var s = this.stream.parseStringISO(content, content + Math.min(len, hardLimit));
        if (this.reSeemsASCII.test(s))
            return s.substring(0, 2 * hardLimit) + ((s.length > 2 * hardLimit) ? ellipsis : "");
        else
            return this.stream.parseOctetString(content, content + len);
    }
    switch (tagNumber) {
    case 0x01: // BOOLEAN
        return (this.stream.get(content) === 0) ? "false" : "true";
    case 0x02: // INTEGER
        return this.stream.parseInteger(content, content + len);
    case 0x03: // BIT_STRING
        return this.sub ? "(" + this.sub.length + " elem)" :
            this.stream.parseBitString(content, content + len);
    case 0x04: // OCTET_STRING
        return this.sub ? "(" + this.sub.length + " elem)" :
            this.stream.parseOctetString(content, content + len);
    //case 0x05: // NULL
    case 0x06: // OBJECT_IDENTIFIER
        return this.stream.parseOID(content, content + len);
    //case 0x07: // ObjectDescriptor
    //case 0x08: // EXTERNAL
    //case 0x09: // REAL
    //case 0x0A: // ENUMERATED
    //case 0x0B: // EMBEDDED_PDV
    case 0x10: // SEQUENCE
    case 0x11: // SET
        return "(" + this.sub.length + " elem)";
    case 0x0C: // UTF8String
        return this.stream.parseStringUTF(content, content + len);
    case 0x12: // NumericString
    case 0x13: // PrintableString
    case 0x14: // TeletexString
    case 0x15: // VideotexString
    case 0x16: // IA5String
    //case 0x19: // GraphicString
    case 0x1A: // VisibleString
    //case 0x1B: // GeneralString
    //case 0x1C: // UniversalString
        return this.stream.parseStringISO(content, content + len);
    case 0x1E: // BMPString
        return this.stream.parseStringBMP(content, content + len);
    case 0x17: // UTCTime
    case 0x18: // GeneralizedTime
        return this.stream.parseTime(content, content + len);
    }
    return null;
};
ASN1.prototype.toString = function () {
    return this.typeName() + "@" + this.stream.pos + "[header:" + this.header + ",length:" + this.length + ",sub:" + ((this.sub === null) ? 'null' : this.sub.length) + "]";
};
ASN1.prototype.print = function (indent) {
    if (indent === undefined) indent = '';
    document.writeln(indent + this);
    if (this.sub !== null) {
        indent += '  ';
        for (var i = 0, max = this.sub.length; i < max; ++i)
            this.sub[i].print(indent);
    }
};
ASN1.prototype.toPrettyString = function (indent) {
    if (indent === undefined) indent = '';
    var s = indent + this.typeName() + " @" + this.stream.pos;
    if (this.length >= 0)
        s += "+";
    s += this.length;
    if (this.tag & 0x20)
        s += " (constructed)";
    else if (((this.tag == 0x03) || (this.tag == 0x04)) && (this.sub !== null))
        s += " (encapsulates)";
    s += "\n";
    if (this.sub !== null) {
        indent += '  ';
        for (var i = 0, max = this.sub.length; i < max; ++i)
            s += this.sub[i].toPrettyString(indent);
    }
    return s;
};
ASN1.prototype.toDOM = function () {
    var node = DOM.tag("div", "node");
    node.asn1 = this;
    var head = DOM.tag("div", "head");
    var s = this.typeName().replace(/_/g, " ");
    head.innerHTML = s;
    var content = this.content();
    if (content !== null) {
        content = String(content).replace(/</g, "&lt;");
        var preview = DOM.tag("span", "preview");
        preview.appendChild(DOM.text(content));
        head.appendChild(preview);
    }
    node.appendChild(head);
    this.node = node;
    this.head = head;
    var value = DOM.tag("div", "value");
    s = "Offset: " + this.stream.pos + "<br/>";
    s += "Length: " + this.header + "+";
    if (this.length >= 0)
        s += this.length;
    else
        s += (-this.length) + " (undefined)";
    if (this.tag & 0x20)
        s += "<br/>(constructed)";
    else if (((this.tag == 0x03) || (this.tag == 0x04)) && (this.sub !== null))
        s += "<br/>(encapsulates)";
    //TODO if (this.tag == 0x03) s += "Unused bits: "
    if (content !== null) {
        s += "<br/>Value:<br/><b>" + content + "</b>";
        if ((typeof oids === 'object') && (this.tag == 0x06)) {
            var oid = oids[content];
            if (oid) {
                if (oid.d) s += "<br/>" + oid.d;
                if (oid.c) s += "<br/>" + oid.c;
                if (oid.w) s += "<br/>(warning!)";
            }
        }
    }
    value.innerHTML = s;
    node.appendChild(value);
    var sub = DOM.tag("div", "sub");
    if (this.sub !== null) {
        for (var i = 0, max = this.sub.length; i < max; ++i)
            sub.appendChild(this.sub[i].toDOM());
    }
    node.appendChild(sub);
    head.onclick = function () {
        node.className = (node.className == "node collapsed") ? "node" : "node collapsed";
    };
    return node;
};
ASN1.prototype.posStart = function () {
    return this.stream.pos;
};
ASN1.prototype.posContent = function () {
    return this.stream.pos + this.header;
};
ASN1.prototype.posEnd = function () {
    return this.stream.pos + this.header + Math.abs(this.length);
};
ASN1.prototype.fakeHover = function (current) {
    this.node.className += " hover";
    if (current)
        this.head.className += " hover";
};
ASN1.prototype.fakeOut = function (current) {
    var re = / ?hover/;
    this.node.className = this.node.className.replace(re, "");
    if (current)
        this.head.className = this.head.className.replace(re, "");
};
ASN1.prototype.toHexDOM_sub = function (node, className, stream, start, end) {
    if (start >= end)
        return;
    var sub = DOM.tag("span", className);
    sub.appendChild(DOM.text(
        stream.hexDump(start, end)));
    node.appendChild(sub);
};
ASN1.prototype.toHexDOM = function (root) {
    var node = DOM.tag("span", "hex");
    if (root === undefined) root = node;
    this.head.hexNode = node;
    this.head.onmouseover = function () { this.hexNode.className = "hexCurrent"; };
    this.head.onmouseout  = function () { this.hexNode.className = "hex"; };
    node.asn1 = this;
    node.onmouseover = function () {
        var current = !root.selected;
        if (current) {
            root.selected = this.asn1;
            this.className = "hexCurrent";
        }
        this.asn1.fakeHover(current);
    };
    node.onmouseout  = function () {
        var current = (root.selected == this.asn1);
        this.asn1.fakeOut(current);
        if (current) {
            root.selected = null;
            this.className = "hex";
        }
    };
    this.toHexDOM_sub(node, "tag", this.stream, this.posStart(), this.posStart() + 1);
    this.toHexDOM_sub(node, (this.length >= 0) ? "dlen" : "ulen", this.stream, this.posStart() + 1, this.posContent());
    if (this.sub === null)
        node.appendChild(DOM.text(
            this.stream.hexDump(this.posContent(), this.posEnd())));
    else if (this.sub.length > 0) {
        var first = this.sub[0];
        var last = this.sub[this.sub.length - 1];
        this.toHexDOM_sub(node, "intro", this.stream, this.posContent(), first.posStart());
        for (var i = 0, max = this.sub.length; i < max; ++i)
            node.appendChild(this.sub[i].toHexDOM(root));
        this.toHexDOM_sub(node, "outro", this.stream, last.posEnd(), this.posEnd());
    }
    return node;
};
ASN1.prototype.toHexString = function (root) {
    return this.stream.hexDump(this.posStart(), this.posEnd(), true);
};
ASN1.decodeLength = function (stream) {
    var buf = stream.get(),
        len = buf & 0x7F;
    if (len == buf)
        return len;
    if (len > 3)
        throw "Length over 24 bits not supported at position " + (stream.pos - 1);
    if (len === 0)
        return -1; // undefined
    buf = 0;
    for (var i = 0; i < len; ++i)
        buf = (buf << 8) | stream.get();
    return buf;
};
ASN1.hasContent = function (tag, len, stream) {
    if (tag & 0x20) // constructed
        return true;
    if ((tag < 0x03) || (tag > 0x04))
        return false;
    var p = new Stream(stream);
    if (tag == 0x03) p.get(); // BitString unused bits, must be in [0, 7]
    var subTag = p.get();
    if ((subTag >> 6) & 0x01) // not (universal or context)
        return false;
    try {
        var subLength = ASN1.decodeLength(p);
        return ((p.pos - stream.pos) + subLength == len);
    } catch (exception) {
        return false;
    }
};
ASN1.decode = function (stream) {
    if (!(stream instanceof Stream))
        stream = new Stream(stream, 0);
    var streamStart = new Stream(stream),
        tag = stream.get(),
        len = ASN1.decodeLength(stream),
        header = stream.pos - streamStart.pos,
        sub = null;
    if (ASN1.hasContent(tag, len, stream)) {
        // it has content, so we decode it
        var start = stream.pos;
        if (tag == 0x03) stream.get(); // skip BitString unused bits, must be in [0, 7]
        sub = [];
        if (len >= 0) {
            // definite length
            var end = start + len;
            while (stream.pos < end)
                sub[sub.length] = ASN1.decode(stream);
            if (stream.pos != end)
                throw "Content size is not correct for container starting at offset " + start;
        } else {
            // undefined length
            try {
                for (;;) {
                    var s = ASN1.decode(stream);
                    if (s.tag === 0)
                        break;
                    sub[sub.length] = s;
                }
                len = start - stream.pos;
            } catch (e) {
                throw "Exception while decoding undefined length content: " + e;
            }
        }
    } else
        stream.pos += len; // skip content
    return new ASN1(streamStart, header, len, tag, sub);
};
ASN1.test = function () {
    var test = [
        { value: [0x27],                   expected: 0x27     },
        { value: [0x81, 0xC9],             expected: 0xC9     },
        { value: [0x83, 0xFE, 0xDC, 0xBA], expected: 0xFEDCBA }
    ];
    for (var i = 0, max = test.length; i < max; ++i) {
        var pos = 0,
            stream = new Stream(test[i].value, 0),
            res = ASN1.decodeLength(stream);
        if (res != test[i].expected)
            document.write("In test[" + i + "] expected " + test[i].expected + " got " + res + "\n");
    }
};

// export globals
window.ASN1 = ASN1;
})();/**
 * Retrieve the hexadecimal value (as a string) of the current ASN.1 element
 * @returns {string}
 * @public
 */
ASN1.prototype.getHexStringValue = function () {
  var hexString = this.toHexString();
  var offset = this.header * 2;
  var length = this.length * 2;
  return hexString.substr(offset, length);
};

/**
 * Method to parse a pem encoded string containing both a public or private key.
 * The method will translate the pem encoded string in a der encoded string and
 * will parse private key and public key parameters. This method accepts public key
 * in the rsaencryption pkcs #1 format (oid: 1.2.840.113549.1.1.1).
 *
 * @todo Check how many rsa formats use the same format of pkcs #1.
 *
 * The format is defined as:
 * PublicKeyInfo ::= SEQUENCE {
 *   algorithm       AlgorithmIdentifier,
 *   PublicKey       BIT STRING
 * }
 * Where AlgorithmIdentifier is:
 * AlgorithmIdentifier ::= SEQUENCE {
 *   algorithm       OBJECT IDENTIFIER,     the OID of the enc algorithm
 *   parameters      ANY DEFINED BY algorithm OPTIONAL (NULL for PKCS #1)
 * }
 * and PublicKey is a SEQUENCE encapsulated in a BIT STRING
 * RSAPublicKey ::= SEQUENCE {
 *   modulus           INTEGER,  -- n
 *   publicExponent    INTEGER   -- e
 * }
 * it's possible to examine the structure of the keys obtained from openssl using
 * an asn.1 dumper as the one used here to parse the components: http://lapo.it/asn1js/
 * @argument {string} pem the pem encoded string, can include the BEGIN/END header/footer
 * @private
 */
RSAKey.prototype.parseKey = function (pem) {
  try {
    var modulus = 0;
    var public_exponent = 0;
    var reHex = /^\s*(?:[0-9A-Fa-f][0-9A-Fa-f]\s*)+$/;
    var der = reHex.test(pem) ? Hex.decode(pem) : Base64.unarmor(pem);
    var asn1 = ASN1.decode(der);

    //Fixes a bug with OpenSSL 1.0+ private keys
    if(asn1.sub.length === 3){
        asn1 = asn1.sub[2].sub[0];
    }
    if (asn1.sub.length === 9) {

      // Parse the private key.
      modulus = asn1.sub[1].getHexStringValue(); //bigint
      this.n = parseBigInt(modulus, 16);

      public_exponent = asn1.sub[2].getHexStringValue(); //int
      this.e = parseInt(public_exponent, 16);

      var private_exponent = asn1.sub[3].getHexStringValue(); //bigint
      this.d = parseBigInt(private_exponent, 16);

      var prime1 = asn1.sub[4].getHexStringValue(); //bigint
      this.p = parseBigInt(prime1, 16);

      var prime2 = asn1.sub[5].getHexStringValue(); //bigint
      this.q = parseBigInt(prime2, 16);

      var exponent1 = asn1.sub[6].getHexStringValue(); //bigint
      this.dmp1 = parseBigInt(exponent1, 16);

      var exponent2 = asn1.sub[7].getHexStringValue(); //bigint
      this.dmq1 = parseBigInt(exponent2, 16);

      var coefficient = asn1.sub[8].getHexStringValue(); //bigint
      this.coeff = parseBigInt(coefficient, 16);

    }
    else if (asn1.sub.length === 2) {

      // Parse the public key.
      var bit_string = asn1.sub[1];
      var sequence = bit_string.sub[0];

      modulus = sequence.sub[0].getHexStringValue();
      this.n = parseBigInt(modulus, 16);
      public_exponent = sequence.sub[1].getHexStringValue();
      this.e = parseInt(public_exponent, 16);

    }
    else {
      return false;
    }
    return true;
  }
  catch (ex) {
    return false;
  }
};

/**
 * Translate rsa parameters in a hex encoded string representing the rsa key.
 *
 * The translation follow the ASN.1 notation :
 * RSAPrivateKey ::= SEQUENCE {
 *   version           Version,
 *   modulus           INTEGER,  -- n
 *   publicExponent    INTEGER,  -- e
 *   privateExponent   INTEGER,  -- d
 *   prime1            INTEGER,  -- p
 *   prime2            INTEGER,  -- q
 *   exponent1         INTEGER,  -- d mod (p1)
 *   exponent2         INTEGER,  -- d mod (q-1)
 *   coefficient       INTEGER,  -- (inverse of q) mod p
 * }
 * @returns {string}  DER Encoded String representing the rsa private key
 * @private
 */
RSAKey.prototype.getPrivateBaseKey = function () {
  var options = {
    'array': [
      new KJUR.asn1.DERInteger({'int': 0}),
      new KJUR.asn1.DERInteger({'bigint': this.n}),
      new KJUR.asn1.DERInteger({'int': this.e}),
      new KJUR.asn1.DERInteger({'bigint': this.d}),
      new KJUR.asn1.DERInteger({'bigint': this.p}),
      new KJUR.asn1.DERInteger({'bigint': this.q}),
      new KJUR.asn1.DERInteger({'bigint': this.dmp1}),
      new KJUR.asn1.DERInteger({'bigint': this.dmq1}),
      new KJUR.asn1.DERInteger({'bigint': this.coeff})
    ]
  };
  var seq = new KJUR.asn1.DERSequence(options);
  return seq.getEncodedHex();
};

/**
 * base64 (pem) encoded version of the DER encoded representation
 * @returns {string} pem encoded representation without header and footer
 * @public
 */
RSAKey.prototype.getPrivateBaseKeyB64 = function () {
  return hex2b64(this.getPrivateBaseKey());
};

/**
 * Translate rsa parameters in a hex encoded string representing the rsa public key.
 * The representation follow the ASN.1 notation :
 * PublicKeyInfo ::= SEQUENCE {
 *   algorithm       AlgorithmIdentifier,
 *   PublicKey       BIT STRING
 * }
 * Where AlgorithmIdentifier is:
 * AlgorithmIdentifier ::= SEQUENCE {
 *   algorithm       OBJECT IDENTIFIER,     the OID of the enc algorithm
 *   parameters      ANY DEFINED BY algorithm OPTIONAL (NULL for PKCS #1)
 * }
 * and PublicKey is a SEQUENCE encapsulated in a BIT STRING
 * RSAPublicKey ::= SEQUENCE {
 *   modulus           INTEGER,  -- n
 *   publicExponent    INTEGER   -- e
 * }
 * @returns {string} DER Encoded String representing the rsa public key
 * @private
 */
RSAKey.prototype.getPublicBaseKey = function () {
  var options = {
    'array': [
      new KJUR.asn1.DERObjectIdentifier({'oid': '1.2.840.113549.1.1.1'}), //RSA Encryption pkcs #1 oid
      new KJUR.asn1.DERNull()
    ]
  };
  var first_sequence = new KJUR.asn1.DERSequence(options);

  options = {
    'array': [
      new KJUR.asn1.DERInteger({'bigint': this.n}),
      new KJUR.asn1.DERInteger({'int': this.e})
    ]
  };
  var second_sequence = new KJUR.asn1.DERSequence(options);

  options = {
    'hex': '00' + second_sequence.getEncodedHex()
  };
  var bit_string = new KJUR.asn1.DERBitString(options);

  options = {
    'array': [
      first_sequence,
      bit_string
    ]
  };
  var seq = new KJUR.asn1.DERSequence(options);
  return seq.getEncodedHex();
};

/**
 * base64 (pem) encoded version of the DER encoded representation
 * @returns {string} pem encoded representation without header and footer
 * @public
 */
RSAKey.prototype.getPublicBaseKeyB64 = function () {
  return hex2b64(this.getPublicBaseKey());
};

/**
 * wrap the string in block of width chars. The default value for rsa keys is 64
 * characters.
 * @param {string} str the pem encoded string without header and footer
 * @param {Number} [width=64] - the length the string has to be wrapped at
 * @returns {string}
 * @private
 */
RSAKey.prototype.wordwrap = function (str, width) {
  width = width || 64;
  if (!str) {
    return str;
  }
  var regex = '(.{1,' + width + '})( +|$\n?)|(.{1,' + width + '})';
  return str.match(RegExp(regex, 'g')).join('\n');
};

/**
 * Retrieve the pem encoded private key
 * @returns {string} the pem encoded private key with header/footer
 * @public
 */
RSAKey.prototype.getPrivateKey = function () {
  var key = "-----BEGIN RSA PRIVATE KEY-----\n";
  key += this.wordwrap(this.getPrivateBaseKeyB64()) + "\n";
  key += "-----END RSA PRIVATE KEY-----";
  return key;
};

/**
 * Retrieve the pem encoded public key
 * @returns {string} the pem encoded public key with header/footer
 * @public
 */
RSAKey.prototype.getPublicKey = function () {
  var key = "-----BEGIN PUBLIC KEY-----\n";
  key += this.wordwrap(this.getPublicBaseKeyB64()) + "\n";
  key += "-----END PUBLIC KEY-----";
  return key;
};

/**
 * Check if the object contains the necessary parameters to populate the rsa modulus
 * and public exponent parameters.
 * @param {Object} [obj={}] - An object that may contain the two public key
 * parameters
 * @returns {boolean} true if the object contains both the modulus and the public exponent
 * properties (n and e)
 * @todo check for types of n and e. N should be a parseable bigInt object, E should
 * be a parseable integer number
 * @private
 */
RSAKey.prototype.hasPublicKeyProperty = function (obj) {
  obj = obj || {};
  return (
    obj.hasOwnProperty('n') &&
    obj.hasOwnProperty('e')
  );
};

/**
 * Check if the object contains ALL the parameters of an RSA key.
 * @param {Object} [obj={}] - An object that may contain nine rsa key
 * parameters
 * @returns {boolean} true if the object contains all the parameters needed
 * @todo check for types of the parameters all the parameters but the public exponent
 * should be parseable bigint objects, the public exponent should be a parseable integer number
 * @private
 */
RSAKey.prototype.hasPrivateKeyProperty = function (obj) {
  obj = obj || {};
  return (
    obj.hasOwnProperty('n') &&
    obj.hasOwnProperty('e') &&
    obj.hasOwnProperty('d') &&
    obj.hasOwnProperty('p') &&
    obj.hasOwnProperty('q') &&
    obj.hasOwnProperty('dmp1') &&
    obj.hasOwnProperty('dmq1') &&
    obj.hasOwnProperty('coeff')
  );
};

/**
 * Parse the properties of obj in the current rsa object. Obj should AT LEAST
 * include the modulus and public exponent (n, e) parameters.
 * @param {Object} obj - the object containing rsa parameters
 * @private
 */
RSAKey.prototype.parsePropertiesFrom = function (obj) {
  this.n = obj.n;
  this.e = obj.e;

  if (obj.hasOwnProperty('d')) {
    this.d = obj.d;
    this.p = obj.p;
    this.q = obj.q;
    this.dmp1 = obj.dmp1;
    this.dmq1 = obj.dmq1;
    this.coeff = obj.coeff;
  }
};

/**
 * Create a new JSEncryptRSAKey that extends Tom Wu's RSA key object.
 * This object is just a decorator for parsing the key parameter
 * @param {string|Object} key - The key in string format, or an object containing
 * the parameters needed to build a RSAKey object.
 * @constructor
 */
var JSEncryptRSAKey = function (key) {
  // Call the super constructor.
  RSAKey.call(this);
  // If a key key was provided.
  if (key) {
    // If this is a string...
    if (typeof key === 'string') {
      this.parseKey(key);
    }
    else if (
      this.hasPrivateKeyProperty(key) ||
      this.hasPublicKeyProperty(key)
    ) {
      // Set the values for the key.
      this.parsePropertiesFrom(key);
    }
  }
};

// Derive from RSAKey.
JSEncryptRSAKey.prototype = new RSAKey();

// Reset the contructor.
JSEncryptRSAKey.prototype.constructor = JSEncryptRSAKey;


/**
 *
 * @param {Object} [options = {}] - An object to customize JSEncrypt behaviour
 * possible parameters are:
 * - default_key_size        {number}  default: 1024 the key size in bit
 * - default_public_exponent {string}  default: '010001' the hexadecimal representation of the public exponent
 * - log                     {boolean} default: false whether log warn/error or not
 * @constructor
 */
var JSEncrypt = function (options) {
  options = options || {};
  this.default_key_size = parseInt(options.default_key_size) || 1024;
  this.default_public_exponent = options.default_public_exponent || '010001'; //65537 default openssl public exponent for rsa key type
  this.log = options.log || false;
  // The private and public key.
  this.key = null;
};

/**
 * Method to set the rsa key parameter (one method is enough to set both the public
 * and the private key, since the private key contains the public key paramenters)
 * Log a warning if logs are enabled
 * @param {Object|string} key the pem encoded string or an object (with or without header/footer)
 * @public
 */
JSEncrypt.prototype.setKey = function (key) {
  if (this.log && this.key) {
    console.warn('A key was already set, overriding existing.');
  }
  this.key = new JSEncryptRSAKey(key);
};

/**
 * Proxy method for setKey, for api compatibility
 * @see setKey
 * @public
 */
JSEncrypt.prototype.setPrivateKey = function (privkey) {
  // Create the key.
  this.setKey(privkey);
};

/**
 * Proxy method for setKey, for api compatibility
 * @see setKey
 * @public
 */
JSEncrypt.prototype.setPublicKey = function (pubkey) {
  // Sets the public key.
  this.setKey(pubkey);
};

/**
 * Proxy method for RSAKey object's decrypt, decrypt the string using the private
 * components of the rsa key object. Note that if the object was not set will be created
 * on the fly (by the getKey method) using the parameters passed in the JSEncrypt constructor
 * @param {string} string base64 encoded crypted string to decrypt
 * @return {string} the decrypted string
 * @public
 */
JSEncrypt.prototype.decrypt = function (string) {
  // Return the decrypted string.
  try {
    return this.getKey().decrypt(b64tohex(string));
  }
  catch (ex) {
    return false;
  }
};

/**
 * Proxy method for RSAKey object's encrypt, encrypt the string using the public
 * components of the rsa key object. Note that if the object was not set will be created
 * on the fly (by the getKey method) using the parameters passed in the JSEncrypt constructor
 * @param {string} string the string to encrypt
 * @return {string} the encrypted string encoded in base64
 * @public
 */
JSEncrypt.prototype.encrypt = function (string) {
  // Return the encrypted string.
  try {
    return hex2b64(this.getKey().encrypt(string));
  }
  catch (ex) {
    return false;
  }
};

/**
 * Getter for the current JSEncryptRSAKey object. If it doesn't exists a new object
 * will be created and returned
 * @param {callback} [cb] the callback to be called if we want the key to be generated
 * in an async fashion
 * @returns {JSEncryptRSAKey} the JSEncryptRSAKey object
 * @public
 */
JSEncrypt.prototype.getKey = function (cb) {
  // Only create new if it does not exist.
  if (!this.key) {
    // Get a new private key.
    this.key = new JSEncryptRSAKey();
    if (cb && {}.toString.call(cb) === '[object Function]') {
      this.key.generateAsync(this.default_key_size, this.default_public_exponent, cb);
      return;
    }
    // Generate the key.
    this.key.generate(this.default_key_size, this.default_public_exponent);
  }
  return this.key;
};

/**
 * Returns the pem encoded representation of the private key
 * If the key doesn't exists a new key will be created
 * @returns {string} pem encoded representation of the private key WITH header and footer
 * @public
 */
JSEncrypt.prototype.getPrivateKey = function () {
  // Return the private representation of this key.
  return this.getKey().getPrivateKey();
};

/**
 * Returns the pem encoded representation of the private key
 * If the key doesn't exists a new key will be created
 * @returns {string} pem encoded representation of the private key WITHOUT header and footer
 * @public
 */
JSEncrypt.prototype.getPrivateKeyB64 = function () {
  // Return the private representation of this key.
  return this.getKey().getPrivateBaseKeyB64();
};


/**
 * Returns the pem encoded representation of the public key
 * If the key doesn't exists a new key will be created
 * @returns {string} pem encoded representation of the public key WITH header and footer
 * @public
 */
JSEncrypt.prototype.getPublicKey = function () {
  // Return the private representation of this key.
  return this.getKey().getPublicKey();
};

/**
 * Returns the pem encoded representation of the public key
 * If the key doesn't exists a new key will be created
 * @returns {string} pem encoded representation of the public key WITHOUT header and footer
 * @public
 */
JSEncrypt.prototype.getPublicKeyB64 = function () {
  // Return the private representation of this key.
  return this.getKey().getPublicBaseKeyB64();
};

exports.JSEncrypt = JSEncrypt;
})(JSEncryptExports);
var JSEncrypt = JSEncryptExports.JSEncrypt;

/*!
 * JavaScript Cookie v2.1.3
 * https://github.com/js-cookie/js-cookie
 *
 * Copyright 2006, 2015 Klaus Hartl & Fagner Brack
 * Released under the MIT license
 */
;(function (factory) {
	var registeredInModuleLoader = false;
	if (typeof define === 'function' && define.amd) {
		define(factory);
		registeredInModuleLoader = true;
	}
	if (typeof exports === 'object') {
		module.exports = factory();
		registeredInModuleLoader = true;
	}
	if (!registeredInModuleLoader) {
		var OldCookies = window.Cookies;
		var api = window.Cookies = factory();
		api.noConflict = function () {
			window.Cookies = OldCookies;
			return api;
		};
	}
}(function () {
	function extend () {
		var i = 0;
		var result = {};
		for (; i < arguments.length; i++) {
			var attributes = arguments[ i ];
			for (var key in attributes) {
				result[key] = attributes[key];
			}
		}
		return result;
	}

	function init (converter) {
		function api (key, value, attributes) {
			var result;
			if (typeof document === 'undefined') {
				return;
			}

			// Write

			if (arguments.length > 1) {
				attributes = extend({
					path: '/'
				}, api.defaults, attributes);

				if (typeof attributes.expires === 'number') {
					var expires = new Date();
					expires.setMilliseconds(expires.getMilliseconds() + attributes.expires * 864e+5);
					attributes.expires = expires;
				}

				try {
					result = JSON.stringify(value);
					if (/^[\{\[]/.test(result)) {
						value = result;
					}
				} catch (e) {}

				if (!converter.write) {
					value = encodeURIComponent(String(value))
						.replace(/%(23|24|26|2B|3A|3C|3E|3D|2F|3F|40|5B|5D|5E|60|7B|7D|7C)/g, decodeURIComponent);
				} else {
					value = converter.write(value, key);
				}

				key = encodeURIComponent(String(key));
				key = key.replace(/%(23|24|26|2B|5E|60|7C)/g, decodeURIComponent);
				key = key.replace(/[\(\)]/g, escape);

				return (document.cookie = [
					key, '=', value,
					attributes.expires ? '; expires=' + attributes.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
					attributes.path ? '; path=' + attributes.path : '',
					attributes.domain ? '; domain=' + attributes.domain : '',
					attributes.secure ? '; secure' : ''
				].join(''));
			}

			// Read

			if (!key) {
				result = {};
			}

			// To prevent the for loop in the first place assign an empty array
			// in case there are no cookies at all. Also prevents odd result when
			// calling "get()"
			var cookies = document.cookie ? document.cookie.split('; ') : [];
			var rdecode = /(%[0-9A-Z]{2})+/g;
			var i = 0;

			for (; i < cookies.length; i++) {
				var parts = cookies[i].split('=');
				var cookie = parts.slice(1).join('=');

				if (cookie.charAt(0) === '"') {
					cookie = cookie.slice(1, -1);
				}

				try {
					var name = parts[0].replace(rdecode, decodeURIComponent);
					cookie = converter.read ?
						converter.read(cookie, name) : converter(cookie, name) ||
						cookie.replace(rdecode, decodeURIComponent);

					if (this.json) {
						try {
							cookie = JSON.parse(cookie);
						} catch (e) {}
					}

					if (key === name) {
						result = cookie;
						break;
					}

					if (!key) {
						result[name] = cookie;
					}
				} catch (e) {}
			}

			return result;
		}

		api.set = api;
		api.get = function (key) {
			return api.call(api, key);
		};
		api.getJSON = function () {
			return api.apply({
				json: true
			}, [].slice.call(arguments));
		};
		api.defaults = {};

		api.remove = function (key, attributes) {
			api(key, '', extend(attributes, {
				expires: -1
			}));
		};

		api.withConverter = init;

		return api;
	}

	return init(function () {});
}));


/* 验证码切换 */
function changeCode(e){
	var src = e.attr('src').split('?')[0];
	e.attr('src', src + '?' + Math.random());
}


/**
 * 判断 一个选择器文本框 是否填写了值
 * 
 * @param selector
 * @returns {Boolean}
 */
function isInputEmpty(selector){
	var selectorInput = $(selector);
	
	// 选择器上面的
	var str = $.trim(selectorInput.val());
	
	if (str != undefined && str != null && str != "" && str != "undefined"){
		return false;
	} else{
		return true;
	}
}

/* 验证密码 */
function isPassword(password){
	if(isNotNullOrEmpty(password)){
		if(password.length < 6 || password.length > 20){
			return false;
		}
		var reg =/^(?!\d{6,8}$)(?! )(?=.*[A-Z])(?=.*[a-z])(?=.*[0-9])[a-zA-Z0-9_]{6,20}$/;
		var reg1 =/^(?=.*[0-9])(?=.*[a-z])(?=.*[!@#$%^&*])(?=.*[A-Z]).{6,20}$/;
		if (reg.exec(password)||reg1.exec(password)){
			return true;
		}
	}
	return false;
}

/*
 * check date 适用 2000-5-5这种格式，日期在1900-0-0到2099-12-31之间 2000-05-05 也可
 */
function isDate(date){
	var datePattern = /^(19|20)\d{2}-(1[0-2]|0?[1-9])-(0?[1-9]|[1-2][0-9]|3[0-1])$/;
	return datePattern.test(date);
}

/* check Email */
function checkEmail(email){
	// var emailPattern = /^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/; 
	var emailPattern = /^([a-zA-Z0-9_\-\.]+)@.+\.([a-zA-Z]{2,4}|[0-9]{1,3})$/;
	return emailPattern.test(email);
}

/** 校验电话号码 */
function checkPhone(phone){
	if(isNotNullOrEmpty(phone)){
		if(phone.length!=11){
			return false;
		}
	}
	var phonePatternCN = /^1\d{10}$/;
	return phonePatternCN.test(phone);
}

/** 校验长电话号码 */
function checkLongPhone(phone){
	var phonePattern = /^[\d]{8,15}$/;
	return phonePattern.test(phone);
}

/**
 * 判断字符是不是空
 * 
 * @param str
 */
function isNotNullOrEmpty(str){
	if (str != undefined && str != null && str != "" && str != "undefined"){
		return true;
	} else{
		return false;
	}
}

function hitch(scope , method){
	if (!method){
		method = scope;
		scope = null;
	}
	if (this.isString(method)){
		scope = scope || _g;
		if (!scope[method]){
			throw ([ 'hitch: scope["', method, '"] is null (scope="', scope, '")' ].join(''));
		}
		return function(){
			return scope[method].apply(scope, arguments || []);
		}; // Function
	}
	return !scope ? method : function(){
		return method.apply(scope, arguments || []);
	};
}

function isString(obj){
	return typeof obj === "string" || obj instanceof String;
}

/** 判断是否游客 */
function isGuest(){
	return !isNotNullOrEmpty(member);
}

// /获取cookie值
function getCookie(NameOfCookie){
	// 首先我们检查下cookie是否存在.
	// 如果不存在则document.cookie的长度为0
	if (document.cookie.length > 0){
		
		// 如果begin的变量值得到的是-1那么说明不存在
		begin = document.cookie.indexOf(NameOfCookie + "=");
		if (begin != -1){
			
			// 说明存在我们的cookie.
			begin += NameOfCookie.length + 1;// cookie值的初始位置
			end = document.cookie.indexOf(";", begin);// 结束位置
			if (end == -1)
				end = document.cookie.length;//没有;则end为字符串结束位置   
			return unescape(document.cookie.substring(begin, end));
		}
	}
	return null;
}

/** 判断字符中是否包含有特殊字符： */
function containSpecial(s){
	var containSpecial = /^[(\ )(\~)(\!)(\@)(\#)(\$)(\%)(\^)(\&)(\*)(\()(\))(\-)(\_)(\+)(\=)(\[)(\])(\{)(\})(\|)(\\)(\;)(\:)(\')(\")(\,)(\.)(\/)(\<)(\>)(\?)(\)]+$/;
	var flag=false;
    for (var i = 0; i < s.length; i++){
    	var substring=s.substring(i,i+1);
		flag=containSpecial.test(substring);
    	if(flag){
    		return true;
    	}
    }
    return false;
}

/**
 * mobile只能输入和粘贴数字
 */
function replaceMobileDigit(obj){
	var objValue = obj.value;
	if (objValue.indexOf("+") >= 0){
		var prefix = objValue.split(" ")[0];
		var digit = objValue.split(" ")[1];
		if (isNotNullOrEmpty(digit)){
			obj.value = "+" + prefix.replace(/\D/g, '') + " " + digit.replace(/\D/g, '');
		} else{
			obj.value = "+" + prefix.replace(/\D/g, '');
		}
	} else{
		obj.value = obj.value.replace(/\D/g, '')
	}
}

$(function(){
	$('.com-table1 tbody>tr').eq(1).children("td").css('border', 'none');
});

/** 电话增加前缀 */
function appendPhonePrefix(obj){
	var defaults = {
	    HK : "+852",
	    CN : "+86"
	};
	var regHK = /^\d{8}$/;// 香港
	var regCN = /^\d{11}$/;// 大陆
	
	var mobile = $(obj).val();
	if (regHK.test(mobile)){
		$(obj).val(defaults.HK + " " + mobile);
	} else if (regCN.test(mobile)){
		$(obj).val(defaults.CN + " " + mobile);
	}
}
//格式化金额，保留两位小数
function formatCurrency(num) {
	if(null==num){
		num = "0";
	}
    num = num.toString().replace(/\$|\,/g,'');
    if(isNaN(num))
    num = "0";
    sign = (num == (num = Math.abs(num)));
    num = Math.floor(num*100+0.50000000001);
    cents = num%100;
    num = Math.floor(num/100).toString();
    if(cents<10)
    cents = "0" + cents;
    for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++)
    num = num.substring(0,num.length-(4*i+3))+','+
    num.substring(num.length-(4*i+3));
    return (((sign)?'':'-') + num + '.' + cents);
}

//格式化货币不保留小数
function matNum(num) {
	if(null==num){
		num = "0";
	}
	num = num.toString().replace(/\$|\,/g,'');
	if(isNaN(num)){
		num = "0";
	}
	sign = (num == (num = Math.abs(num)));
	num = Math.floor(num*100+0.50000000001);
	cents = num%100;
	num = Math.floor(num/100).toString();
	if(cents<10)
	    cents = "0" + cents;
	for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++)
	    num = num.substring(0,num.length-(4*i+3))+','+
	num.substring(num.length-(4*i+3));
	var str= (((sign)?'':'-') + num + '.' + cents);
	
	return str.substring(0,str.indexOf("."));
}

/**
 * 设置错误信息
 * @param obj
 * @param flag
 * @param message
 * @param isfocus
 */
function setValidMessage(obj, flag, message) {
	
	//此处注释掉 以适应，弹框多次弹出 删除报错样式
	$(obj).attr("check",flag)
	
	var group=$(obj).parents(".spice-form-group");
//	var msgBolok=$(obj).parent().siblings(".spice-msg-block");
	
	var msgBolok = getMsgBlock(obj);
	if (!flag) {
		$(group).addClass("spice-error");		
		$(msgBolok).find("span").html(message);
		$(msgBolok).show();
		return false;
	}
	$(group).removeClass("spice-error");
	$(msgBolok).find("span").html(message);
	$(msgBolok).hide();
}

function getMsgBlock(elem){
	return $(elem).parent().siblings(".spice-msg-block").length != 0?$(elem).parent().siblings(".spice-msg-block"):$(elem).parents(".spice-form-group").find(".spice-msg-block")
}


/**
 * keyup事件
 * @param obj
 * @returns
 */
function specialCharFiltering(obj){
	var str=$(obj).val();
	
	//数字字母
	var reg = /^[0-9a-zA-Z]+$/;
	//汉字
	var reg1=/[\u4e00-\u9fa5]$/;
	//空格
	var reg2=/\s/;
	//特殊字符
	var specialChars=new Array("!","\"","#","$","%","&","'","(",")","*","+",",","-",".","/",":",";","<",">","?","@","[","]","^","_","`","{","|","}","~","«","´","·","»","—","‘","’","“","”","…","……","‹","›","、","。","〈","〉","《","》","「","」","『","』","【","】","〔","〕","〖","〗","〞","﹝","﹞","！","＂","＃","＆","＇","（","）","，","：","；","＜","＞","？","＠","［","］","｛","｝","～","￥");
	
	var value="";
	var flag=true;
	for(var i=0;i<str.length;i++){
		var char=str.charAt(i);
		if(reg.test(char) || reg1.test(char) || $.inArray(char,specialChars)>=0 || reg2.test(char)){
			value=value+char;
		}else{
			showErrorMsg();
		}
			
	}
	return value;
}



/**
 * 特殊字符过滤，判断是否存在不能输入的特殊字符 bulr事件
 * @returns
 */
function specialCharFilteringWithBulr(obj){
	var str=$(obj).val();
	
	//数字字母
	var reg = /^[0-9a-zA-Z]+$/;
	//汉字
	var reg1=/[\u4e00-\u9fa5]$/;
	
	//空格 tab 换行
	var reg2=/(\u0020|\u0009|[\u000d\u000a])/;
	//特殊字符
	var specialChars=new Array("!","\"","#","$","%","&","'","(",")","*","+",",","-",".","/",":",";","<",">","?","@","[","]","^","_","`","{","|","}","~","«","´","·","»","—","‘","’","“","”","…","……","‹","›","、","。","〈","〉","《","》","「","」","『","』","【","】","〔","〕","〖","〗","〞","﹝","﹞","！","＂","＃","＆","＇","（","）","，","：","；","＜","＞","？","＠","［","］","｛","｝","～","￥");
	
	var value="";
	var flag=true;
	for(var i=0;i<str.length;i++){
		var char=str.charAt(i);
		if(reg.test(char) || reg1.test(char) || $.inArray(char,specialChars)>=0 || reg2.test(char)){
			value=value+char;
		}
	}
	return value;
}


function showErrorMsg(){
	$(".gift-tips").show();
	var timerCountDown = setInterval(
			function() {
				$(".gift-tips").hide();
				clearInterval(timerCountDown);					
			}, 2000);
}





$(function(){
	var baseUrl =base+"/search?";
	var $headerSearch = $(".spice-header-search");
	$headerSearch.find(".spice-seach-box-button").live("tap", function(){
		var keyword = $.trim($(".spice-seach-box-input").val());
		if (keyword) {
			$headerSearch.find(".spice-seach-box-input").parent().removeClass("spice-error");
			window.showFooterEmailMsg( $('.e-header-search') );
			baseUrl += "keyword=" + keyword;
			window.location.href = baseUrl;
		} else {
			window.showFooterEmailErrorMsg( $('.e-header-search') );
			$headerSearch.find(".spice-seach-box-input").parent().addClass("spice-error");
		}
	})
	$headerSearch.find(".spice-seach-box-input").bind("keypress", function(event){
		var keyword = $.trim($(".spice-seach-box-input").val());
		var content = $(this).val();
		if(event.keyCode == '13')
		{
			if (keyword) {
				$headerSearch.find(".spice-seach-box-input").parent().removeClass("spice-error");
				window.showFooterEmailMsg( $('.e-header-search') );
				baseUrl += "keyword=" + keyword;
				window.location.href = baseUrl;
			} else {
				window.showFooterEmailErrorMsg( $('.e-header-search') );
				$headerSearch.find(".spice-seach-box-input").parent().addClass("spice-error");
			}
		}
	})
})



window.showFooterEmailErrorMsg = function(ele){
	$('.spice-no-error-style-input [data-errormsg]',ele).attr('placeholder',function(){
		return $(this).attr('data-errormsg');
	})
}
window.showFooterEmailMsg = function(ele){
	$('.spice-no-error-style-input [data-errormsg]',ele).attr('placeholder',function(){
		return $(this).attr('data-msg');
	})
}
var resetPasswordTimerCountDown;
var tokenCount=6;
$(function() {
	var checkMedia = function(){
	    var media = null
	        , w = window.innerWidth
	    if ( w <= 767 ) {
	        media = 'mobile'
	    } else if( w <= 1023 ){
	        media = 'pad'
	    } else if( w == 1024){
	        media = 'wpad'
	    } else{
	        media = 'pc'
	    }
	    return media
	}
	
	// 购物车数量
	window.loadingShoppingcartCout();

	// 心愿单数量
	window.loadingMiniWashCout();

	// 顶部登录的悬浮事件
	if (window.spice.hasTouch) {
		$(".spice-nav-btn-login").on('tap', function() {
			if ($('body').hasClass('e-spice-standard')) {
				if (!$(this).hasClass('spice-active')) {
					showUserLogin();
					return false
				}
			}
		})
	} else {
		$(".spice-nav-btn-login").mouseover(function() {
			var showUserLogin = window.showUserLogin;
			showUserLogin();
		}).mouseout(function() {
			var hideUserLogin = window.hideUserLogin;
			hideUserLogin();
		});
	}

	// 顶部用户中心悬浮事件
	if (window.spice.hasTouch) {
		$(".spice-nav-btn-login-after").on('tap', function() {
			if ($('body').hasClass('e-spice-standard')) {
				if (!$(this).hasClass('spice-active')) {
					showUserLoginAfter()
					return false
				}
			}
		})
	} else {
		$(".spice-nav-btn-login-after").mouseover(function() {
			var showUserLoginAfter = window.showUserLoginAfter;
			showUserLoginAfter();
		}).mouseout(function() {
			var hideUserLoginAfter = window.hideUserLoginAfter;
			hideUserLoginAfter();
		});
	}
	if (window.spice.hasTouch && $(window).width() >= 1024) {
		$(document).on('tap', function(e) {
			var $target = $(e.target)
			if ($target.closest(".spice-nav-btn-login").length == 0) {
				hideUserLogin();
			}
			if ($target.closest(".spice-nav-btn-login-after").length == 0) {
				hideUserLoginAfter();
			}
		})
	}

	window.isFirstLoadingCart = true;
	// 购物车点击
	$(".e-header-shoppingcart a").on("tap", function() {
		// PC
		if ($("body").hasClass("e-spice-standard")) {
			window.location.href = base + "/shoppingcart";
		} else {
			if (window.isFirstLoadingCart) {
				var data = loxia.syncXhrGet(base + "/shoppingcart/loading", {});
				if (isNotNullOrEmpty(data)) {
					$(this).parents(".e-header-shoppingcart").find(".spice-sub-menu").html(data);
					$(window).trigger('resize.spice-autoReplaceImageSrc');
					window.miniShoppingcartHoverAction();
					window.showMiniShoppingCartOnPadMob.call($('.e-spice-medium .e-header-shoppingcart > a, .e-spice-small .e-header-shoppingcart > a'));
				}
				window.isFirstLoadingCart = false;
			} else {
				window.showMiniShoppingCartOnPadMob.call($('.e-spice-medium .e-header-shoppingcart > a, .e-spice-small .e-header-shoppingcart > a'));
			}
		}
	});

	// 购物车悬浮
	$(".spice-header-shoppingcart").mouseover(function() {
		// PC
		if ($("body").hasClass("e-spice-standard")) {
			if (window.isFirstLoadingCart) {
				var data = loxia.syncXhrGet(base + "/shoppingcart/loading", {});
				if (isNotNullOrEmpty(data)) {
					$(this).find(".spice-sub-menu").html(data);
					$(window).trigger('resize.spice-autoReplaceImageSrc');
					window.miniShoppingcartHoverAction();
					window.showMiniShoppingCart();
				}
			} else {
				window.showMiniShoppingCart();
			}
			window.isFirstLoadingCart = false;
		}
	}).mouseout(function() {
		window.hideMiniShoppingCart();
	});

	window.isFirstLoadingWish = true;
	// mini心愿单
	$(".e-header-wish-list").mouseover(function() {
		if (window.isFirstLoadingWish) {
			var data = loxia.syncXhrGet(base + "/header/wishList", {});
			if (isNotNullOrEmpty(data)) {
				$(this).find(".spice-sub-menu").html(data);
				$(window).trigger('resize.spice-autoReplaceImageSrc');
				window.miniHeartcartHoverAction();
				window.showWishList();
			}
		} else {
			window.showWishList();
		}
		window.isFirstLoadingWish = false;
	}).mouseout(function() {
		window.hideWishList();
	});

	// 登录弹出层
	$(".spice-submenu-btn-login").on("tap", function() {
		headerLoginDialog();
	});

	// 注册弹出层
	 $(".spice-submenu-btn-register").on("tap",function(){
		 dialogHeaderRegister();
	 });

	// 客户服务弹出层
	$(".e-header-customer-service").on("tap", function() {
		var dialogHeaderCustomerService = window.headerDialog.dialogHeaderCustomerService;
		dialogHeaderCustomerService.show();
		dialogHeaderCustomerService.elem.find('#live800').off("tap").on("tap", function() {
			try {
				globalChatClient.openOrClosePopChat();
				lightBoxClose();
			} catch (e) {
				console.log(e);
			}
			dialogHeaderCustomerService.hide();
		});
	});

	// 在线客服
	$(".live800Btn").on("tap", function() {
		try {
			globalChatClient.openOrClosePopChat();
			lightBoxClose();
		} catch (e) {
			console.log(e);
		}
	});

	// 退出登录
	$(".spice-nav-other-bottom .recede-btn").on("tap", function() {
		window.location.href = base + "/member/loginOut";
	});
	
	
	//mini购物车去结算按钮
	$("#needBindMobileHeaderCheckout").live("tap",function(){
		var backUrl=base+"/header/checkout";
		dialogBindMobile(backUrl);
	});
	

	// 搜索联想
	/*
	 * $("#keyword").autocomplete({ source : function(request, response) { var
	 * keyword = $("#keyword").val(); $.post(base + "/suggest", { keyword :
	 * keyword }, function(data) { var wordList = new Array();
	 * $(data).each(function(index, keyword) { var map = new Map(); map["label"] =
	 * keyword; map["value"] = keyword; wordList.push(map); });
	 * response(wordList); }); }, autoFocus : true, mustMatch : true, minLength :
	 * 1, classes: { "ui-autocomplete": "search-flyout" }, position: {of :
	 * ".spice-header-search .spice-sub-menu" },
	 * 
	 * select: function (event, ui) {
	 * 
	 * $("#keyword").val(ui.item.value);
	 * 
	 * if ($('#keyword').val() != '') {
	 * $(".spice-seach-box-button").trigger("tap");             } } });
	 */
	
	//20分钟一次
	var token = window.setInterval(function() {
		tokenCount=tokenCount-1;
		
		loxia.syncXhrGet(base + "/loading", {});
		if(tokenCount == 0){
			clearInterval(token);
		}
	}, 1200000);
	
	$('.e-sales-continue').on("tap", function() {
		var dialogHeaderSalesLogin = window.headerDialog.dialogHeaderSalesLogin;
		dialogHeaderSalesLogin.hide();
	});
	
	$('.spice-sales-consultant-edit').off("tap").on("tap", function() {
		var dialogHeaderSalesLogin = window.headerDialog.dialogHeaderSalesLogin;
		dialogHeaderSalesLogin.show();
	});
});

/**
 * 显示mini购物车，用来加入购物车成功后调用的
 */
function showHeaderShoppingcart() {
	// 修改数量
	window.loadingShoppingcartCout();

	var data = loxia.syncXhrGet(base + "/shoppingcart/loading", {});
	if (isNotNullOrEmpty(data)) {
		window.setMinCartClass();

		$(".spice-header-shoppingcart").find(".spice-sub-menu").html(data);
		$(window).trigger('resize.spice-autoReplaceImageSrc');
		window.miniShoppingcartHoverAction();
		if ($("body").hasClass("e-spice-standard")) {
			window.showMiniShoppingCart();
			var refreshid = window.setInterval(function() {
				window.hideMiniShoppingCart();
				clearInterval(refreshid);
			}, 4000);
		} else {
			setTimeout(function() {
				window.showMiniShoppingCartOnPadMob.call($('.e-spice-medium .e-header-shoppingcart > a, .e-spice-small .e-header-shoppingcart > a'));
			}, 0)
			var refreshid = window.setInterval(function() {
				window.showMiniShoppingCartOnPadMob.call($('.e-spice-medium .e-header-shoppingcart > a, .e-spice-small .e-header-shoppingcart > a'));
				clearInterval(refreshid);
			}, 4000);
		}
	}
}

/**
 * 老用户登录
 * 
 * @param loginName
 */
function oldMemberLogin(loginName) {
	var dialogHeaderIsMember = window.headerDialog.dialogHeaderIsMember;
	dialogHeaderIsMember.show();

	// 选中事件
	dialogHeaderIsMember.elem.find('.spice-icon-checked').off("tap").on("tap", function() {
		if ($(this).hasClass("spice-active")) {
			$(this).removeClass("spice-active");
		} else {
			$(this).addClass("spice-active");
		}
	});

	var pwdInput = dialogHeaderIsMember.elem.find('input[name="password"]');
	// 密码验证
	pwdInput.off("blur").on("blur", function() {
		passwordBlur(pwdInput, false,false);
	});

	// 忘记密码
	dialogHeaderIsMember.elem.find('.e-common-forget-password').off("tap").on("tap", function() {
		dialogHeaderIsMember.hide();
		dialogHeaderForgetPassword(loginName);
	});

	// 用另一个邮箱地址创建账号
	dialogHeaderIsMember.elem.find('.e-new-other-account').off("tap").on("tap", function() {
		dialogHeaderIsMember.hide();		

		// 注册方法
		dialogHeaderRegister();
	});

	// 登录事件
	dialogHeaderIsMember.elem.find('.spice-form-btn-submit').off("tap").on("tap", function() {
		pwdInput.trigger("blur");
		var pwdCheck = $(pwdInput).attr("check");
		if (pwdCheck) {
			// 选中记住我时，保存用户名到cookie中
			var isRemberMeLoginName = dialogHeaderIsMember.elem.find(".spice-icon-checked").hasClass("spice-active");
			var pwd = $.trim($(pwdInput).val());
			var crypt = new JSEncrypt();
			crypt.setPublicKey($("#sensitivePublicKey").val());
			var enc_password = crypt.encrypt(pwd);
			var json = {
				'loginName' : loginName,
				'password' : enc_password,
				'isRemberMeLoginName' : isRemberMeLoginName
			};
			var data = loxia.syncXhrPost(base + "/member/goLogin", json);
			// 登录成功
			if (data.result) {
				if (data.statusCode == "login.mobile.isnull") {
					var backUrl = window.location.href;
					dialogHeaderIsMember.hide();
					dialogBindMobile(backUrl);
				} else {
					window.location.reload();
				}
			} else {
				// 提示错误信息
				setValidMessage($(pwdInput), false, loxia.getLocaleMsg("member.login.emailOrPwdError"));
				$(pwdInput).parents(".spice-validator-slot").addClass("spice-error");
			}
		} else {
			$(pwdInput).parents(".spice-validator-slot").addClass("spice-error");
		}
	});
}

// 忘记密码弹框
function dialogHeaderForgetPassword(emailOrMobile) {
	var dialogHeaderForgetPassword = window.headerDialog.dialogHeaderForgetPassword;
	var emailOrMobileInput = dialogHeaderForgetPassword.elem.find('input[name="emailOrMobile"]');

	// 清空忘记密码的输入值
	$(emailOrMobileInput).val("");
	setValidMessage($(emailOrMobileInput), true, "");

	if (isNotNullOrEmpty(emailOrMobile)) {
		$(emailOrMobileInput).val(emailOrMobile);
	}
	dialogHeaderForgetPassword.show();
	emailOrMobileInput.off("blur").on("blur", function() {
		emailOrMobileBlur(emailOrMobileInput, false, false);
	});

	dialogHeaderForgetPassword.elem.find('.e-reset-password').off("tap").on("tap", function() {
		// emailOrMobileInput.trigger("blur");
		emailOrMobileBlur(emailOrMobileInput, false, true);

		var emailOrMobileCheck = $(emailOrMobileInput).attr("check");
		if (emailOrMobileCheck == "true") {
			emailOrMobile = $.trim($(emailOrMobileInput).val());

			// 校验输入值类型，email、mobile
			var type = checkEmail(emailOrMobile) ? "email" : "mobile";

			if ("email" == type) {// 邮箱的情况直接发送重置密码链接
				var json = {
					'loginEmail' : emailOrMobile
				};
				var data = loxia.syncXhrPost(base + "/m/sendForgetEmail", json);
				// 登录成功
				if (data.result) {
					dialogHeaderForgetPassword.hide();
					dialogHeaderThank(emailOrMobile);
				} else {
					// 提示错误信息
					setValidMessage(emailOrMobileInput, false, loxia.getLocaleMsg(data.statusCode));
				}
			} else {// 手机号码的情况，打开手机发送验证码重置密码弹框
				dialogHeaderForgetPassword.hide();
				dialogHeaderResetPassword(emailOrMobile);
			}
		}
	});
}

// 邮件改密码 谢谢弹框
function dialogHeaderThank(email) {
	// 显示弹框
	var dialogHeaderThank = window.headerDialog.dialogHeaderThank;
	dialogHeaderThank.show();

	// 定义事件
	dialogHeaderThank.elem.find('.e-reset-success').off("tap").on("tap", function() {
		dialogHeaderThank.hide();
	});
}

// 手机改密码弹框
function dialogHeaderResetPassword(mobile) {
	// 显示弹框
	var dialogHeaderResetPassword = window.headerDialog.dialogHeaderResetPassword;
	var dialogHeaderResetSuccess = window.headerDialog.dialogHeaderResetSuccess;
	
	//根据传入的手机号，修改弹框标题
	dialogHeaderResetPassword.elem.find(".spice-dialog-title-sub").empty().html("获取验证码后，手机验证码将发送至您的手机" + mobile + "，请注意查收");

	// 初始化
	var graphicVerificationInput = dialogHeaderResetPassword.elem.find("input[name='graphicVerification']");// 图形验证码
	var mobileCaptchaInput = dialogHeaderResetPassword.elem.find("input[name='mobileCaptcha']");// 短信验证码
	var passwordInput = dialogHeaderResetPassword.elem.find(".password-place-pc input[name='password']");// 密码
	var confirmPasswordInput = dialogHeaderResetPassword.elem.find(".password-place-pc input[name='confirmPassword']");// 确认密码
	var mobilePasswordInput = dialogHeaderResetPassword.elem.find(".password-place-mb input[name='password']");// 密码

	//重置定时器
	if(resetPasswordTimerCountDown){
		clearInterval(resetPasswordTimerCountDown);
	}
	
	//重置获取验证码
	dialogHeaderResetPassword.elem.find(".sendMobileCaptcha span").removeClass("is-disabled").html(loxia.getLocaleMsg("member.login.getCaptchaCode"));
	
	// 清空输入
	$(graphicVerificationInput).val("");
	$(mobileCaptchaInput).val("");
	$(passwordInput).val("");
	$(confirmPasswordInput).val("");
	$(mobilePasswordInput).val("");
	setValidMessage($(graphicVerificationInput), true, "");
	setValidMessage($(mobileCaptchaInput), true, "");
	setValidMessage($(passwordInput), true, "");
	setValidMessage($(confirmPasswordInput), true, "");
	setValidMessage($(mobilePasswordInput), true, "");

	// 加载验证码
	var captchaDiv = dialogHeaderResetPassword.elem.find(".spice-identifying-png");
	refreshCaptcha(captchaDiv);

	//显示手机重置密码弹框
	dialogHeaderResetPassword.show();

	// 点击"刷新"
	dialogHeaderResetPassword.elem.find(".spice-identifying-text").off("tap").on("tap", function() {
		refreshCaptcha(captchaDiv);
	});

	// 验证码离焦事件
	graphicVerificationInput.off("blur").on("blur", function() {
		var userInput = $(this).val();
		if (isNotNullOrEmpty(userInput)) {
			setValidMessage($(this), true, "");
		} else {
			setValidMessage($(this), false, loxia.getLocaleMsg("member.login.captchaCodeIsNull"));
		}
	});

	// 发送验证码点击事件
	dialogHeaderResetPassword.elem.find(".sendMobileCaptcha").off("tap").on("tap", function() {
		var span = $(this).find("span");
		if (!$(span).hasClass("is-disabled")) {
			graphicVerificationInput.trigger("blur");
			var captchaCheck = $(graphicVerificationInput).attr("check");
			if (captchaCheck == "true") {
				var instanceId = dialogHeaderResetPassword.elem.find(".spice-identifying-png").find("input[type='hidden']").val();
				var userInput = $.trim($(graphicVerificationInput).val());
				var captchaId = $(graphicVerificationInput).parent("div").siblings(".spice-identifying-box").find("input[type='hidden']").attr("id");
				
				var json = {
					'mobile' : mobile,
					'userInput' : userInput,
					'instanceId' : instanceId,
					'captchaId' : 'captcha_pc_login'
				};
				var result = loxia.syncXhrGet(base + "/member/sendResetPasswordMessage", json);
				if (result) {
					if (result.result) {
						$(span).addClass("is-disabled");
						var count = 60;
						$(span).html('重新发送(' + count + ')');
						
						resetPasswordTimerCountDown = setInterval(function() {
							if (count > 1) {
								count--;
								$(span).html('重新发送(' + count + ')');
							} else {
								clearInterval(resetPasswordTimerCountDown);
								$(span).removeClass("is-disabled");
								$(span).html(loxia.getLocaleMsg("member.login.getCaptchaCode"));
							}
						}, 1000);
					} else {
						var statusCode = result.statusCode;
						if (statusCode == "member.mobile.error") {
							setValidMessage(mobileCaptchaInput, false, loxia.getLocaleMsg("member.login.smsCodeError"));
						} else if (statusCode == "captcha.error") {
							setValidMessage(graphicVerificationInput, false, loxia.getLocaleMsg("member.login.captchaCodeError"));
							refreshCaptcha(captchaDiv);
						} else {
							setValidMessage(mobileCaptchaInput, false, loxia.getLocaleMsg("member.login.sendSmsError"));
						}
					}
				}
			}
		}
	});

	// 重置密码提交
	dialogHeaderResetPassword.elem.find(".e-reset-password-submit").off("tap").on("tap", function() {
		graphicVerificationInput.trigger("blur");
		mobileCaptchaInput.trigger("blur");
		passwordInput.trigger("blur");
		mobilePasswordInput.trigger("blur");
		confirmPasswordInput.trigger("blur");

		var graphicVerificationInputCheck = $(graphicVerificationInput).attr("check");
		var mobileCaptchaInputCheck = $(mobileCaptchaInput).attr("check");
		var passwordInputCheck = $(passwordInput).attr("check");
		var mobilePasswordInputCheck = $(mobilePasswordInput).attr("check");
		var confirmPasswordInputCheck = $(confirmPasswordInput).attr("check");

		var deviceType = checkMobileOrPC();
		 
		var password;
		var validateCheck;
		if(deviceType == "mobile"){
			password = $(mobilePasswordInput).val();			
			validateCheck = graphicVerificationInputCheck == "true" && mobileCaptchaInputCheck == "true" && mobilePasswordInputCheck == "true"; 
		}else{
			password = $(passwordInput).val();
			validateCheck = graphicVerificationInputCheck == "true" && mobileCaptchaInputCheck == "true" && passwordInputCheck == "true" && confirmPasswordInputCheck == "true"
		}
		
		var crypt = new JSEncrypt();
		crypt.setPublicKey($("#sensitivePublicKey").val());
		var enc_password = crypt.encrypt(password);

		if (validateCheck) {
			var json = {
				'mobileCaptcha' : mobileCaptchaInput.val(),
				'mobile' : mobile,
				'password' : enc_password
			};
			var result = loxia.syncXhrGet(base + "/member/mobileResetPassword", json);
			if (result) {
				if (result.result) {
					dialogHeaderResetPassword.hide();
					dialogHeaderResetSuccess.show();
					dialogHeaderResetSuccess.elem.find(".spice-btn-golden").off("tap").on("tap", function() {
						window.location.href = base + "/member/login";
					})
					dialogHeaderResetSuccess.elem.find(".e-spice-dialog-icon-close").off("tap").on("tap", function() {
						dialogHeaderResetSuccess.hide();
					})
				} else {
					var statusCode = result.statusCode;
					if (statusCode == "member.mobile.error") {
						setValidMessage(mobileCaptchaInput, false, loxia.getLocaleMsg("member.login.mobileErro"));
					} else if (statusCode == "member.reset.mobileNotExisit") {
						setValidMessage(mobileCaptchaInput, false, loxia.getLocaleMsg("member.reset.mobileNotExisit"));
					} else if (statusCode == "member.login.passwordError") {
						if(deviceType == "mobile"){
							setValidMessage(mobilePasswordInput, false, loxia.getLocaleMsg("member.login.passwordError"));
						}else{
							setValidMessage(passwordInput, false, loxia.getLocaleMsg("member.login.passwordError"));	
						}
					} else if (statusCode == "member.login.passwordEqualError") {
						if(deviceType == "mobile"){
							setValidMessage(mobilePasswordInput, false, loxia.getLocaleMsg("member.login.passwordEqualError"));
						}else{
							setValidMessage(passwordInput, false, loxia.getLocaleMsg("member.login.passwordEqualError"));	
						}
					} else if (statusCode == "register.captchaCode.isError") {
						setValidMessage(mobileCaptchaInput, false, loxia.getLocaleMsg("member.login.smsCodeError"));
					} else {
						setValidMessage(mobileCaptchaInput, false, loxia.getLocaleMsg("member.login.resetPasswordError"));
					}
				}
			}
		}
	});

	// 手机验证码
	mobileCaptchaInput.off("blur").on("blur", function() {
		var mobileCaptcha = $(this).val();
		if (isNotNullOrEmpty(mobileCaptcha)) {
			setValidMessage($(mobileCaptchaInput), true, "");
		} else {
			setValidMessage($(this), false, loxia.getLocaleMsg("member.login.smsCodeIsNull"));
		}
	});

	// 密码验证
	passwordInput.off("blur").on("blur", function() {
		passwordBlur(passwordInput, true,false);
	});
	
	// 密码验证
	mobilePasswordInput.off("blur").on("blur", function() {
		passwordBlur(mobilePasswordInput, true,false);
	});

	// 确认密码验证
	confirmPasswordInput.off("blur").on("blur", function() {
		var flag = passwordBlur(confirmPasswordInput, true,true);
		if (flag) {
			var password = $(passwordInput).val();
			var password2 = $(confirmPasswordInput).val();
			if (password2 == password) {
				setDialogPwdValidMessage(confirmPasswordInput, true, "");
			} else {
				setDialogPwdValidMessage(confirmPasswordInput, false, loxia.getLocaleMsg("member.login.confirmPasswordError"));
			}
		}
	});
}

/**
 * 邮箱校验
 * 
 * @param emailInput
 * @param isRegister
 */
function emailBlur(emailInput, isRegister, flag) {
	var email = $.trim($(emailInput).val());
	if (!isNotNullOrEmpty(email)) {
		setValidMessage($(emailInput), false, loxia.getLocaleMsg("member.login.emailIsNull"));
	} else {
		if (checkEmail(email)) {
			setValidMessage($(emailInput), true, "");

			// 如果是注册，判断邮箱是否注册过
			if (isRegister && flag) {
				var json = {
					'email' : email
				};
				var data = loxia.syncXhrPost(base + "/member/checkLoginEmailAvailable", json);
				if (!data.result) {
					var statusCode = data.statusCode;
					if (statusCode == "member.email.error") {
						setValidMessage($(this), false, loxia.getLocaleMsg("member.login.emailError"));
					} else if (statusCode == "register.loginemail.unavailable") {
						window.headerDialog.dialogHeaderRegister.hide();
						oldMemberLogin(email);
					}
				}
			}
		} else {
			setValidMessage($(emailInput), false, loxia.getLocaleMsg("member.login.emailError"));
		}
	}
}

// 邮箱、手机统一校验
function emailOrMobileBlur(emailOrMobileInput, isRegister, flag) {
	var emailOrMobile = $.trim($(emailOrMobileInput).val());
	if (!isNotNullOrEmpty(emailOrMobile)) {
		setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.login.emailOrMobileIsNull"));
	} else {
		if (checkEmail(emailOrMobile) || checkPhone(emailOrMobile)) {
			setValidMessage($(emailOrMobileInput), true, "");

			// flag为true时,判断是否已注册
			if (flag) {
				var type = checkEmail(emailOrMobile) ? "email" : "mobile";
				var data = checkEmailOrMobileIsExisit(emailOrMobile, type);
				// 注册时
				if (isRegister) {
					if (!data.result) {
						var statusCode = data.statusCode;
						if (statusCode == "member.email.error") {
							setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.login.emailError"));
						} else if (statusCode == "register.loginemail.unavailable") {
							window.headerDialog.dialogHeaderRegister.hide();
							oldMemberLogin(email);
						} else if (statusCode == "member.mobile.error") {
							setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.login.mobileError"));
						} else if (statusCode == "register.loginmobile.unavailable") {

						}
					}
				} else {// 非注册情况
					if (data.result) {
						if ("email" == type) {
							setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.reset.emailNotExisit"));
						} else if ("mobile" == type) {
							setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.reset.mobileNotExisit"));
						}
					} else {
						var statusCode = data.statusCode;
						if (statusCode == "member.email.error") {
							setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.login.emailError"));
						} else if (statusCode == "member.mobile.error") {
							setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.login.mobileError"));
						}
					}
				}
			}
		} else {
			setValidMessage($(emailOrMobileInput), false, loxia.getLocaleMsg("member.login.emailOrMobileError"));
		}
	}
}

/**
 * 发送请求校验mobile或者email是否存在
 * 
 * @value
 * @type 为"mobile"时，校验mobile;为"email"时校验email
 * 
 * @result data.statusCode描述: member.email.error 电子邮箱格式错误
 *         register.loginemail.unavailable 电子邮箱被占用 member.mobile.error 电话号码格式错误
 *         register.loginmobile.unavailable mobile被占用
 */
function checkEmailOrMobileIsExisit(value, type) {
	var json;
	if ("email" == type) {
		json = {
			'email' : value
		};
	} else {
		json = {
			'mobile' : value
		};
	}

	var data = loxia.syncXhrPost(base + "/member/checkEmailOrMobileAvailable", json);
	return data;
}

/**
 * 密码校验
 * 
 * @param pwdInput
 * @param isRegister
 */
function passwordBlur(pwdInput, isRegister,isConfirmPassword) {
	 $(pwdInput).parents(".spice-form-group").removeClass("spice-error");
	var password = $(pwdInput).val();
	if (!isNotNullOrEmpty(password)) {
		if (isRegister) {
			isConfirmPassword?setDialogPwdValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.confirmPasswordIsNull")):setDialogPwdValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.passwordIsNull"));
		} else {
			isConfirmPassword?setValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.confirmPasswordIsNull")):setValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.passwordIsNull"));
		}
		return false;
	} else {
		if (isRegister) {
			if (isPassword(password)) {
				setDialogPwdValidMessage(pwdInput, true, "");
				return true;
			} else {
				setDialogPwdValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.passwordError"));
				return false;
			}
		} else {
			setValidMessage(pwdInput, true, "");
			return true;
		}
	}
}

function setDialogPwdValidMessage(obj, flag, message) {
	$(obj).attr("check", flag)

	var group = $(obj).parents(".spice-form-validator");
	var msgBolok = $(obj).parent().siblings(".spice-msg-block");
	if (!flag) {
		$(group).addClass("spice-error");
		$(msgBolok).find("span").html(message);
		$(msgBolok).show();
		return false;
	}
	$(group).removeClass("spice-error");
	$(msgBolok).find("span").html(message);
	$(msgBolok).hide();
}

window.loadingShoppingcartCout = function() {
	window.isFirstLoadingCart = true;

	var count = "0";
	var value = Cookies.get('s_c_c_n');
	if (isNotNullOrEmpty(value)) {
		count = value;
	}
	if (count != '0') {
		$(".spice-header-shoppingcart").find(".shopping-bag-product-count").html(count);
	} else {
		$(".spice-header-shoppingcart").find(".shopping-bag-product-count").html("");
	}
}

// 初始化购物车数量为0
function initCartCount() {
	Cookies.set('s_c_c_n', '0');
	var countSpan = $(".spice-header-shoppingcart").find(".shopping-bag-product-count");
	countSpan.html('');
	window.setMinCartClass();
}

// 初始化min心愿单数量为0
function initMiniWashCout() {
	Cookies.set('c_w_n_g_p', '0');
	$(".e-header-wish-list").find(".wish-savedItems-num").html("");
	$(".e-header-wish-list").find(".icon-svg-saved-items").css("opacity","0.5")
	window.setMinCartClass();
}

window.loadingMiniWashCout = function() {
	window.isFirstLoadingWish = true;

	var count = "0";
	var value = Cookies.get('c_w_n_g_p');
	if (isNotNullOrEmpty(value)) {
		count = value;
	}

	if (count != '0') {
		$(".e-header-wish-list").find(".wish-savedItems-num").html(count);
		$(".e-header-wish-list").find(".icon-svg-saved-items").css("opacity","1")
	} else {
		$(".e-header-wish-list").find(".wish-savedItems-num").html("");
		$(".e-header-wish-list").find(".icon-svg-saved-items").css("opacity","0.5")
	}
	window.setMinCartClass();
}

window.showHeaderMiniWash = function() {
	if ($("body").hasClass("e-spice-standard")) {
		var data = loxia.syncXhrGet(base + "/header/wishList", {});
		if (isNotNullOrEmpty(data)) {
			$('.e-header-wish-list').find(".spice-sub-menu").html(data);
			$(window).trigger('resize.spice-autoReplaceImageSrc');
			window.showWishList();
		}
		var refreshid = window.setInterval(function() {
			window.hideWishList();
			clearInterval(refreshid);
		}, 4000);
	}
	window.setMinCartClass();
}

window.setMinCartClass = function() {
	var $minShoppingCart = $('.spice-header-shoppingcart'), numS = $('.shopping-bag-product-count', $minShoppingCart).text().slice(1, -1), $minWishList = $('.spice-header .spice-nav-other-login')
	// , numW = $('.wish-savedItems-num', $minWishList).text().slice(1,-1)

	if (numS > 0) {
		$minShoppingCart.addClass('spice-header-mincart-length-1')
	}
	$minWishList.addClass('spice-header-mincart-length-' + numS.length)
}
$(function() {
	// 点击更换验证码
	$('.spice-identifying-png img:not(.pre-gif)').live('tap',function(){
		refreshCaptcha($(this).parent());
	});
	
});

/**
 * 顶部登录弹出层
 */
function headerLoginDialog() {
	var dialogHeaderLogin = window.headerDialog.dialogHeaderLogin;
	var loginNameInput = dialogHeaderLogin.elem.find('input[name="loginName"]');
	var pwdInput = dialogHeaderLogin.elem.find('input[name="password"]');

	var data = loxia.syncXhrPost(base + "/member/getRemeberMe", {});
	if (isNotNullOrEmpty(data) && isNotNullOrEmpty(data.returnObject)) {
		if (isNotNullOrEmpty(data.returnObject.loginName)) {
			loginNameInput.val(data.returnObject.loginName);
			dialogHeaderLogin.elem.find(".spice-icon-checked").addClass("spice-active")
		}
		if (isNotNullOrEmpty(data.returnObject.password)) {
			pwdInput.val(data.returnObject.password);
		}
	}

	dialogHeaderLogin.show();

	// 邮箱验证
	loginNameInput.off("blur").on("blur", function() {
		var loginName = $.trim($(loginNameInput).val());
		if (!isNotNullOrEmpty(loginName)) {
			setValidMessage(loginNameInput, false, "请输入手机号码/电子邮箱");
		} else {
			if (checkEmail(loginName) || checkPhone(loginName)) {
				setValidMessage(loginNameInput, true, "");
			} else {
				setValidMessage(loginNameInput, false, "请输入正确的手机号码/电子邮箱");
			}
		}
	});

	// 密码验证
	pwdInput.off("blur").on("blur",function() {
		var password = $(pwdInput).val();
		if (!isNotNullOrEmpty(password)) {
			setValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.passwordIsNull"));
		} else {
			if (!isPassword(password)) {
				setValidMessage(pwdInput, false, loxia.getLocaleMsg("member.login.passwordError"));
			} else {
				setValidMessage(pwdInput, true, "");
			}
		}
	});

	// 选中事件
	dialogHeaderLogin.elem.find('.spice-icon-checked').off("tap").on("tap",function() {
		if ($(this).hasClass("spice-active")) {
			$(this).removeClass("spice-active");
		} else {
			$(this).addClass("spice-active");
		}
	});

	// 登录事件
	dialogHeaderLogin.elem.find('.spice-form-btn-submit').off("tap").on("tap",function() {
		loginNameInput.trigger("blur");
		pwdInput.trigger("blur");
		var loginNameCheck = $(loginNameInput).attr("check");
		var pwdCheck = $(pwdInput).attr("check");
		if (loginNameCheck == "true" && pwdCheck == "true") {
			var isRemberMeLoginName = dialogHeaderLogin.elem.find(".spice-icon-checked").hasClass("spice-active");
			var loginName = $.trim($(loginNameInput).val());
			var pwd = $.trim($(pwdInput).val());
			var crypt = new JSEncrypt();
			crypt.setPublicKey($("#sensitivePublicKey").val());
			var enc_password = crypt.encrypt(pwd);

			var json = {
				'loginName' : loginName,
				'password' : enc_password,
				'isRemberMeLoginName' : isRemberMeLoginName
			};
			var data = loxia.syncXhrPost(base + "/member/goLogin", json);
			// 登录成功
			if (data.result) {
				if (data.statusCode == "login.mobile.isnull") {
					var backUrl = window.location.href;
					dialogHeaderLogin.hide();
					dialogBindMobile(backUrl);
				} else {
					window.location.reload();
				}
			} else {
				// 提示错误信息
				setValidMessage($(pwdInput), false, loxia.getLocaleMsg("member.login.emailOrPwdError"));
			}
		}
	});

	// 忘记密码
	dialogHeaderLogin.elem.find('.e-common-forget-password').off("tap").on("tap", function() {
		dialogHeaderLogin.hide();
		dialogHeaderForgetPassword($(loginNameInput).val());
	});
}

/**
 * 完善信息弹出层
 */
window.dialogBindMobile = function(backUrl,loginData) {
	var dialogeffect = window.commonEffectFn.dialogeffect;
	var bindMobileDialog = new dialogeffect({
		template : $('#common-dialog-bind-mobile').html(),
		dialogClass : 'new-member-dialog',
		maskClose : false
	});

	// 加载验证码
	var captchaDiv = bindMobileDialog.elem.find(".spice-identifying-png");
	refreshCaptcha(captchaDiv);

	bindMobileDialog.show();

	// 刷新验证码
	bindMobileDialog.elem.find(".spice-identifying-text").off("tap").on("tap",function() {			
		refreshCaptcha(captchaDiv);
	});

	var mobileInput = bindMobileDialog.elem.find("input[name='mobile']");
	var captchaInput = bindMobileDialog.elem.find("input[name='captcha']");
	var mobileCaptchaInput = bindMobileDialog.elem.find("input[name='mobileCaptcha']");

	// 手机号码失去焦点事件
	mobileInput.off("blur").on("blur", function() {
		var mobile = $(this).val();
		if (isNotNullOrEmpty(mobile)) {
			if (checkPhone(mobile)) {
				setValidMessage($(this), true, "");
			} else {
				setValidMessage($(this), false, "请输入正确的手机号码");
			}
		} else {
			setValidMessage($(this), false, "请输入手机号码");
		}
	});

	// 验证码
	captchaInput.off("blur").on("blur", function() {
		var userInput = $(this).val();
		if (isNotNullOrEmpty(userInput)) {
			setValidMessage($(captchaInput), true, "");
		} else {
			setValidMessage($(captchaInput), false, "请输入图形验证码");
		}
	});

	// 发送验证码
	bindMobileDialog.elem.find(".sendMobileCaptcha").off("tap").on("tap",function() {
		var span = $(this).find("span");
		if (!$(span).hasClass("is-disabled")) {
			captchaInput.trigger("blur");
			mobileInput.trigger("blur");
			var mobileCheck = $(mobileInput).attr("check");
			var captchaCheck = $(captchaInput).attr("check");
			if (mobileCheck == "true" && captchaCheck == "true") {
				var instanceId = bindMobileDialog.elem.find(".spice-identifying-png").find("input[type='hidden']").val();
				var userInput = $.trim($(captchaInput).val());
				var mobile = $.trim(mobileInput.val());

				var json = {
					'captchaId' : "captcha_pc_login",
					'userInput' : userInput,
					'instanceId' : instanceId,
					'mobile' : mobile
				};
				var result = loxia.syncXhrGet(base+ "/member/sendBindMobileMessage",json);
				if (result) {
					if (result.result) {
						$(span).addClass("is-disabled");
						var count = 60;
						$(span).html('重新发送(' + count + ')');
						var timerCountDown = setInterval(
							function() {
								if (count > 1) {
									count--;
									$(span).html('重新发送(' + count+ ')');
								} else {
									clearInterval(timerCountDown);
									$(span).removeClass("is-disabled");
									$(span).html("获取验证码");
								}
							}, 1000);
					} else {
						var statusCode = result.statusCode;

						if (statusCode == "captcha.error") {
							setValidMessage($(captchaInput),false, "图形验证码不正确或已过期");
							refreshCaptcha(captchaDiv);
						} else if (statusCode == "member.not.login") {
							setValidMessage(mobileInput, false,"请重新登录!");
						} else if (statusCode == "member.mobile.error") {
							setValidMessage(mobileInput, false,"请输入正确的手机号码");
						} else if (statusCode == "loginmobile.unavailable") {
							setValidMessage(mobileInput, false,"此手机号码已被注册");
						} else {
							setValidMessage(captchaInput,false, "发送验证码失败，请重新在试");
						}
					}
				}

			}
		}
	});

	// 手机验证码
	mobileCaptchaInput.off("blur").on("blur", function() {
		var mobileCaptcha = $(this).val();
		if (isNotNullOrEmpty(mobileCaptcha)) {
			setValidMessage($(mobileCaptchaInput), true, "");
		} else {
			setValidMessage($(this), false, "请输入手机验证码");
		}
	});

	// 提交
	bindMobileDialog.elem.find(".e-new-user-account-login").off("tap").on("tap",function() {
		mobileInput.trigger("blur");
		captchaInput.trigger("blur");
		mobileCaptchaInput.trigger("blur");

		var mobileCheck = $(mobileInput).attr("check");
		var captchaCheck = $(captchaInput).attr("check");
		var mobileCaptchaCheck = $(mobileCaptchaInput).attr("check");
		if (mobileCheck == "true" && captchaCheck == "true"	&& mobileCaptchaCheck == "true") {
			var json = {'mobileCaptcha' : mobileCaptchaInput.val(),'mobile' : mobileInput.val()};
			var result = loxia.syncXhrGet(base+ "/member/bindMobile", json);
			if (result) {
				if (result.result) {
					if(loginData){
						//合并购物车
						bindMobileDialog.hide();
						syncShoppingCart(loginData);
					}else{
						if (!isNotNullOrEmpty(backUrl)) {
							backUrl = base + "/myaccount";
						}
						bindMobileDialog.hide();
						dialogBindMobileSuccess(backUrl);
					}
				} else {
					var statusCode = result.statusCode;
					if (statusCode == "member.not.login") {
						setValidMessage(mobileInput, false,"请重新登录!");
					} else if (statusCode == "member.mobile.error") {
						setValidMessage(mobileInput, false,"请输入正确的手机号码");
					} else if (statusCode == "loginmobile.unavailable") {
						setValidMessage(mobileInput, false,"此手机号码已被注册");
					} else if (statusCode == "mobileCaptcha.error") {
						setValidMessage(mobileCaptchaInput,false, "手机验证码不正确或已过期");
					} else {
						setValidMessage(mobileCaptchaCheck,false, "绑定失败，请重新再试");
					}
				}
			}
		}
	});

	// 关闭
	bindMobileDialog.elem.find(".e-spice-dialog-icon-close").off("tap").on("tap", function() {
		if(loginData){
			//合并购物车
			bindMobileDialog.hide();
			syncShoppingCart(loginData);
		}else{
			if (backUrl) {
				window.location.href = backUrl;
			} else {
				window.location.href = base + "/myaccount";
			}
		}
	});
}

/**
 * 完善信息成功
 */
function dialogBindMobileSuccess(backUrl) {
	var dialogeffect = window.commonEffectFn.dialogeffect;
	var bindMobileDialogSuccess = new dialogeffect({
		template : $('#common-dialog-bind-mobile-success').html(),
		dialogClass : 'new-member-dialog'
	});
	bindMobileDialogSuccess.show();

	// 确定
	bindMobileDialogSuccess.elem.find(".spice-btn-golden").off("tap").on("tap",function() {
		window.location.href = backUrl;
	});

	// 关闭
	bindMobileDialogSuccess.elem.find(".e-spice-dialog-icon-close").off("tap").on("tap", function() {
		window.location.href = backUrl;
	});
}

/**
 * 顶部注册
 */
function dialogHeaderRegister() {
	
	var newMemberDialog = window.headerDialog.dialogHeaderRegister;
	
	// 加载验证码
	var captchaDiv = newMemberDialog.elem.find(".spice-identifying-png");
	refreshCaptcha(captchaDiv);

	newMemberDialog.show();

	// 刷新验证码
	newMemberDialog.elem.find(".spice-identifying-text").off("tap").on("tap",function() {
		refreshCaptcha(captchaDiv);
	});

	var mobileInput = newMemberDialog.elem.find("#registerNumber");
	var captchaInput = newMemberDialog.elem.find("#tupian_match");
	var passWordInput = newMemberDialog.elem.find("#password");
	var confirmPasswordInput = newMemberDialog.elem.find("#confirmPassword");
	var emailInput = newMemberDialog.elem.find("#userSendEmail");
	var mobileCaptchaInput = newMemberDialog.elem.find("#messageCode");
	var agreeClause = newMemberDialog.elem.find("#agreeClause");

	// 手机校验
	mobileInput.off("tap").on("blur",function() {
		var mobile = $.trim($("#registerNumber").val());
		if (isNotNullOrEmpty(mobile)) {
			if (checkPhone(mobile)) {
				var json = {'mobile' : mobile};
				var data = loxia.syncXhrPost(base+ "/member/checkRegistermobileAvailable",json);
				if (data.result) {
					setValidMessage(mobileInput, true, "");
				} else {
					setValidMessage(mobileInput,false,loxia.getLocaleMsg("此手机号码已被注册"));
				}
			} else {
				setValidMessage($(this), false, "请输入正确的手机号码");
			}
		} else {
			setValidMessage($(this), false, "请输入手机号码");
		}
	});

	// 图形验证码
	captchaInput.off("blur").on("blur", function() {
		var userInput = $(this).val();
		if (isNotNullOrEmpty(userInput)) {
			setValidMessage($(captchaInput), true, "");
		} else {
			setValidMessage($(captchaInput), false, "请输入图形验证码");
			$(captchaInput).parent().parent().next().find("span").show().html("请输入图形验证码");
		}
	});
	
	// 手机验证码
	mobileCaptchaInput.off("blur").on("blur", function() {
		var userInput = $(this).val();
		if (isNotNullOrEmpty(userInput)) {
			setValidMessage($(mobileCaptchaInput), true, "");
		} else {
			setValidMessage($(mobileCaptchaInput), false, "请输入手机验证码");
			$(mobileCaptchaInput).parent().parent().next().find("span").show().html("请输入手机验证码");
		}
	});
	
	// 发送短信
	var oldchangeMobile=$.trim($("#registerNumber").val());
	var sendSpan = newMemberDialog.elem.find(".send-check");
	sendSpan.off("tap").on("tap",function() {
		if (!$(sendSpan).hasClass("is-disabled")) {
			captchaInput.trigger("blur");
			mobileInput.trigger("blur");
			var mobileCheck = $(mobileInput).attr("check");
			var captchaCheck = $(captchaInput).attr("check");
			
			if (mobileCheck == "true" && captchaCheck == "true") {//手机号正确并且验证码正确
				var instanceId = newMemberDialog.elem.find(".spice-identifying-png").find("input[type='hidden']").val();
				var userInput = $.trim($(captchaInput).val());
				var mobile = $.trim(mobileInput.val());
				var json = {
					'captchaId' : "captcha_pc_login",
					'userInput' : userInput,
					'instanceId' : instanceId,
					'mobile' : mobile
				};
				var result = loxia.syncXhrPost(base+ "/member/sendRegistermobileMessage",json);
				if (result) {
					if (result.result) {
						//发送短信成功
					    $(".message-code").removeClass("spice-error");
						$(sendSpan).addClass("is-disabled");
						var count = 60;
						$(sendSpan).html('重新发送(' + count + ')');
						var timerCountDown = setInterval(
								function() {
									if (count > 1) {
										count--;
										$(sendSpan).html('重新发送(' + count + ')');
									} else {
										clearInterval(timerCountDown);
										$(sendSpan).removeClass("is-disabled");
										$(sendSpan).html("获取验证码");
									}
								}, 1000);
					} else {
						var statusCode = result.statusCode;
						if (statusCode == "captcha.error") {
							setValidMessage($(captchaInput),false, "图形验证码不正确或已过期");
							refreshCaptcha(captchaDiv);
						} else if (statusCode == "member.mobile.error") {
							setValidMessage(mobileInput, false,"请输入正确的手机号码");
						} else if (statusCode == "loginmobile.unavailable") {
							setValidMessage(mobileInput, false,"此手机号码已被注册");
						} else {
							setValidMessage(mobileCaptchaInput,false, "发送验证码失败，请重新在试");
						}
					}
				}
			
			}
		}
	});

	// 设置密码和确认密码
	passWordInput.off("blur").on("blur",function() {
		var password = $(this).val();
		if (!isNotNullOrEmpty(password)) {
			$("#password").parent().parent().addClass("spice-error");
			$("#password").parent().next().find("span").show().html("密码不能为空");
		} else {
			if (isPassword(password)) {
				$("#password").parent().parent().removeClass("spice-error");
				$("#password").parent().next().find("span").hide();
			} else {
				$("#password").parent().parent().addClass("spice-error");
				$("#password").parent().next().find("span").show().html("至少6个字符含数字大小写字母");
			}
		}
	});

	confirmPasswordInput.off("blur").on("blur",function() {
		var confirmpassword = $(this).val();
		if (!isNotNullOrEmpty(confirmpassword)) {
			$("#confirmPassword").parent().parent().addClass("spice-error");
			$("#confirmPassword").parent().next().find("span").show().html("确认密码不能为空");
		} else {
			var password = $("#password").val();
			if (isPassword(password)) {// 确保密码符合规范
				if (confirmpassword == password) {
					$("#confirmPassword").parent().parent().removeClass("spice-error");
					$("#confirmPassword").parent().next().find("span").hide();
				} else {
					$("#confirmPassword").parent().parent().addClass("spice-error");
					$("#confirmPassword").parent().next().find("span").show().html("确认密码和密码需要保持一致");
				}
			} else {
					$("#confirmPassword").parent().parent().addClass("spice-error");
					$("#confirmPassword").parent().next().find("span").show().html("确认密码和密码需要保持一致");
			}
		}
	});
	
	//mobile密码失焦事件
	$("#mobile_password").off("blur").on("blur",function(){
		var mobiePassword=$(this).val();
		if(!isNotNullOrEmpty(mobiePassword)){
			setValidMessage($(this), false,"密码不能为空");
		}else{
			if(isPassword(mobiePassword)){
				setValidMessage($(this), true,"");
			}else{
				setValidMessage($(this), false,"至少6个字符含数字大小写字母");
			}
		}
	})

	// 邮箱
	emailInput.off("tap").on("blur",function() {
			var checkDiv = newMemberDialog.elem.find(".spice-myCheckbox");
			if (checkDiv.hasClass("spice-active")) {
				var userEmail = $.trim(emailInput.val().toLowerCase());
				if (!isNotNullOrEmpty(userEmail)) {
					setValidMessage(emailInput,	false,loxia.getLocaleMsg("请输入电子邮箱"));
				} else {
					if (checkEmail(userEmail)) {
						var json = {"email" : userEmail};
						var data = loxia.syncXhrPost(base+ "/member/checkLoginEmailAvailable",json);
						if (data.result) {
							setValidMessage(emailInput, true, "");
						} else {
							setValidMessage(emailInput,false,loxia.getLocaleMsg("此电子邮箱已被注册，请重新输入"));
						}
					} else {
						setValidMessage(emailInput,false,loxia.getLocaleMsg("请输入正确的电子邮箱"));
					}
				}
			} else {
				setValidMessage(emailInput, true, "");
			}
	});
	
	//新修改的
	newMemberDialog.elem.find('.spice-checkbox').off('tap').on('tap', function(e){
		  		var $checkbox = $(this)
		        ,$thisFather = $checkbox.parents(".spice-form-group")

		    		if($(e.target).is('a')){
		    			e.stopPropagation()
		    		}else{
		    		if( $checkbox.hasClass('spice-active') ){
			        $checkbox.removeClass('spice-active');
			        if(!$thisFather.next('.email-place').hasClass('spice-none')){
			          $thisFather.next('.email-place').addClass('spice-none')
			        }
			      }else{
			        $checkbox.addClass('spice-active');
			        $thisFather.removeClass("spice-error");
			        if($thisFather.next('.email-place').hasClass('spice-none')){
			          $thisFather.next('.email-place').removeClass('spice-none')
			        }
			        setValidMessage($("#agreeClause"), true, "");
			      }
			      return false;
		    	}
			});
	
	// 创建账号
	newMemberDialog.elem.find("#createAccount").off("tap").on("tap",function() {
		
			
			if (!agreeClause.hasClass("spice-active")) {
				setValidMessage(agreeClause, false, "请接受隐私政策和销售条款");
				$(agreeClause).next().find("span").show().html("请接受隐私政策和销售条款");
				
			}else{
				setValidMessage(agreeClause, true, "");
			}
			
			
			var tupian_match=$("#tupian_match").val();
			if(!isNotNullOrEmpty(tupian_match)){
				setValidMessage($("#tupian_match"), false, "请输入图形验证码");
			}else{
				setValidMessage($("#tupian_match"), true, "");
			}
			
			mobileInput.trigger("blur");
			mobileCaptchaInput.trigger("blur");
			passWordInput.trigger("blur");
			confirmPasswordInput.trigger("blur");
			emailInput.trigger("blur");
			$("#mobile_password").trigger("blur")
			$("#tupian_match").trigger("blur");
	
			var mobileCheck = mobileInput.attr("check");
			var mobileCaptchaCheck = mobileCaptchaInput.attr("check");
			var passWordCheck=$("#password").parent().parent().hasClass("spice-error");
			var confirmPasswordCheck=$("#confirmPassword").parent().parent().hasClass("spice-error");
			var mobile_password=$("#mobile_password").attr("check");
				
			var emailCheck = emailInput.attr("check");
			if(checkMobileOrPC() == "mobile"){
				if (mobileCheck == "true" && mobileCaptchaCheck == "true" && mobile_password == "true" && emailCheck == "true" && agreeClause.hasClass("spice-active")) {
					var mobile = $.trim(mobileInput.val());
					var mobileCaptcha = $.trim(mobileCaptchaInput.val());
					var mobile_password =$("#mobile_password").val();
					var email = $.trim(emailInput.val());
		
					var crypt = new JSEncrypt();
					crypt.setPublicKey($("#sensitivePublicKey").val());
					var enc_mobile_password = crypt.encrypt(mobile_password);
					
					var email=$.trim(emailInput.val().toLowerCase());
					// 发送注册请求到后台
					var json = {
						"loginMobile" : mobile,
						"captcha" : mobileCaptcha,
						"password" : enc_mobile_password,
						"repassword" : enc_mobile_password,
					};
					
					if($(".spice-myCheckbox").hasClass("spice-active")){
						if(isNotNullOrEmpty(email)){
							json["loginEmail"]=email;
							json["receiveMail"]="1";
						}
					}else{
						json["receiveMail"]="0";
					}
					
					var data = loxia.syncXhrPost(base + "/member/register",	json);
					
					if (data && data.result) {
						window.location.href = base + "/myaccount";
					} else {
						var returnObj = data.returnObject;
						if(returnObj!=null){
							if(returnObj["loginMobile"]=="register.loginmobile.unavailable"){
								oldMemberLogin(mobile);
							}
							
							if(returnObj["loginEmail"]=="register.loginemail.unavailable"){
								oldMemberLogin(email);
							}
						}else{
							var resultMessage = data.resultMessage;
							if(isNotNullOrEmpty(resultMessage)){
								var msgCode=resultMessage.message;
								if(msgCode=="loginMobile.field.required"){
									setLoginValidMessage(mobileInput, false, "请输入手机号码");
								}else if(msgCode=="password.field.required"){
									setPassValidMessage(passWordInput, false, loxia.getLocaleMsg("member.login.passwordIsNull"));
								}else if(msgCode=="member.mobile.error"){
									setLoginValidMessage(mobileInput, false, "请输入正确的手机号码");
								}else if(msgCode=="passwordAgain.error"){
									setPassValidMessage(confirmPasswordInput, false, loxia.getLocaleMsg("member.login.confirmPasswordError"));
								}else if(msgCode=="register.mobile.captcha.error"){
									setValidMessage(mobileCaptchaInput,false,loxia.getLocaleMsg("手机验证码不正确或已过期"));
								}else{
									setLoginValidMessage(passWordInput, false, loxia.getLocaleMsg("member.register.fail"));
								}
							}
						}				
					}
				}
			}else if (mobileCheck == "true" && mobileCaptchaCheck == "true"&& passWordCheck==false  && confirmPasswordCheck == false && emailCheck == "true" && agreeClause.hasClass("spice-active")) {
				var mobile = $.trim(mobileInput.val());
				var mobileCaptcha = $.trim(mobileCaptchaInput.val());
				var passWord = passWordInput.val();
				var confirmPassword = confirmPasswordInput.val();
				var email = $.trim(emailInput.val());
	
				var crypt = new JSEncrypt();
				crypt.setPublicKey($("#sensitivePublicKey").val());
				var enc_password = crypt.encrypt(passWord);
				var enc_password2 = crypt.encrypt(confirmPassword);
				
				var email=$.trim(emailInput.val().toLowerCase());
				// 发送注册请求到后台
				var json = {
					"loginMobile" : mobile,
					"captcha" : mobileCaptcha,
					"password" : enc_password,
					"repassword" : enc_password2,
				};
				if($(".spice-myCheckbox").hasClass("spice-active")){
					if(isNotNullOrEmpty(email)){
						json["loginEmail"]=email;
						json["receiveMail"]="1";
					}
				}else{
					json["receiveMail"]="0";
				}
				var data = loxia.syncXhrPost(base + "/member/register",	json);
				
				if (data && data.result) {
					window.location.href = base + "/myaccount";
				} else {
					var returnObj = data.returnObject;
					if(returnObj!=null){
						if(returnObj["loginMobile"]=="register.loginmobile.unavailable"){
							oldMemberLogin(mobile);
						}
						
						if(returnObj["loginEmail"]=="register.loginemail.unavailable"){
							oldMemberLogin(email);
						}
					}else{
						var resultMessage = data.resultMessage;
						if(isNotNullOrEmpty(resultMessage)){
							var msgCode=resultMessage.message;
							if(msgCode=="loginMobile.field.required"){
								setLoginValidMessage(mobileInput, false, "请输入手机号码");
							}else if(msgCode=="password.field.required"){
								setPassValidMessage(passWordInput, false, loxia.getLocaleMsg("member.login.passwordIsNull"));
							}else if(msgCode=="repassword.field.required"){
								setPassValidMessage(confirmPasswordInput, false, loxia.getLocaleMsg("member.login.confirmPasswordIsNull"));
							}else if(msgCode=="member.mobile.error"){
								setLoginValidMessage(mobileInput, false, "请输入正确的手机号码");
							}else if(msgCode=="passwordAgain.error"){
								setPassValidMessage(confirmPasswordInput, false, loxia.getLocaleMsg("member.login.confirmPasswordError"));
							}else if(msgCode=="register.mobile.captcha.error"){
								setValidMessage(mobileCaptchaInput,false,loxia.getLocaleMsg("手机验证码不正确或已过期"));
							}else{
								setLoginValidMessage(passWordInput, false, loxia.getLocaleMsg("member.register.fail"));
							}
						}
					}				
				}
			}
			
			
			
			
	});
}

//刷新验证码
function refreshCaptcha(captchaDiv){
	
	//移除captcha
	$(captchaDiv).find("input,img").not(".pre-gif").remove();
	//显示loading图
	$(captchaDiv).find(".pre-gif").removeClass("spice-none");

	var result=loxia.syncXhrPost(base+"/member/loadCaptcha",{});
	if(result){
		$(result).appendTo(captchaDiv);
		
		$(captchaDiv).find("img").not(".pre-gif").hide();
		$(captchaDiv).find("img").not(".pre-gif").on("load",function(){
			$(this).show();
			$(captchaDiv).find(".pre-gif").addClass("spice-none");
		})
	}
}

function checkMobileOrPC(){
    var media = null
        , w = window.innerWidth
    if ( w <= 767 ) {
        media = 'mobile'
    } else if( w <= 1023 ){
        media = 'pad'
    } else if( w == 1024){
        media = 'wpad'
    } else{
        media = 'pc'
    }
    return media
}

/**
 * LB只在用户第一次会话时出现。也就是说第二次会话（关了浏览器之后再来）就没有了。 LB只在某个目标页面被第一次访问到时才会弹出。
 * 如果后台更改了目标页面，那么之后这个页面就不会在显示LB了。这里的User是指后台配置者。 如果有关闭按钮，并且用户点了关闭按钮，那么关闭LB。
 * 在Mobile上，如果用户切换了页面或者点了最小化按钮，那么LB最小化。
 * 在PC上，如果用户切换了页面，LB会在后续的6个页面上持续保持最小化。但在第7个页面上会关闭。
 * 如果一个LB因为以上原因被最小化了，那么当一个新的目标页面被打开时，LB会重新变大。然后以上最小化的规则会被重新计算，重新计数
 * 在后台，应该可以设置哪些页面是目标页面，或者设置出了某些页面外其他页面都显示。但无论如何，购物车和 checkout页面都不应该显示，连最小化都不应该有。
 * 如果没有设置目标页，那么默认所有页面都显示。 PC 和Pad：页面打开5秒后。 手机：看第二个页面时。
 */

// lightbox
var type_narrow = "narrow";
var type_enlarge = "enlarge";
var type_close = "close";

// lightbox状态
var cookie_lightbox_show_status = "c_lb_s_s";
// 剩余存活次数
var cookie_lightbox_left_live_times = "c_lb_l_l_ts";
// 存活次数
var cookie_lightbox_live_times = "c_lb_l_ts";
// lightbox版本
var cookie_lightbox_version = "c_lb_v";

var media = null,
checkMedia = function(){
    var media = null
        , w =  window.innerWidth
    if ( w <= 767 ) {
        media = 'mobile'
    } else if( w <= 1023 ){
        media = 'pad'
    } else{
        media = 'pc'
    }
    return media
}

$(function() {
	// 校验当前lightbox版本
	var lightbox_version = getCookie(cookie_lightbox_version);

	// 未获取到版本的直接返回
	if (lightBoxVersion == "") {
		return;
	}

	// 如果版本不一致，初始化lightbox相关缓存
	if (lightBoxVersion != lightbox_version) {
		Cookies.set(cookie_lightbox_left_live_times, "", -1);
		Cookies.set(cookie_lightbox_live_times, "", -1);
		Cookies.set(cookie_lightbox_version, lightBoxVersion);
		Cookies.set(cookie_lightbox_show_status, "", -1);
	}

	// 获取通用参数
	// live800的状态，
	var live800_status = getCookie("live800_openWindow") != "open" && getCookie("live800_openWindow") != "min" ? false : true;

	// 如果live800为展开状态，则一律不显示lightbox,live800关闭后重新计数
	if (!live800_status) {
		// 从cookie中获取剩余数据
		var left_live_times = getCookie(cookie_lightbox_left_live_times);
		var live_times = getCookie(cookie_lightbox_live_times);
		var show_status = getCookie(cookie_lightbox_show_status);

		// 判断是否为所有页面加载lightbox，为全部页面时，第一次加载lightbox，pc、mobile有不同的加载方式
		if (lightBoxRangeType == "all") {
			// PC端第一次进入页面，mobile端第二次进入页面
			if ("" == left_live_times || live_times == left_live_times) {
				// 目标页
				if (lightBoxIsTarget == "true") {
					if (isMobileDevice == "true") {
						if ("" == left_live_times) {// mobile第一次进入目标页
							// 将剩余存活次数置为存活次数
							Cookies.set(cookie_lightbox_left_live_times, lightBoxLiveTimes);
							Cookies.set(cookie_lightbox_live_times, lightBoxLiveTimes);
							Cookies.set(cookie_lightbox_show_status, type_enlarge);
						} else if (left_live_times == live_times) {// mobile第二次进入目标页,打开lightbox
							runLightbox(lightBoxContentUrl, lightBoxTemplate, type_enlarge, left_live_times);
						} else {
							runLightbox(lightBoxContentUrl, lightBoxTemplate, type_enlarge, left_live_times);
						}
					} else {// pc第一次进入目标页,5s后加载lightbox
						if ("" == left_live_times) {
							Cookies.set(cookie_lightbox_left_live_times, lightBoxLiveTimes);
							Cookies.set(cookie_lightbox_live_times, lightBoxLiveTimes);
							Cookies.set(cookie_lightbox_show_status, type_enlarge);
							setTimeout(function() {
								runLightbox(lightBoxContentUrl, lightBoxTemplate, type_enlarge, lightBoxLiveTimes);
							}, 5000);
						}
					}
				}
			} else {
				// 剩余存活次数大于0
				if (parseInt(left_live_times) < parseInt(live_times) && parseInt(left_live_times) >= 0) {
					if (lightBoxIsTarget == "true" && show_status != type_close) {
						if (isMobileDevice == "true") {
							runLightbox(lightBoxContentUrl, lightBoxTemplate, type_narrow, left_live_times);
						} else {
							setTimeout(function() {
								runLightbox(lightBoxContentUrl, lightBoxTemplate, type_narrow, left_live_times);
							}, 5000);
						}
					} else {
						// 不支持的页面和非目标页 啥事不干
					}
				} else {
					// 存活次数已空，再见
				}
			}
		} else {// 目标页不是所有页面
			// PC端第一次进入页面，mobile端第二次进入页面
			if ("" == left_live_times) {
				// 目标页
				if (lightBoxIsTarget == "true") {
					// 将剩余存活次数置为存活次数
					Cookies.set(cookie_lightbox_left_live_times, lightBoxLiveTimes);
					Cookies.set(cookie_lightbox_live_times, lightBoxLiveTimes);
					Cookies.set(cookie_lightbox_show_status, type_enlarge);
					setTimeout(function() {
						runLightbox(lightBoxContentUrl, lightBoxTemplate, type_enlarge, lightBoxLiveTimes);
					}, 5000);
				}
			} else {
				// 剩余存活次数大于0
				if (parseInt(left_live_times) < parseInt(live_times) && parseInt(left_live_times) > 0) {
					if (lightBoxIsTarget == "true" && show_status != type_close) {
						runLightbox(lightBoxContentUrl, lightBoxTemplate, type_narrow, left_live_times);
					} else {
						// 不支持的页面和非目标页 啥事不干
					}
				} else {
					// 存活次数已空，再见
				}
			}
		}
	}
});

// 唤起lightbox
function runLightbox(contentUrl, template, status, times) {
	// // 关闭按钮
	// $(".e-spice-lightbox-close").on("tap", function() {
	// lightboxFunctionClose();
	// Cookies.set(cookie_lightbox_show_status, type_close);
	// });
	//
	// // 缩小
	// $(".e-spice-lightbox-small").on("tap", function() {
	// lightboxFunctionMin();
	// Cookies.set(cookie_lightbox_show_status, type_narrow);
	// });
	//
	// // 放大
	// $(".spice-lightbox-smalltit").on("tap", function() {
	// lightboxFunctionMax();
	// Cookies.set("cookie_show_status", type_enlarge);
	// });
	//
	// if(status == type_narrow){
	// $(".e-spice-lightbox-small").trigger("tap");
	// }else if(status == type_close){
	// $(".e-spice-lightbox-close").trigger("tap");
	// }else{
	// $(".spice-lightbox-smalltit").trigger("tap");
	// }

	Cookies.set(cookie_lightbox_left_live_times, parseInt(times) - 1); // 记录剩余次数
	Cookies.set(cookie_lightbox_show_status, status);

	// 调起lightbox
	var url = base + "/cms/data/custom";
	var data = loxia.syncXhrPost(url, {
		instanceUrl : contentUrl
	});
	if (null != data) {
		$(data.content).appendTo($("body"));
		if (status == type_narrow) {
			$(".e-spice-lightbox-small").trigger("tap");
		} else if (status == type_close) {
			$(".e-spice-lightbox-close").trigger("tap");
		} else {
			$(".spice-lightbox-smalltit").trigger("tap");
		}
	}
}

function maxLightbox() {
	Cookies.set("c_lb_s_s", "enlarge");
}

function minLightbox() {
	Cookies.set("c_lb_s_s", "narrow");
}

function closeLightbox() {
	Cookies.set("c_lb_s_s", "close");	
}




loxia.regional['zh_CN'] = {
	"member.login.emailOrMobileError":"请输入正确的手机号码/电子邮箱",
	"member.login.emailOrMobileIsNull":"请输入手机号码/电子邮箱",
	"member.reset.emailNotExisit":"该邮箱不存在",
	"member.reset.mobileNotExisit":"该手机号不存在",
	"member.login.emailIsNull":"请输入电子邮箱",
	"member.login.emailError":"请输入正确的电子邮箱",
	"member.login.mobileIsNull":"请输入手机号码",
	"member.login.mobileError":"请输入正确的手机号码",
	"member.login.captchaCodeIsNull":"请输入图形验证码",
	"member.login.captchaCodeError":"图形验证码不正确或已过期",
	"member.login.smsCodeIsNull":"请输入手机验证码",
	"member.login.smsCodeError":"手机验证码不正确或已过期",
	"member.login.getCaptchaCode":"获取验证码",
	"member.login.sendSmsError":"发送验证码失败，请重新再试",
	"member.login.resetPasswordError":"重置密码失败，请重新再试",
	"member.register.messageCode.isNull":"请输入手机验证码",
