		//调用的方式 S.lightBox({data:{type:{decor:1}}})

	}

	$(function(){
		spice.init({
			throttleTime:100
			, debounceTime:300
			, resize: {
		        standard: 1023
		        , medium: 768
		        , small: 767
		    }

		})

		S.setMultiLanguage(['common'])

		var href = location.href
