}

.spice-dialog-icon-close{
  position: absolute;
  top: 0;
  right: 0;
  width: $dialog-close-icon-size;
  height: $dialog-close-icon-size;
  line-height: $dialog-close-icon-size;
  cursor: pointer;
  font-size: $dialog-close-icon-font-size;
  i{
    font-style: normal;
  }
} */
