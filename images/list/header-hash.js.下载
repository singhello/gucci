	    // 购物车 pad + mobile
	    // window.showMiniShoppingCartOnPadMob.call($('.e-spice-medium .e-header-shoppingcart > a, .e-spice-small .e-header-shoppingcart > a'));
	    window.showMiniShoppingCartOnPadMob = function(){
	      var $self = $(this),
	          $target,
	          $subMenu = $self.next('.spice-sub-menu'),
	          $parent = $self.parent(),
	          $searchDom
	      if (!$parent.hasClass('spice-active')) {
	          if(( $searchDom = $parent.next('.e-header-search')).hasClass('spice-active') ){
	              $searchDom.removeClass('spice-active');
	          }

	          $parent.addClass('spice-active');
	          $('.spice-mini-list-tinyscrollbar .spice-nav .spice-tinyscrollbar').data('spice.Tinyscrollbar') && $('.spice-mini-list-tinyscrollbar .spice-nav .spice-tinyscrollbar').data('spice.Tinyscrollbar').update()
	          $('body').on('tap.shoppingcart', function(e) {
	              var $target = $(e.target),
	                  $subMenu = $('> .spice-sub-menu', $shoppingcart)
	              if (!$target.hasClass('e-header-shoppingcart') && $target.closest('.e-header-shoppingcart').length == 0) {
	                  if ($parent.hasClass('spice-active')) {
	                      $shoppingcart.removeClass('spice-active')
	                      $('body').off('tap.shoppingcart')
	                    //   return false
	                  }
	              }
	          })
	      } else {
	          $parent.removeClass('spice-active')
	          $('body').off('tap.shoppingcart')
	      }
	    //   return false
	    }

	    // mini购物车产品hover效果
	    window.miniShoppingcartHoverAction=function(){
	      if(spice.hasTouch){
	        $('.spice-nav','.e-header-shoppingcart').find('li').on({
	          touchstart:function(){
	            var $dom = $(this);
	            bgcolorShow($dom);
	          },
	          touchend:function(){
	            var $dom = $(this);
	            bgcolorHide($dom);
	          }
	        })
	      }else{
	        $('.spice-nav','.e-header-shoppingcart').find('li').on({
	          mouseenter:function(e){
	            var $dom = $(this);
	            bgcolorShow($dom);
	          },
	          mouseleave:function(e){
	            var $dom = $(this);
	            bgcolorHide($dom);
	          }
	        })
	      }
	    }
	    // mini心愿单产品hover效果
	    window.miniHeartcartHoverAction=function(){
	      if(spice.hasTouch){
	        $('.spice-nav','.e-header-wish-list').find('li').on({
	          touchstart:function(){
	            var $dom = $(this);
	            bgcolorShow($dom);
	          },
	          touchend:function(){
	            var $dom = $(this);
	            bgcolorHide($dom);
	          }
	        })
	      }else{
	        $('.spice-nav','.e-header-wish-list').find('li').on({
	          mouseenter:function(e){
	            var $dom = $(this);
	            bgcolorShow($dom);
	          },
	          mouseleave:function(e){
	            var $dom = $(this);
	            bgcolorHide($dom);
	          }
	        })
	      }
	    }

	    
	    function bgcolorShow(dom){
	      dom.find('.spice-background-gray').show();
	      dom.find('.spice-background-white').hide();
	      dom.css('backgroundColor','rgba(229,223,217,0.5)');
	    }
	    function bgcolorHide(dom){
	      dom.find('.spice-background-gray').hide();
	      dom.find('.spice-background-white').show();
	      dom.css('backgroundColor','#ffffff');
	    }
	    var handler=function (event) {
	        event.preventDefault();
	    }
	   function disableScroll(state){
	        document[state?"addEventListener":"removeEventListener"]('touchmove',handler);
	    }

