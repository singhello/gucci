	    // 显示关闭mini购物车和心愿单
	    // 调用方法 showWishList()
	    window.showWishList = function(){
	      addMouseenter($('.e-spice-standard .e-header-wish-list'),timerWish);
	      $('.spice-mini-list-tinyscrollbar .spice-nav .spice-tinyscrollbar').data('spice.Tinyscrollbar') && $('.spice-mini-list-tinyscrollbar .spice-nav .spice-tinyscrollbar').data('spice.Tinyscrollbar').update()
	    }
	    window.hideWishList = function(){
	      addMouseleave($('.e-spice-standard .e-header-wish-list'),timerWish);
	    }
	    window.showMiniShoppingCart = function(){
	      addMouseenter($('.e-spice-standard .e-header-shoppingcart'),timerShoppingcart);
	      $('.spice-mini-list-tinyscrollbar .spice-nav .spice-tinyscrollbar').data('spice.Tinyscrollbar') && $('.spice-mini-list-tinyscrollbar .spice-nav .spice-tinyscrollbar').data('spice.Tinyscrollbar').update()
	    }
	    window.hideMiniShoppingCart = function(){
	      addMouseleave($('.e-spice-standard .e-header-shoppingcart'),timerShoppingcart);
	    }
	    window.showUserLogin = function(){
	      addMouseenter($('.e-spice-standard .spice-nav-btn-login'),timerLogin);
	    }
	    window.hideUserLogin = function(){
	      addMouseleave($('.e-spice-standard .spice-nav-btn-login'),timerLogin);
	    }
	    window.showUserLoginAfter = function(){
	      addMouseenter($('.e-spice-standard .e-header-login-after'),timerLoginAfter);
	    }
	    window.hideUserLoginAfter = function(){
	      addMouseleave($('.e-spice-standard .e-header-login-after'),timerLoginAfter);
	    }

