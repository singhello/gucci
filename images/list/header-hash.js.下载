		function footerLiSlide(self) {
			var _this = $(self),
				_thisParent = _this.parent();

			console.log(_thisParent.attr('class'));
			if (_thisParent.hasClass('spice-open')) {
				_thisParent.removeClass('spice-open');
			}else{
				_thisParent.addClass('spice-open');
			}

			if (_thisParent.hasClass('spice-form-registered')) {
				$('.e-slide-item').removeClass('spice-open');
				$('.e-slide-item ul').stop(true, true).slideUp(300);
				$('.spice-centent', _thisParent).stop(true, true).slideToggle(300);
			}else{
				$('.spice-form-registered').removeClass('spice-open');
				$('.spice-form-registered .spice-centent').stop(true, true).slideUp(300);
				_thisParent.siblings('.e-slide-item').removeClass('spice-open');
				_thisParent.siblings('.e-slide-item').find('ul,.spice-centent').stop(true, true).slideUp(300);
				_thisParent.find('ul,.spice-centent').stop(true, true).slideToggle(300);
			}
		}


		$('.spice-footer-left ul li .spice-footer-title').on('tap', function() {
			if ( $('body').hasClass('e-spice-small') ) {
				footerLiSlide(this)
			}
		});

		$('.spice-form-registered .spice-footer-title').on('tap', function() {
			if ( $('body').hasClass('e-spice-small') ) {
