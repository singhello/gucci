	    var wrapper, slideSize, wrapperSize, direction, isScrolling, containerSize;

	    /*=========================
	      Default Parameters
	      ===========================*/
	    var defaults = {
	        eventTarget: 'wrapper', // or 'container'
	        mode : 'horizontal', // or 'vertical'
	        touchRatio : 1,
	        speed : 300,
	        freeMode : false,
	        freeModeFluid : false,
	        momentumRatio: 1,
	        momentumBounce: true,
	        momentumBounceRatio: 1,
	        slidesPerView : 1,
