	// 小屏幕点击按钮展开菜单
	var $wrapper = $('#spice-wrapper'),
	    $navbarMenu = $('.e-navbar-menu'),
	    $navbarScroller = $('.spice-navbar-scroller-container', $navbarMenu),
	    $navToggleBtn = $('.e-navbar-btn-toggle'),
	    mask = new Mask({
	        maskClass: 'spice-header-mask'
	    }),
	    $curMenuSite = $navbarScroller,
	    getMenuHeight = function() {
	        var wH = $navbarMenu.outerHeight(),
	            menuHeight = 0

	        $('>*', $curMenuSite).outerHeight(function(i, n) {
	            menuHeight += n
	        })
	        return  menuHeight < wH ? wH : menuHeight;
	        // return  spice.android ? Math.max(wH,menuHeight)+80 :  Math.max(wH,menuHeight);
	    },
	    navbarMenuIScroll,
	    subMenuIScroll,
	    headerTimer = null,
	    bReady = true,
	    bReadyTimer = null,
	    originElem = null,
	    newElem = null,
	    hoverTime = null,
	    windowScrollTop = 0
	$wrapper.data('left', 0)
	$navToggleBtn.on('tap', function() {
	    var navbarWidth = $navbarMenu.outerWidth(true),
	        wrapperLeft = navbarWidth,
	        navbarLeft = 0,
	        classChange = 'addClass'

	    // $('html,body').scrollTop(0);

	    // 判断是否是展开状态
	    // 如果是展开状态，将元素 left 变成 0
	    if ($wrapper.data('left') != 0) {
	        wrapperLeft = 0
	        navbarLeft = -navbarWidth
	        classChange = 'removeClass'
	        $wrapper.data('left', 0)
	    } else {
	        mask.show()
	        $wrapper.data('left', navbarWidth)
	        var currentScrollTop = $(window).scrollTop();
	        window.scrollTo(0,-1);
	        setTimeout(function(){
	            $('body').scrollTop(currentScrollTop);
	        },0);
	    }

	    // 初始化iscroll插件
	    if (navbarMenuIScroll) {
	        navbarMenuIScroll.destroy()
	    }

	    if (spice.hasTouch) {
	        navbarMenuIScroll = new IScroll($('.spice-navbar-menu-container', $navbarMenu)[0], {
	            preventDefaultException: {
	                tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A|IMG|I|SPAN|DIV|UL|LI)$/
	            }
	        })
	    }

	    $navbarMenu
	        .css({
	            left: navbarLeft
	        })[classChange]('spice-navbar-menu-open')

	    $navbarScroller.height(getMenuHeight());
	    if ($('body').height() >= $(window).height()) {
	        $('body, html')[wrapperLeft == 0 ? 'removeClass' : 'addClass']('spice-overflow-hidden')
	    }

	    if (spice.hasTouch) {
	        $navbarMenu[navbarWidth == 0 ? 'removeClass' : 'addClass']('spice-overflow-hidden')
	        $('.spice-navbar-menu-return', $navbarMenu)[navbarWidth == 0 ? 'removeClass' : 'addClass']('spice-block-sm')
	    }

	    // 恢复 isroll效果并刷新效果
	    navbarMenuIScroll && navbarMenuIScroll.enable()
	    navbarMenuIScroll && navbarMenuIScroll.refresh()

