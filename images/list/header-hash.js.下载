	  return "							<li class=\"spice-menu-back spice-none spice-block-xs\">\r\n								<a><i class=\"spice-icon spice-icon-arrow-left\"></i>"
	    + container.escapeExpression(((helper = (helper = helpers.backWords || (depth0 != null ? depth0.backWords : depth0)) != null ? helper : helpers.helperMissing),(typeof helper === "function" ? helper.call(depth0 != null ? depth0 : (container.nullContext || {}),{"name":"backWords","hash":{},"data":data}) : helper)))
	    + "</a>\r\n							</li>\r\n";
	},"13":function(container,depth0,helpers,partials,data) {
	    var stack1, helper, alias1=depth0 != null ? depth0 : (container.nullContext || {}), alias2=helpers.helperMissing, alias3="function";

	  return "							<li"
	    + ((stack1 = helpers["if"].call(alias1,(depth0 != null ? depth0["class"] : depth0),{"name":"if","hash":{},"fn":container.program(14, data, 0),"inverse":container.noop,"data":data})) != null ? stack1 : "")
	    + "><a href=\""
	    + container.escapeExpression(((helper = (helper = helpers.href || (depth0 != null ? depth0.href : depth0)) != null ? helper : alias2),(typeof helper === alias3 ? helper.call(alias1,{"name":"href","hash":{},"data":data}) : helper)))
	    + "\">"
	    + ((stack1 = ((helper = (helper = helpers.words || (depth0 != null ? depth0.words : depth0)) != null ? helper : alias2),(typeof helper === alias3 ? helper.call(alias1,{"name":"words","hash":{},"data":data}) : helper))) != null ? stack1 : "")
	    + "</a></li>\r\n";
	},"14":function(container,depth0,helpers,partials,data) {
