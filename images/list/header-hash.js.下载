	        resizeReInit : false,
	        //DOMAnimation
	        DOMAnimation : true,
	        //Slides Loader
	        loader: {
	            slides: [], //array with slides
	            slidesHTMLType: 'inner', // or 'outer'
	            surroundGroups: 1, //keep preloaded slides groups around view
	            logic: 'reload', //or 'change'
	            loadAllSlides: false
	        },
	        // One way swipes
	        swipeToPrev: true,
	        swipeToNext: true,
