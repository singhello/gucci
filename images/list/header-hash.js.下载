	var headerMedia = null
	    , checkoutHeaderMedia = function(){
	        var media = null
	            , w = $(window).width()
	        if ( w <= 767 ) {
	            media = 'mobile'
	        } else if( w <= 1023 ){
	            media = 'pad'
	        } else{
	            media = 'pc'
	        }
	        return media
	    }
	$(window).on('resize.header', function() {
	    var mediaNow = checkoutHeaderMedia()
	    if ( headerMedia != mediaNow ) {
	        var $self = $(this)
	        mask.hide()

	        var isHeaderOpen = parseInt( $wrapper.css('left') ) > 0 ? true : false

	        $wrapper
