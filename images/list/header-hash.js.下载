	var windowData = window.multiLanguage;

	// 购物车 + 心愿单 + 登录 + 用户中心下拉
	// require.ensure([], function(require) {

	    var miniList = __webpack_require__(71),
	        headerLogin = __webpack_require__(84),
	        userCenter = __webpack_require__(91),
	        $shoppingcart = $('.e-header-shoppingcart'),
	        $headerShoppingcart = $('.spice-dropdown-menu-content', $shoppingcart),
