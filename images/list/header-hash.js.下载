	    $sendEmailBtn.on('tap.email', function() {
	        // dialogHeaderCustomerService.hide()
	        // dialogHeaderSendEmail.show()
	        // tab按键模拟处理
	        $('.spice-add-for-tab')
	            .off('focus.spiceForTab')
	            .on('focus.spiceForTab', function(e) {
	                var $parentDom = $(e.target).parent();
	                $('.spice-dropdown-btn', $parentDom).click();
	            })
	            .off('blur.spiceForTab')
	            .on('blur.spiceForTab', function(e) {
	                var $parentDom = $(e.target).parent();
	                $('.spice-dropdown-btn', $parentDom).click();
	                $parentDom.removeClass('spice-active');
	                $('.spice-dropdown-btn', $parentDom).removeClass('spice-active');
	            })
	    })
