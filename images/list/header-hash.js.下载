	},"9":function(container,depth0,helpers,partials,data) {
	    var stack1;

	  return "				<div class=\"spice-sub-menu\">\r\n					<ul>\r\n"
	    + ((stack1 = helpers.each.call(depth0 != null ? depth0 : (container.nullContext || {}),(depth0 != null ? depth0.blocks : depth0),{"name":"each","hash":{},"fn":container.program(10, data, 0),"inverse":container.noop,"data":data})) != null ? stack1 : "")
	    + "					</ul>\r\n				</div>\r\n";
	},"10":function(container,depth0,helpers,partials,data) {
