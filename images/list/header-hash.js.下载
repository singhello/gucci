/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, handlebars = __webpack_require__(8)

	// 重写if
	handlebars.registerHelper('if', function(index, val, opt) {
		if( opt ){
			if( index == val ){
				return opt.fn(this)
			}else{
				return opt.inverse(this)
			}
		}else{
			if( index ){
				return val.fn(this)
			}else{
				return val.inverse(this)
			}
		}
	})

	// handlebars.registerHelper('checkKey', function(key, val, opt) {
	// 	var m = window.multiLanguage
	//     	, index = (!key || !m) ? "" : m[key]
	// 	if( opt ){
	// 		if( index == val ){
	// 			return opt.fn(this)
	// 		}else{
	// 			return opt.inverse(this)
	// 		}
	// 	}else{
	// 		if( index ){
	// 			return val.fn(this)
	// 		}else{
	// 			return val.inverse(this)
	// 		}
	// 	}
	// })

	handlebars.registerHelper('isArray', function(val, opt) {
		if( $.isArray(val) ){
			return opt.fn(this)
		}else{
			return opt.inverse(this)
		}
	})

	handlebars.registerHelper('isPlainObject', function(val, opt) {
		if( $.isPlainObject(val) ){
			return opt.fn(this)
		}else{
			return opt.inverse(this)
		}
	})

	handlebars.registerHelper('term', function(o, e) {
		if( !o ) return ''
		if( $.type(o) === 'string' ){
			return new handlebars.SafeString(' ' + ($.type(e) === 'string' ? e : 'spice-data-term') + '=\'' + o + '\'') 
		}else if( $.type(o) === 'array' ){
			var term = []
			$.each( o, function(key, v){
				term.push( '"' + v.name + '":' + '"' + (v.id || v.text) + '"' )
			})
			return new handlebars.SafeString(' ' + ($.type(e) === 'string' ? e : 'spice-data-term') + '=\'{' + term.join(',') + '}\'')
		}else{
			var term = []
			$.each( o, function(key, value){
				term.push( '"' + key + '":' + '"' + value + '"' )
			})
			return new handlebars.SafeString(' ' + ($.type(e) === 'string' ? e : 'spice-data-term') + '=\'{' + term.join(',') + '}\'')
		}
		
	})

	handlebars.registerHelper('isTrue', function(o, opt) {
		if( o == 'true' ){
			return opt.fn(this)
		}else{
			return opt.inverse(this) 
		}
	})

