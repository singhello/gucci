	    //设置顶部导航弹层公用样式
	    var setHeaderDialogCommon = function(dialog){
	        dialog.headerIScroll = null
	        dialog.elem.on('spice.dialog-show-after', function(e,o){
	          var $elem = o.elem
	              , $headerCountryIScroll = $('.spice-dialog-top-position-scroll', $elem)
	          if ( $(window).width() <= 1023 && !!$headerCountryIScroll.length ) {
	              $headerCountryIScroll.height( Math.floor($(window).height() - $('.spice-dialog-top-position',$elem).height()))
	              dialog.headerIScroll && dialog.headerIScroll.destroy()
	              dialog.headerIScroll = new IScroll($headerCountryIScroll[0], {
	                  preventDefaultException: {
	                      tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A|IMG|I|SPAN|DIV|UL|LI)$/
	                  }
	              })

	              var $textareaDom = $elem.find('textarea');
	              if(!!$textareaDom && $textareaDom.length && spice.android){
	                //   dialog.headerIScroll.destroy();
	                //    $headerCountryIScroll.removeAttr('style');
	                  $textareaDom.off('foucs.mobile').on('focus.mobile',function(){
	                      var _this = this;
	                      $('.spice-form-group-btn',$elem).css('marginBottom',$(this).height()+50);
	                      // $(this).css('marginBottom',$(this).height()+50);
	                      // dialog.headerIScroll.scrollTo(0,currentPosTop - $(this).height()-50);
	                    dialog.headerIScroll.refresh();
	                  }).off('blur.mobile').on('blur.mobile',function(){
	                      $('.spice-form-group-btn',$elem).removeAttr('style');
	                      // $(this).removeAttr('style');
	                      dialog.headerIScroll.refresh();
	                  })
	              }
	          }
	        })
	        dialog.elem.on('spice.dialog-show-before', function(e,o){
	            $navbarMenu.addClass('header-hidden')
	        })
	        dialog.elem.on('spice.dialog-hide', function(e,o){
	            $navbarMenu.removeClass('header-hidden')
	        })
	        dialog.btnClose.on('tap', function(){
	            if ( $wrapper.data('left') != 0 ) {
	              $navToggleBtn.trigger('tap')
	            }
	            mask.hide()
	        })
	    }
	    setHeaderDialogCommon( dialogHeaderCountryMap )
	    setHeaderDialogCommon( dialogHeaderCustomerService )
	    setHeaderDialogCommon( dialogHeaderSendEmail )
	    setHeaderDialogCommon( dialogHeaderEmailSuccess )
	    setHeaderDialogCommon( dialogHeaderAboutGucci )

	    // 年月日初始化
	    var settingYmdelect;
	    if(!spice.hasTouch){
	        settingYmdelect = new ymdselecteffect('.e-spice-ymd-select.spice-special', {
	            defaultText: ['年', '月', '日'],
	            year: [1920, 2017]
	        });
	    }else{
	        settingYmdelect = new ymdselecteffect('.spice-semi-simulation.e-spice-ymd-select', {
	            defaultText: ['年', '月', '日'],
	            year: [1920, 2017]
	        });
	        $('.spice-semi-simulation.e-spice-ymd-select').removeClass('spice-none');
	        $('.e-spice-ymd-select.spice-special').addClass('spice-none');
	    }

