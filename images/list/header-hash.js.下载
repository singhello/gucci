/* 67 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
	    , spice = __webpack_require__(2)
		, template = __webpack_require__(68)
		, slot = __webpack_require__(54)
		, handlebars = __webpack_require__(8)

	__webpack_require__(70)

	function SlotStandard(config){
		this.config = $.extend(true, {}, this.config, config)
		this.template = template
		this.data = this.config
	}

	SlotStandard.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function(elem){
			this.drawEvents(elem)
		}
		, drawEvents: function( elem ){
			var $menuLeft = $('.header-nav-section-left', elem)
				, $menuRight = $('.header-nav-section-right', elem)
				, $carousel = $('.e-header-nav-carousel', elem)
				, isLeftEmpty = false
				, isRightEmpty = false

			var media = null
		    , checkoutMedia = function(){
		        var media = null
		            , w = $(window).width()
		        if ( w <= 767 ) {
		            media = 'mobile'
		        } else if( w <= 1023 ){
		            media = 'pad'
		        } else{
		            media = 'pc'
		        }
		        return media
		    }

			$menuLeft.addClass( 'header-nav-section-left-' + $('>li', $menuLeft).length )
			if ( $menuRight.length > 0 ) {
				$menuRight.addClass( 'header-nav-section-right-' + $('>div', $menuRight).length )
				if ( $('.header-nav-section-right-image', $menuRight).length > 0 ) {
					$('.spice-header-menu-container', elem).addClass('spice-header-menu-story')
					$(".right-bgimg",elem).on('spice.autoReplaceImageSrc-before', function() {
						$(".header-nav-section-right",elem).css("background-image","url("+src+")");
					});

					var media = checkoutMedia()
					if(media == "pc"){
						$(".header-nav-section-right",elem).removeClass("no-bg")
					}else{
						$(".header-nav-section-right",elem).addClass("no-bg")
					}
					$(window).on('resize.bgimg', function() {
						var isMobile =  window.innerWidth <= 1023
					 		,mediaNow = checkoutMedia()

					 	//刚切换至mb
					 	if ( mediaNow != media && isMobile) {
					 		console.log("to mb");
					 		$(".header-nav-section-right",elem).addClass("no-bg")
					 	}
					 	//pc
					 	if(!isMobile){

					 	}
					 	//刚切换至pc
					 	if( mediaNow != media && !isMobile ){
					 		// console.log("to pc");
					 		$(".header-nav-section-right",elem).removeClass("no-bg")
					 	}		
					 	media = mediaNow;
					}).trigger('resize.bgimg')
					
				}
				if ( $carousel.length == 0 ){
					$menuRight.addClass('header-nav-section-right-menuGroup-slide')
				}
			}

			if ( $menuLeft.length == 0 || $menuLeft.find('>*').length == 0 ) {
				isLeftEmpty = true
			}
			if ( $menuRight.length == 0 || $menuRight.find('>*').length == 0 ) {
				isRightEmpty = true
			}

			if( isLeftEmpty && isRightEmpty ) {
				elem.siblings('a').addClass('header-no-hover')
				elem.remove()
			}

			if( $menuLeft.length != 0 && isRightEmpty ) {
				$menuLeft.addClass('header-nav-section-no-right')
			}


			var $subMenuLeft = $menuLeft.find('.spice-menu-list:not(.spice-menu-list-sub-title) .spice-sub-menu')

			$subMenuLeft.each(function(i,j){
				var $self = $(j)
					, $li = $self.find('li')
					, l = Math.ceil( $li.length/2 )
					, $liA = $li.filter(':lt(' + l + ')')
					, $liB = $li.filter(':gt(' + (l-1) + ')')
				$self.html( $('<ul></ul><ul></ul>') )
				$('ul', $self).eq(0).html( $liA )
				$('ul', $self).eq(1).html( $liB )
			})

			$(window).trigger('resize.spice-autoReplaceImageSrc')
			
		}
	})

