	    dropdown = new dropdowneffect($sendEmailDropdown)
	    dropdownAboutGucci = new dropdowneffect($('.spice-dropdown:not(.spice-ymd-select .spice-dropdown)',$aboutGucciDropdown))


	    $sendEmailDropdown.add($aboutGucciDropdown).on('spice.dropdown-menu-tap', function(e, o) {
	        var $self = o.curMenu,
	            $btn = $('.spice-dropdown-text', o.self.elem)
	        $btn.text($('> a', $self).text())
	    })
	    $('select', $sendEmailDropdown).add($('select',$aboutGucciDropdown)).on('change', function() {
	        var $self = $(this),
	            $btn = $('.spice-dropdown-text', $self.siblings('.spice-dropdown-btn'))
	        $btn.text($('option:checked', $self).text())
	    }).on('focus', function(){
	        $(this).closest('.spice-dropdown').addClass('spice-active')
	    })
	    .on('blur', function(){
	        $(this).closest('.spice-dropdown').removeClass('spice-active')
	    })
	    // 添加国旗
	    // S.staticbase + '/data/multiLanguage/'+ S.locale + '/country.json'
	    // S.asyncXhrGet( '/country.json' , {}, function(data, textStatus, errorThrown){  //如果需要国家数据打开这行，将下面的if换成        if ( textStatus == "success" ) 
	        if ( window.countryJson ) {
	          $('.e-spice-country', dialogHeaderSendEmail.elem).add($('.e-spice-country', dialogHeaderAboutGucci.elem)).each(function(i,j){
	              var $country = $(j)
	                , $ul = $('ul', $country)
	                  , $select = $('select', $country)
	                  , $dropdown = $('.spice-dropdown', $country)
	                  , array = window.countryJson.content
	                  , html = []
	                  , li = ''
	                  , name = $('.spice-dropdown-btn .spice-country-name', $country).html()

	              if ( spice.hasTouch ) {
	                  for (var i = 0; i < array.length; i++) {
	                      var obj = array[i]
	                      if ( !$country.hasClass('e-spice-country-nophone') ) {
	                        html.push('<option spice-data-value="'+obj.callingCode+'">'+ obj.callingCode + ' ' + obj.countryName +'</option>')
	                      } else{
	                        if ( name == obj.countryName ) {
	                          html.push('<option spice-data-value="" selected="selected">'+ obj.countryName +'</option>')
	                        } else{
	                          html.push('<option spice-data-value="">'+ obj.countryName +'</option>')
	                        }
	                      }
	                  }
	                  li = html.join('');
	                  $select.html(li)
	                  $ul.closest('.spice-dropdown-menu').remove()
	              } else{
	                  for (var i = 0; i < array.length; i++) {
	                      var obj = array[i]
	                      if ( name == obj.countryName ) {
	                        html.push('<li spice-data-value="" class="spice-current">')
	                      } else{
	                        html.push('<li spice-data-value="">')
	                      }
	                      html.push('<a>')
	                      html.push('<i class="spice-icon-country" style="background-position: ' + obj.backgroundPosition + '"></i>')
	                      if ( !$country.hasClass('e-spice-country-nophone') ) {
	                        html.push('<span class="spice-calling-code">'+ obj.callingCode +'</span>')
	                    }
	                      html.push('<span class="spice-country-name">' + obj.countryName + '</span>')
	                      html.push('</a>')
	                      html.push('</li>')
	                  }
	                  li = html.join('');
	                  $ul.html(li)
	                  $select.remove()
	              }

	              $dropdown.trigger('spice.loadCountryDataEnd');

	              $dropdown
	              .find('li').on('tap.spice-country-tap', function(){
	                var $this = $(this)
	                  , $subMenu = $this.closest('.spice-dropdown-menu')
	                  , $li = $('li', $subMenu)
	                  , $text = $subMenu.siblings('.spice-dropdown-btn').find('.spice-dropdown-text')
	                $li.removeClass('spice-current')
	                $this.addClass('spice-current')
	                $text.html( $('>a', $this).html() )
	                $country.trigger('spice.spice-country-tap-li', {
	                    elem: $this
	                })
	              })
	          })


	        }
	    // })
	    //客户服务弹层内部展开收起
	    if (!!dialogHeaderCustomerService) {
	        var dialogHeaderCustomerServiceElem = dialogHeaderCustomerService.elem,
	            $CustomerServiceTitle = $('.spice-service-title', dialogHeaderCustomerServiceElem)
	        $CustomerServiceTitle.on('tap', function() {
	            var $self = $(this),
	                $submenu = $self.siblings('.spice-service-content'),
	                $parent = $self.closest('.spice-service-district'),
	                $parentSiblings = $parent.siblings('.spice-service-district')
	            $parent.toggleClass('spice-active')
	            $submenu.stop(true, true).slideToggle(300, function(){
	              dialogHeaderCustomerService.headerIScroll && dialogHeaderCustomerService.headerIScroll.refresh()
	              dialogHeaderCustomerService.headerIScroll && dialogHeaderCustomerService.headerIScroll.refresh()
	            })
	            $parentSiblings.removeClass('spice-active')
	                .find('.spice-service-content').stop(true, true).slideUp(300, function(){
	              dialogHeaderCustomerService.headerIScroll && dialogHeaderCustomerService.headerIScroll.refresh()
	              dialogHeaderCustomerService.headerIScroll && dialogHeaderCustomerService.headerIScroll.refresh()
	            })
	        })
	    }
	    dialogHeaderCustomerService.elem.on('spice.dialog-show-before', function(e, o){
	      S.setTel( $('.after-sale-phone'), '400.8210.582' )
	      S.setTel( $('.official-service-phone'), '400.8210.582' )
	      S.setTel( $('.official-service-phone-hk'), '852.2839.9511' )
	      S.setTel( $('.official-service-phone-ww'), '886.2.7706.8505' )
	    });
	    //发送邮件编辑弹层字数计算js
	    var messageLimit = __webpack_require__(37)
	    new messageLimit($('.events-messageLimit2'), {
	        maxCount: 400
	    })
	    //国家弹层pad mobile时收起展开
	    $(document).on('tap', '.e-spice-medium .spice-district-title, .e-spice-small .spice-district-title', function() {
	        var $self = $(this),
	            $content = $self.closest('.country-content'),
	            $submenu = $self.siblings('.spice-district-country'),
	            $notList = $content.find('.spice-district-district-other .spice-district-country'),
	            $listAll = $content.find('.spice-district-country').not($submenu).not($notList)
	        if (!$self.parent().hasClass('spice-district-district-other')) {
	            $listAll.stop(true, true).slideUp(300, function(){
	              dialogHeaderCountryMap.headerIScroll && dialogHeaderCountryMap.headerIScroll.refresh()
	              dialogHeaderCountryMap.headerIScroll && dialogHeaderCountryMap.headerIScroll.refresh()
	            })
	            $listAll.parent().removeClass('is-open')
	            $submenu.stop(true, true).slideToggle(300, function(){
	              dialogHeaderCountryMap.headerIScroll && dialogHeaderCountryMap.headerIScroll.refresh()
	              dialogHeaderCountryMap.headerIScroll && dialogHeaderCountryMap.headerIScroll.refresh()
	            })
	            $submenu.parent().toggleClass('is-open')
	        }

	    })
	    $(window).on('resize', function() {
	        if (!!$('body').hasClass('e-spice-standard')) {
	            $('.spice-district-country').removeAttr('style')
	            $('.spice-header-dialog-common .spice-dialog-top-position-scroll').removeAttr('style')
	        }
	    })

	// }, 'header-dialog')

	// 搜索
	// require.ensure([], function(require) {

	    //pc
	    $(document).on('tap', '.e-spice-standard .e-header-search .spice-seach-box-group', function(e) {
	        var $self = $(this),
	            $input = $self.find('input'),
	            placeholderText = $.parseJSON($input.attr('spice-placeholder'))
	        $(this).closest('.e-header-search').addClass('spice-active')
	        $input.attr({
	            "placeholder": placeholderText.focus
	        })
	        $('body').on('tap.headerSearch', function(e) {
	            var $target = $(e.target)
	            if (!$target.hasClass('e-header-search') && $target.closest('.e-header-search').length == 0) {
	                $('.e-header-search').removeClass('spice-active')
	                $input.attr({
	                    "placeholder": placeholderText.original
	                })
	                $('body').off('tap.headerSearch')
	            }
	        })
	    })
	    //pad + mobile
	    $('.e-header-search > a, .e-header-search > a').on('tap', function() {
	        if( !$('body').hasClass('e-spice-medium') && !$('body').hasClass('e-spice-small') )return ;
	        var $self = $(this),
	            $parent = $self.parent(),
	            $activeShoppingCartDom = $parent.prev('.spice-active')
	        if($activeShoppingCartDom.length){
	            $activeShoppingCartDom.removeClass('spice-active');
	        }
	        console.log($parent.hasClass('spice-active'));
	        $parent.toggleClass('spice-active')
	        $parent.find('input').focus();
	    })

	// }, 'header-search')

	// 计算导航内各item的高度
	/*@param
		{
			currentDom:$submenu,
			wrapperSelector:'.spice-header-menu-container',
			leftSelector:'.header-nav-section-left',
			rightSelector : '.header-nav-section-right',
			liMaxHeight:150
		}
	*/

	function computeNavHeight(obj) {
	    var num = 0;
	    for (key in obj) {
	        num++;
	        if (!obj[key]) return;
	    }
	    var $wrapperDom = obj.currentDom.find(obj.wrapperSelector),
	        navHeight = parseInt($('.e-header-menu-back').css('height')),
	        $leftDom = $wrapperDom.find(obj.leftSelector),
	        $liDoms = $leftDom.find('>li'),
	        liLength = $liDoms.length,
	        $rightDom = $wrapperDom.find(obj.rightSelector),
	        winHeight = window.innerHeight,
	        leftHeight = winHeight - navHeight - parseInt($rightDom.css('height')),
					liHeight = leftHeight / liLength,
	        liMaxHeight = obj.liMaxHeight,
					liMinHeight = obj.liMinHeight,

	        $liImgDoms = $leftDom.find('.spice-sub-title ul>li'),
	        liImgLength = $liImgDoms.length,
	        liImgHeight = leftHeight / liImgLength

		if ($rightDom.find('.spice-carousel').length != 0) {
	    $leftDom.css('minHeight', leftHeight);

	    $liDoms.css({
	                  'minHeight': liHeight > liMaxHeight ? liMaxHeight : liHeight,
	                  padding:'0px'
	                  });
	    // $liDoms.css({
	    //              'lineHeight': $liDoms.height()+'px'
	    //              });
	    $liDoms.find('>a').css({
	          display: 'inline-block',
	          width: '100%',
	          height: $liDoms.height(),
	          lineHeight: $liDoms.height()+'px'
	    })

	  }else if($rightDom.find('.header-nav-section-right-image').length != 0){
	    // console.log(123456)
	    // console.log("leftHeight",leftHeight)
	    // console.log("rightDomHeight",parseInt($rightDom.css('height')))
	    // console.log("liImgLength",liImgLength)
	    // console.log("liImgHeight",liImgHeight)
	    $leftDom.css('minHeight', leftHeight);
	    liImgHeight = liImgHeight < liMinHeight ? liMinHeight : liImgHeight,
	    $liImgDoms.css({
	                  'minHeight': liImgHeight > liMaxHeight ? liMaxHeight : liImgHeight,
	                  padding:'0px'
	                  });
	    // console.log("liHeighttttt",$liImgDoms.height())
	    $liImgDoms.find('>a').css({
	          display: 'inline-block',
	          width: '100%',
	          height: $liImgDoms.height(),
	          lineHeight: $liImgDoms.height()+'px'
	    })
	  }else{
	    return;
	  }
	    
	}

	module.exports = {
	    // loginForm: loginForm
	    //, headerBarMenu: headerBarMenu
	}
	$('body').on('tap', ".spice-dialog .spice-icon-nav-left",function() {
	    var $self = $(this)
	    $self.closest('.spice-dialog').data('dialog').hide()

	})

	// $("body").on("click",".e-sales-login",function(e){
	$(".e-sales-login").on("tap",function(e){
	    window.headerDialog.dialogHeaderSalesLogin.show();
	    e.stopPropagation();
	  })

	$("body").on("tap",".e-copylink",function(){
	      $(this).siblings("input").select();
	       document.execCommand("Copy");
	    })

