	        var $carousel = $('.header-nav-section-right-carousel .spice-carousel'),
	            checkoutMedia = function() {
	                return $(window).width() <= 1023 ? 'mobile' : 'pc'
	            }

	        $(window).on('resize.header-submenu-scroll', function() {
	            var media = checkoutMedia(),
	                preMedia = $carousel.data('media')
	            if (media != preMedia) {

	                if (media == 'mobile') {
	                    $carousel.each(function(i, o) {
	                        var $this = $(o),
	                            carouselData = $carousel.data('carousel'),
	                            IScrollData = $this.data('IScroll'),
	                            $wrap = $this.find('.spice-carousel-wrap'),
	                            $li = $('li', $wrap)
	                        carouselData && carouselData.destroy()
	                        IScrollData && IScrollData.destroy()
	                        $wrap.width($li.length * $li.outerWidth())
	                        $this.data('IScroll', new IScroll(o, {
	                            scrollX: true,
	                            scrollY: false,
	                            preventDefaultException: {
	                                tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A|IMG|I|SPAN|DIV|UL|LI)$/
	                            }
	                        }))
	                    })

	                } else {
	                    $carousel.each(function(i, o) {
	                        var $this = $(o),
	                            carouselData = $this.data('carousel'),
	                            IScrollData = $carousel.data('IScroll'),
	                            $label = $this.closest('.header-nav-section-right').find('.e-header-nav-carousel-labels li'),
	                            $labelul = $this.closest('.header-nav-section-right').find('.e-header-nav-carousel-labels'),
	                            timer = null
	                        carouselData && carouselData.destroy()
	                        IScrollData && IScrollData.destroy()

	                        var $li = $('li', $this)

	                        if ($li.length > 1) {
	                            var carousel = new Carousel($this, {
	                                dot: true,
	                                type: 'scroll-loop',
																	autoPlay: true

	                            })
	                            $this.data('carousel', carousel)
	                            $label
	                                .off('mouseenter mouseleave')
	                                .on('mouseenter', function() {
	                                    var $this = $(this),
	                                        index

	                                    if($this.hasClass("haveimg")){
	                                      index = $labelul.find("li.haveimg").index($this);
	                                      clearTimeout(timer);
	                                      setTimeout(function() {
	                                          carousel.scrollToUnlimited(index + 1)
	                                      }, 100)
	                                    }
	                                })
	                        }
	                    })
	                }

	            }
	            $carousel.data('media', media)

	        }).trigger('resize.header-submenu-scroll')

	        var eventClass = '.e-navbar-menu [spice-data-id]',
	            $allSubMenu = $('> .spice-sub-menu', $(eventClass)),
	            addMouseenterUp = function($elem, fn) {
	                // 效果一
	                  // var $self = $elem
	                  // 	, $subMenu = $('> .spice-sub-menu', $self)
	                  // 	, oldH = +$subMenu.outerHeight(true)
	                  // 	, $div = $('>div', $subMenu)

	                  // $subMenu.removeAttr('style')
	                  // $div.show()
	                  // var newH = $subMenu.outerHeight(true)
	                  // $subMenu.css({height: oldH})
	                  // $('>div', $allSubMenu).hide()

	                  // $allSubMenu.stop(true, true).show()
	                  // $('>.spice-sub-menu', $self.nextAll('li').add( $self.prevAll('li') )).hide()
	                  // $div.stop(true, true).fadeIn('fast')
	                  // $allSubMenu.stop(true, true).animate({height: newH}, 'fast', function(){})



	                  // 效果二
	                  // $('> .spice-sub-menu', $elem.siblings()).stop(true, true)
	                  // $('> .spice-sub-menu', $elem).stop(true, true).delay(100).slideDown('fast')

	                  /*var $pills = $elem.parents('.spice-nav-pills')

	                  if( !bReady ) return;
	                  bReady = true;


	                  if( $pills.hasClass('spice-nav-is-open') ){
	                  	$elem.addClass('spice-active').siblings().removeClass('spice-active');
	                  }else{
	                  	clearTimeout(headerTimer);
	                  	headerTimer = setTimeout( function(){
	                  		$elem.addClass('spice-active').siblings().removeClass('spice-active');
	                  		$elem.off('transitionend.lala').on('transitionend.lala',function(){
	                  			$pills.addClass('spice-nav-is-open')
	                  		});
	                  	} , 300);
	                  }*/
	                newElem = $elem;
	                hoverTime = false;
	                setTimeout(function() {
	                    hoverTime = true;
	                }, 300);

	                var $pills = $elem.parents('.spice-nav-pills');
	                if ($pills.hasClass('spice-nav-is-open')) {
	                    clearTimeout(headerTimer);
	                    if ($pills.find('li.spice-nav-menu.spice-active').length) {
	                      headerTimer = setTimeout(function() {
	                        $elem.addClass('spice-active-block').siblings().removeClass('spice-active');
	                      }, 300);
	                        // $elem.addClass('spice-active-block').siblings().removeClass('spice-active');
	                    } else if ($pills.find('li.spice-nav-menu.spice-active-block').length) {
	                        if (!bReady) return;
	                        bReady = false;
	                        headerTimer = setTimeout(function() {
	                          $elem.addClass('spice-active-block').siblings().removeClass('spice-active-block');
	                        }, 300);
	                        // $elem.addClass('spice-active-block').siblings().removeClass('spice-active-block');
	                    }
	                } else {
	                    $pills.addClass('spice-nav-is-open');
	                    clearTimeout(headerTimer);
	                    headerTimer = setTimeout(function() {
	                        $elem.addClass('spice-active');
	                    }, 300);
	                }

	                /*setTimeout(function () {
	                	$pills.addClass('spice-nav-is-open')
	                }, 500);*/
	            },
	            addMouseleaveUp = function($elem, fn) {
	                // 效果一
	                  // var $self = $elem
	                  // $allSubMenu.stop(true, true)
	                  // $('>div', $('.spice-sub-menu', $self))
	                  // 	.stop(true, true)
	                  // 	.delay(300)
	                  // 	.fadeOut('fast')
	                  // $allSubMenu
	                  // 	.stop(true, true)
	                  // 	.delay(300)
	                  // 	.animate({height: 0}, 'fast', function(){
	                  // 		$allSubMenu.hide()
	                  // 	})
	                  // 效果二
	                  // $('> .spice-sub-menu', $elem).stop(true, true).slideUp('fast')


	                  // if ($self.parents('.spice-nav-pills').hasClass('spice-nav-is-open')) {
	                  // 	$self.removeClass('spice-active')
	                  // }else{
	                  // 	setTimeout(function () {
	                  // 		$self.removeClass('spice-active')
	                  // 	}, 600);
	                  // }

	                  /*				var $pills = $elem.parents('.spice-nav-pills')

	                  				$elem.removeClass('spice-active')
	                  				$elem.parents('.spice-nav-pills').removeClass('spice-nav-is-open')*/

	                  /*var $pills = $elem.parents('.spice-nav-pills')

	                  if( $pills.hasClass('spice-nav-is-open') ){
	                  	clearTimeout(headerTimer);
	                  	headerTimer = setTimeout( function(){
	                  		$elem.removeClass('spice-active')
	                  		$pills.removeClass('spice-nav-is-open')
	                  		bReady = true;
	                  	} ,1000);
	                  }else{
	                  	if( $elem.hasClass('spice-active') ){
	                  		$pills.removeClass('spice-nav-is-open')
	                  		headerTimer = setTimeout( function(){
	                  			$elem.removeClass('spice-active')
	                  			$pills.removeClass('spice-nav-is-open')
	                  			bReady = true;
	                  		} ,1000);
	                  	}else{
	                  		clearTimeout(headerTimer);
	                  	}
	                  }*/

	                var $pills = $elem.parents('.spice-nav-pills');
	                originElem = $pills.find('li.spice-nav-menu.spice-active-block');

	                if ($pills.find('li.spice-nav-menu.spice-active').length) {
	                    clearTimeout(headerTimer);
	                    headerTimer = setTimeout(function() {
	                        $pills.find('li.spice-nav-menu.spice-active').removeClass('spice-active');
	                        $pills.removeClass('spice-nav-is-open');
	                    }, 500);
	                } else if ($pills.find('li.spice-nav-menu.spice-active-block').length) {
	                    clearTimeout(bReadyTimer);
	                    bReadyTimer = setTimeout(function() {
	                        bReady = true;
	                        if (newElem != originElem) {
	                            newElem.addClass('spice-active-block').siblings().removeClass('spice-active-block');
	                        }
	                    }, 300);
	                    clearTimeout(headerTimer);
	                    headerTimer = setTimeout(function() {
	                        $pills.find('li.spice-nav-menu.spice-active-block').removeClass('spice-active-block');
	                        $pills.removeClass('spice-nav-is-open');
	                    }, 500);
	                } else {
	                    $pills.removeClass('spice-nav-is-open');
	                    clearTimeout(headerTimer);
	                }
	            }

	        // 大屏和中屏展开效果
	        if (spice.hasTouch) {
	            // 绑定 tap事件
	            $(document)
	                .on('tap', '.e-spice-standard ' + eventClass + ' > a', function(e) {
	                    var $self = $(this),
	                        $siblings = $self.siblings(),
	                        $parent = $self.parent()
	                    if ($parent.hasClass('spice-active-block')) {
	                        window.location.href = $self.attr('href')
	                        addMouseleaveUp($parent)
	                    } else {
	                        addMouseleaveUp($parent.siblings())
	                        addMouseenterUp($parent)
	                    }
	                    return false
	                })
	          // 点击其他地方收起
	          $('body').css('cursor','pointer');
	          $('body').on('tap', function(e) {
	            var _con = $('.spice-nav-pills').eq(0).children('li'); 
	            if(!_con.is(e.target) && _con.has(e.target).length === 0){ 
	                addMouseleaveUp($('.spice-nav-pills').eq(0).children('li'))
	            }

	          })
	               
	               
	        } else {
	            $(document)
	                .on('mouseenter', '.e-spice-standard ' + eventClass, function() {
	                    addMouseenterUp($(this))

	                }).on('mouseleave', '.e-spice-standard ' + eventClass, function() {
	                    addMouseleaveUp($(this))
	                })
	        }
	       

	        // 小屏幕展开效果
	        $(document)
	            .on('tap', '.e-spice-small ' + eventClass + ' a, .e-spice-medium ' + eventClass + ' a', function(e) {
	                var $self = $(this),
	                    $subMenu = $self.siblings('.spice-sub-menu'),
	                    $closestSubMenu = $self.closest('.spice-sub-menu'),
	                    $IScroll = $('.spice-carousel', $subMenu),
	                    $li = $self.closest('li'),
	                    left = 0


	                if ($subMenu.length != 0) {
	                    if ($self.parent().hasClass('spice-nav-menu')) {
	                        if ($closestSubMenu.length == 0) {
	                            $closestSubMenu = $self.closest('.spice-navbar-scroller-container')
	                            left = '100%'
	                        }

	                        $closestSubMenu
	                            .css({
	                                left: left
	                            })

	                        $subMenu
	                            .show()
	                            .css({
	                                left: '-100%'
	                            })
	                            .find('.spice-header-menu-standard-slot')
	                            .addClass('header-sub-menu-fixed')

	                        $curMenuSite = $subMenu
	                        navbarMenuIScroll && navbarMenuIScroll.destroy();
	                        $self.closest('.spice-navbar-scroller-container')
	                            .height(getMenuHeight())
	                        $self.closest('.spice-navbar-scroller').removeAttr('style')
	                        var IScrollData = $IScroll.data('IScroll')
	                        IScrollData && IScrollData.refresh()
	                        $IScroll.velocity({
	                            left: 0
	                        }, {
	                            duration: 300,
	                            easing: 'easing'
	                        })
	                        computeNavHeight({
	                            currentDom: $li,
	                            wrapperSelector: '.spice-header-menu-container',
	                            leftSelector: '.header-nav-section-left',
	                            rightSelector: '.header-nav-section-right',
								              liMaxHeight:80,
	                            liMinHeight:38
	                        })

	                        $('.spice-navbar-menu-return', $navbarMenu).removeClass('spice-block-sm')

	                        $('.spice-header-menu-container-scroll', $subMenu).height( window.innerHeight - $('.spice-header-menu-back', $subMenu).height() )
	                        var $navCarouselDom = $('.e-header-nav-carousel','.spice-header-menu-container-scroll');
	                        // if( $navCarouselDom.length>0 && spice.android ){
	                        //     $navCarouselDom.css('marginBottom',Math.min(parseInt($navCarouselDom.css('height')) , 0 ));
	                        // }
	                        subMenuIScroll && subMenuIScroll.destroy();
	                        subMenuIScroll = new IScroll($('.spice-header-menu-container-scroll', $subMenu)[0], {
	                            preventDefaultException: {
	                                tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A|IMG|I|SPAN|DIV|UL|LI)$/
	                            }
	                        })

	                        return false
	                    } else if( $self.parent().hasClass('spice-menu-list') ) {
	                        $li.toggleClass('is-open').siblings('li').removeClass('is-open')
	                        $closestSubMenu.find('.header-nav-section-right-menuGroup-slide .header-nav-section-right-menuGroup').removeClass('is-open')
	                        $subMenu.slideToggle(300,function() {
	                            $self.closest('.spice-navbar-scroller-container').height(getMenuHeight())
	                            subMenuIScroll && subMenuIScroll.refresh()

	                            var IScrollData = $IScroll.data('IScroll')
	                            IScrollData && IScrollData.refresh()
	                        });
	                        $('.spice-sub-menu', $closestSubMenu).not($subMenu).slideUp(300,function() {
	                            subMenuIScroll && subMenuIScroll.refresh()
	                        });

	                        return false
	                    }

	                } else if ($self.parent().hasClass('e-header-menu-back')) {
	      
	                    if ( !$self.hasClass('backWords-link') ) {

	                      var $closestParentSubMenu = $closestSubMenu.closest('.spice-navbar-scroller-container')
	                      left = 0

	                      $closestParentSubMenu.css({
	                          left: left
	                      })
	                      $subMenu.show().css({
	                          left: '-100%',
	                          top: 0,
	                          height: '100%'
	                      })
	                      $curMenuSite = $closestParentSubMenu
	                      $self.closest('.spice-navbar-scroller-container').height(getMenuHeight())


	                      $closestSubMenu.css({
	                          left: '100%'
	                      })
	                      $closestSubMenu
	                        .hide()
	                        .find('.spice-header-menu-standard-slot')
	                        .removeClass('header-sub-menu-fixed')

	                      subMenuIScroll && subMenuIScroll.destroy();
	                      $('.spice-header-menu-container-scroll', $closestSubMenu).removeAttr('style')
	                      $('.spice-header-menu-container', $closestSubMenu).removeAttr('style')

	                      //$('.spice-sub-menu', $closestSubMenu).removeAttr('style')
	                      // navbarMenuIScroll && navbarMenuIScroll.refresh()

	                      $('.spice-header-menu-standard-slot .spice-menu-list').removeClass('is-open').find('.spice-sub-menu').removeAttr('style')
	                      $allSubMenu.find('li').removeClass('is-open').find('.spice-sub-menu').removeAttr('style')
	                      $carousel.removeAttr('style')
	                      $('.e-navbar-btn-toggle').trigger('tap')

	                      mask.hide()

	                      return false
	                    }
	                }


	            })
	        // $(document)
	        //     .on('tap', '.style-spice-standard [spice-href]', function(e) {
	        //         var $self = $(this)
	        //             , href = $self.attr('spice-href')
	        //         window.location.href = href
	        //     })
	        var headerMediaHref = null
	        $(window).on('resize.header-spice-href', function() {
	            var mediaNow = checkoutHeaderMedia()
	            if ( headerMediaHref != mediaNow ) {
	                var $nav = $('[spice-href]', $navbarMenu)
	                $nav.each(function(i,j){
	                    var $this = $(j)
	                        , href = $this.attr('spice-href')
	                    if ( mediaNow == 'pc' ) {
	                        $this
	                          .off('tap.spice-href')
	                          .attr('href', href)
	                    } else{
	                        $this
	                          .removeAttr('href')
	                    }
	                })
	                headerMediaHref = mediaNow
	            }
	        }).trigger('resize.header-spice-href')

	        // $(document)
	        //     .on('tap', '.e-spice-small .header-nav-section-right-menuGroup-slide .header-nav-section-right-menuGroup>a, .e-spice-medium .header-nav-section-right-menuGroup-slide .header-nav-section-right-menuGroup>a', function(e) {
	        //         var $self = $(this),
	        //             $subMenu = $self.siblings('.spice-sub-menu'),
	        //             $closestSubMenu = $self.closest('.spice-sub-menu'),
	        //             $li = $self.closest('.header-nav-section-right-menuGroup')

	        //         if ( $subMenu.find('li').length != 0 ) {
	        //             $li.toggleClass('is-open')
	        //             $closestSubMenu.find('.spice-nav>.spice-menu-list').removeClass('is-open')
	        //             $subMenu.slideToggle(300,function() {
	        //                 $self.closest('.spice-navbar-scroller-container').height(getMenuHeight())
	        //                 subMenuIScroll && subMenuIScroll.refresh()
	        //             });
	        //             $('.spice-sub-menu', $closestSubMenu).not($subMenu).slideUp(300,function() {
	        //                 subMenuIScroll && subMenuIScroll.refresh()
	        //             });

	        //             return false
	        //         }
	        //     })
	        $('.header-nav-section-right-menuGroup-slide .header-nav-section-right-menuGroup>a')
	            .add($('.header-nav-section-right-menuGroup-slide .header-nav-section-right-menuGroup>a'))
	            .on('tap', function(e) {
	                if ( $('body').hasClass('e-spice-small') || $('body').hasClass('e-spice-medium') ) {
	                    var $self = $(this),
	                        $subMenu = $self.siblings('.spice-sub-menu'),
	                        $closestSubMenu = $self.closest('.spice-sub-menu'),
	                        $li = $self.closest('.header-nav-section-right-menuGroup')

	                    if ( $subMenu.find('li').length != 0 ) {
	                        $li.toggleClass('is-open')
	                        $closestSubMenu.find('.spice-nav>.spice-menu-list').removeClass('is-open')
	                        $subMenu.slideToggle(300,function() {
	                            $self.closest('.spice-navbar-scroller-container').height(getMenuHeight())
	                            subMenuIScroll && subMenuIScroll.refresh()
	                        });
	                        $('.spice-sub-menu', $closestSubMenu).not($subMenu).slideUp(300,function() {
	                            subMenuIScroll && subMenuIScroll.refresh()
	                        });

	                        return false
	                    }
	                }
	            })
	        $('.e-header-menu-back .spice-icon-nav-left').on('tap', function() {
	            var $self = $(this),
	                $parent = $self.parent(),
	                $closestSubMenu = $parent.closest('.spice-sub-menu'),
	                $closestParentSubMenu = $closestSubMenu.closest('.spice-navbar-scroller-container')
	            left = 0

	            $closestParentSubMenu.css({
	                left: left
	            })
	            $curMenuSite = $closestParentSubMenu
	            $self.closest('.spice-navbar-scroller-container').height(getMenuHeight())
	            navbarMenuIScroll && navbarMenuIScroll.refresh()

	            $closestSubMenu
	                .css({
	                    left: '100%'
	                })
	                .hide()
	                .find('.spice-header-menu-standard-slot')
	                .removeClass('header-sub-menu-fixed')

	            subMenuIScroll && subMenuIScroll.destroy();
	            $('.spice-header-menu-container-scroll', $closestSubMenu).removeAttr('style')
	            $('.spice-header-menu-container', $closestSubMenu).removeAttr('style')

	            $allSubMenu.find('li').removeClass('is-open').find('.spice-sub-menu').removeAttr('style')
	            $carousel.removeAttr('style')

	            $('.spice-navbar-menu-return', $navbarMenu).addClass('spice-block-sm')

	            navbarMenuIScroll && navbarMenuIScroll.destroy()
	            navbarMenuIScroll = new IScroll($('.spice-navbar-menu-container', $navbarMenu)[0], {
	                preventDefaultException: {
	                    tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A|IMG|I|SPAN|DIV|UL|LI)$/
	                }
	            })

	        })

	        
	      }

