	    /*弹层相关事件*/
	    $('.spice-checkbox', dialogHeaderRegister.elem).off('tap.new-member').on('tap.new-member',function(e){
	      var $checkbox = $(this)
	        ,$thisFather = $checkbox.parents(".spice-form-group")

	        if($(e.target).is('a')){e.stopPropagation()}else{
	            if( $checkbox.hasClass('spice-active') ){
	                $checkbox.removeClass('spice-active');
	                if(!$thisFather.next('.email-place').hasClass('spice-none')){
	                    $thisFather.next('.email-place').addClass('spice-none')
	                }
	            }else{
	                $checkbox.addClass('spice-active');
	                if($thisFather.next('.email-place').hasClass('spice-none')){
	                    $thisFather.next('.email-place').removeClass('spice-none')
	                }
	            }
	            return false;
	        }
	      
	    });

	    dialogHeaderResetPassword.elem.on('spice.dialog-show-before', function(e, o){
