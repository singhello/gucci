	    // 顶部导航下拉
	    var HeaderMenu = __webpack_require__(53),
	        Carousel = __webpack_require__(64),
	        IScroll = __webpack_require__(30),
	        headSourceReq = __webpack_require__(66),
	        headerBarMenu = new HeaderMenu($navbarMenu, {
	            sourceReq: headSourceReq
	            // url: '../../data/language/' + S.language + '/header-submenu.json'
