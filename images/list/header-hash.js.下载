	        timerLogin = {},
	        timerLoginAfter = {},
	        timerWish = {},
	        timerShoppingcart = {},
	        speed = 300,
	        addMouseenter = function($elem, timerObj) {
	            var $self = $elem
	            timerObj.mouseEnterTimer = setTimeout(function() {
	                $self.addClass('spice-active')
	            }, speed)
	            timerObj.mouseLeaveTimer && clearTimeout(timerObj.mouseLeaveTimer)
	        },
	        addMouseleave = function($elem, timerObj) {
	            var $self = $elem
	            timerObj.mouseLeaveTimer = setTimeout(function() {
	                $self.removeClass('spice-active')
	                /*if( $self.hasClass('spice-nav-wish-list') ){
	                	if( $self.find('.wish-savedItems-num').html().slice(1,-1) > 0 ){
	                		$self.find('.wish-savedItems-num').show();
	                	}else{
	                		$self.find('.wish-savedItems-num').hide();
	                	}
	                }*/
	            }, speed)
	            timerObj.mouseEnterTimer && clearTimeout(timerObj.mouseEnterTimer)
	        }
	    // 心愿单提取
	    function wishListAction(elem){
	      var $wishLi = elem.parents('.spice-nav-wish-list');
	  		if( elem.find('.spice-mini-list-tinyscrollbar li').length ){
	  			// $wishLi.find('.wish-savedItems-num').show();
	  			$wishLi.find('.wish-savedItems-num').html('('+elem.find('.spice-mini-list-tinyscrollbar li').length+')');
	  		}else{
	  			// $wishLi.find('.wish-savedItems-num').hide();
	  		}
	    }
	    wishListAction($headerWishListContent);

	    window.setMinCartClass = function(){
	        var $minShoppingCart = $('.spice-header-shoppingcart')
	            , numS = $('.shopping-bag-product-count', $minShoppingCart).text()
	            , $minWishList = $('.spice-header .spice-nav-other-login')
	            // , numW = $('.wish-savedItems-num', $minWishList).text().slice(1,-1)

	        if ( numS > 0 ) {
	          $minShoppingCart.addClass('spice-header-mincart-length-1')
	        }
	        $minWishList.addClass('spice-header-mincart-length-' + numS.length)
	    }
	    setMinCartClass()
	    // 登录模块提取
	    headerLoginAction($heaerLoginParent);
	    function headerLoginAction(elem){
	      $('.spice-submenu-btn-login', elem).on('tap', function(){
	  			elem.trigger('spice.submenu-btn-login')
	  		})
	  		$('.spice-submenu-btn-register', elem).on('tap', function(){
	  			elem.trigger('spice.submenu-btn-register')
	  		})
	    }

