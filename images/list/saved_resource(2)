/* 146 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(147)
		, slot = __webpack_require__(144)
		, spice = __webpack_require__(2)

	__webpack_require__(148);

	function SlotStandard(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	SlotStandard.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function(elem){
			this.drawEvents(elem);
		}
		, drawEvents: function( elem ){
			var self = this
				, $elem = $(elem)
				, $window = $(window)
				, config = self.config
				, data = self.data

			// 下划线
			var $subTitle = $('.spice-hero-subtitle', $elem)
				, $copy = $('.spice-hero-copy1', $elem).add( $('.spice-hero-copy2', $elem) )
			if ( $subTitle.length > 0 && $copy.length > 0 ) {
				$subTitle.after('<span class="spice-hero-title-underline"></span>')
			}
			var dialogFn = __webpack_require__(149)
				, dialogSourceReq = __webpack_require__(156)
			if ( data.shareOverlay ) {
				//获取弹层模板和方法
				var shareOverlay = new dialogFn( $('.e-spice-flex-standard-share', elem), {
						sourceReq: dialogSourceReq
				    })
				data.shareOverlay.slot = 'slot-extend-hero-share-overlay'
				shareOverlay.drawModule( data.shareOverlay )
				$('.e-spice-dialog-close', shareOverlay.dialog.elem).on('tap', function(){
					shareOverlay.dialog.hide()
				})

				var $weixinBtn = $('.e-hero-share-weixin', shareOverlay.dialog.elem)
				if ( $weixinBtn.length > 0 ) {
					$weixinBtn.data({
						'dialog': shareOverlay
					})
				}
			}

			if ( data.qrCodeOverlay ) {
				//获取弹层模板和方法

		        var qrShareOverlay = new dialogFn( $('.e-code-dialog', elem), {
						sourceReq: dialogSourceReq
				    })
				data.qrCodeOverlay.slot = 'slot-extend-hero-qrshare-overlay'
				qrShareOverlay.drawModule( data.qrCodeOverlay )

				$('.e-spice-dialog-close', qrShareOverlay.dialog.elem).on('tap', function(){
					qrShareOverlay.dialog.hide()
				})
			}
			var timeUpdate = function(){
				$('.e-end-time').each(function(index, el) {
					var $that = $(this),
						endTime = $that.data('end-time'),
						timeDetail = {};
					if(endTime < 0){ $(".spice-flex-standard-slot-content").removeClass("countdowm-own");return;}
					timeDetail = {
						'day': parseInt(endTime / 60 / 60 / 24),
						'hour': parseInt(endTime / 60 / 60 % 24),
						'minute': parseInt(endTime / 60 % 60),
						'second': parseInt(endTime % 60)
					};
					$.each(timeDetail, function(name, value) {
						if(name !== 'day'){
							$('.time-' + name, $that).text(value > 9 ? value : '0' + value);
							if(name !== 'second'){
								$('.time-' + name, $that).text(value > 9 ? value +':': '0' + value+":");
							}
							
						}else{
							if(value){
								$('.time-' + name, $that).text(value + 'Day' + (value > 1 ? 's':'')+":");
							}else{
								$('.time-' + name, $that).remove();
							}
						}

						
					});
					$that.data('end-time', --endTime);
				});
			};
			
			var countdownTime = data.countdownTime;
			if( countdownTime ){
				S.asyncXhrGet("/countdownSecond",{"countdownTime":countdownTime}, function(data, textStatus, errorThrown) {
					if (textStatus == "success") {
						var endTime = data;
						// var endTime = 100;
						if(endTime>0){
							$(".spice-flex-standard-slot-content").addClass("countdowm-own")
						}
						$('.e-end-time').data('end-time', endTime);
						timeUpdate();
						setInterval(timeUpdate, 1000);
					}
				})			
			}

			// if( countdownTime ){
			// 	var endTime = 1000;
			// 	if(endTime>0){
			// 		$(".spice-flex-standard-slot-content").addClass("countdowm-own")
			// 	}
			// 	$('.e-end-time').data('end-time', endTime);
			// 	timeUpdate();
			// 	setInterval(timeUpdate, 1000);
			// }

			if ( data.heroHelpDialog ) {
				var oHelp = {}
				switch( data.heroHelpDialog ){
					case '0':
					  oHelp = window.multiLanguage.common['common-hero-dialog-help-0']
					  break;
					case '1':
					  oHelp = window.multiLanguage.common['common-hero-dialog-help-1']
					  break;
				}
				var helpOverlay = new dialogFn( $('.e-hero-help', elem), {
						sourceReq: dialogSourceReq
						, dialogConfig:{
							dialogClass: 'free-distribution-dialog'
						}
				    })
				helpOverlay.drawModule( oHelp )
				$('.e-spice-dialog-close', helpOverlay.dialog.elem).on('tap', function(){
					helpOverlay.dialog.hide()
				})
			}

			var $filterDami = $('>.e-flex-filter', elem)
			if ( $filterDami.length > 0 ) {
				$('.spice-flex-standard-slot-label', elem).append( $filterDami )
				$('>.e-flex-filter', elem).remove();
			}

			var $filter = $('.e-flex-filter', elem)
			if ( $filter.length > 0 ) {
				$('.spice-flex-filter-menu>a', $filter).on('tap', function(){
					$(this).closest('.spice-flex-filter-menu').toggleClass('is-open')
				})
			}


			// 视频js
			var $videoContainer = $('.spice-hero-standard-slot', $elem)
				, $closeBtn = $('.e-video-close', $elem)
				, $volumeBtn = $('.e-video-volume', $elem)
				, $loopVideoContent = $('.homepage-video-content-loop', $elem)
				, $videoContent = $('.homepage-video-content', $elem)
			
			// 添加音量键class
			if ( $('.spice-flex-standard-slot-promotion', $elem).length > 0 && $('.spice-flex-standard-slot-volume', $elem).length > 0 ) {
				$('.spice-flex-standard-slot-label', $elem).addClass('spice-flex-standard-slot-label-double')
			}

			if ( $videoContent.length > 0 ) {
				$('.spice-flex-standard-slot-content', $elem).append('<a class="spice-hero-btn-play e-homepage-video"></a>')
			}
			var $videoBtn = $('.e-homepage-video', $elem)

			// flash视频js 
			if ( data.hasFlash ) {
				var videoTimer = null
					, setVideoEvents = function(){
						var $div = $('.homepage-video-content>div', $elem)
							, $object =  $('.homepage-video-content object', $elem)
						if($object.length > 0 ){
							var player = getSWF( $object.attr('id') )
							$videoBtn.on('tap', function(){
								$videoContainer.removeClass('is-loop').addClass('is-play')
								$loopVideoContent.remove()
								player.start()
								player.setVolume(0.2)
							})
							$closeBtn.on('tap', function(){
								$videoContainer.removeClass('is-play')
								player.pause()
							})
						} else{
							clearTimeout(videoTimer)
							videoTimer = setTimeout(setVideoEvents, 400)
						}
					}
					, loopVideoTimer = null
					, setLoopVideoEvents = function(){
						var $div = $('.homepage-video-content-loop>div', $elem)
							, $object =  $('.homepage-video-content-loop object', $elem)
						if( $object.length > 0 ){
							var player = getSWF( $object.attr('id') )
							if ( !$videoContainer.hasClass('is-loop') && !!player && !!player.start ) {
								var eW = $elem.width()
									, eH = $elem.height()
									, loopHeight = Math.ceil(eW * parseInt(data.loopVideoData.height) / parseInt(data.loopVideoData.width) ) / eH * 100
								$loopVideoContent.css({
									'height': loopHeight + '%'
									, 'top': '-' + (loopHeight-100)/2 + '%'
								})
								setTimeout(function(){
									player.start()
									player.setVolume(0)

									$volumeBtn.on('tap', function(){
										var $this = $(this)
										if ( $this.hasClass('volume_on') ) {
											$this.removeClass('volume_on')
											player.setVolume(0)
										} else{
											$this.addClass('volume_on')
											player.setVolume(0.5)
										}
									})
								}, 1000)
								
							} else{
								clearTimeout(loopVideoTimer)
								loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
							}
			
						} else{
							clearTimeout(loopVideoTimer)
							loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
						}
					}
				if ( $videoContent.length > 0 ) {
					videoTimer = setTimeout(setVideoEvents, 400)
				}


				if ( $loopVideoContent.length > 0 ) {
					if ( spice.hasTouch ) {
						$loopVideoContent.remove()
					} else{
						loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
					}
				}

				$videoContent.data('setting', {
					config: {
						rightmenu_enable: 0
						, tip_enable: 0
						, recommend_enable: 0
						, rightmenu_enable: 0
						, bigbutton_enable: 0
					}
				})
				$loopVideoContent.data('setting', {
					config: {
						control_enable: 0
						, loading_enable: 0
						, alert_enable: 0
						, bigbutton_enable: 0
						, rightmenu_enable: 0
						, tip_enable: 0
						, recommend_enable: 0
						, rightmenu_enable: 0
					}
					, startFn: function(){
						$videoContainer.addClass('is-loop')
						return;
					}
					, stopFn: function(){
						var $div = $('>div', $loopVideoContent)
							, $object =  $('object', $loopVideoContent)
							, player = getSWF( $object.attr('id') )
							, count = $loopVideoContent.data('count')
							, maxCount = $loopVideoContent.attr('spice-data-max-count')
						if ( maxCount ) {
							count = count ? count : 0;
							if ( count >= maxCount ) {
								$videoContainer.removeClass('is-loop')
								return
							} else{
								$loopVideoContent.data('count', count + 1)
								setTimeout(function(){
									player.start()
								}, 0)
							}
						} else{
							setTimeout(function(){
								player.start()
							}, 0)
						}
						return;
					}
				})

				function getSWF(objectId) { 
					if (window.document[ objectId ]) {
					    return window.document[ objectId ];
					} else if (navigator.appName.indexOf("Microsoft") == -1) {
					    if (document.embeds && document.embeds[ objectId ]) {
					    	return document.embeds[ objectId ];
					    }
					} else {
						return document.getElementById( objectId );
					}
				}
				// flash视频js end
			} else{
				var isMob = false
				if ( $(window).width() <= 767 ) {
					isMob = true
				}

				var $divLoop = $('.homepage-video-content-loop>div', $elem)
					, $video =  $('.homepage-video-content video', $elem)
					, video = $video[0]
					, $videoLoop =  $('.homepage-video-content-loop video', $elem)
					, videoLoop = $videoLoop[0]

				if (videoLoop) {
					videoLoop.volume = 0;

					videoLoop.onplay = function(){
						$videoContainer.addClass('is-loop')
					}
					videoLoop.onended = function(){
						var count = $loopVideoContent.data('count')
							, maxCount = $loopVideoContent.attr('spice-data-max-count')
						if ( maxCount ) {
							count = count ? count : 0;
							if ( count >= maxCount ) {
								$videoContainer.removeClass('is-loop')
								return
							} else{
								$loopVideoContent.data('count', count + 1)
								videoLoop.play()
							}
						} else{
							videoLoop.play()
						}
						return;
					}
					videoLoop.oncanplay = function(){
						var eW = $elem.width()
							, eH = $elem.height()
							, loopHeight = Math.ceil(eW * parseInt(data.loopVideoData.height) / parseInt(data.loopVideoData.width) ) / eH * 100
						$loopVideoContent.css({
							'height': loopHeight + '%'
							, 'top': '-' + (loopHeight-100)/2 + '%'
						})
						videoLoop.play()
					}
					
					$volumeBtn.on('tap', function(){
						var $this = $(this)
						if ( $this.hasClass('volume_on') ) {
							$this.removeClass('volume_on')
							videoLoop.volume = 0;
						} else{
							$this.addClass('volume_on')
							videoLoop.volume = 0.5;
						}
					})
				}

				if (video) {
					video.controls = true;
					if ( isMob ) {
						video.controls = false
						S.setFullscreenEvents( video, function(e) {
							if ( $videoContainer.hasClass('is-play') ) {
								$videoContainer.removeClass('is-play')
								setTimeout(function(){
									video.pause()
								},0)
							}else{
								$videoContainer.removeClass('is-loop').addClass('is-play')
								$('.homepage-video-content-loop', $elem).remove()
								setTimeout(function(){
									video.play()
								},0)
							}
						})
					} else{
						video.controls = true;
						video.onplay = function(){
							$videoContainer.removeClass('is-loop').addClass('is-play')
							$('.homepage-video-content-loop', $elem).remove()
						}
						video.onended = function(){
							$videoContainer.removeClass('is-play')
						}
					}
					
					$videoBtn.on('tap', function(){
						video.play()
						isMob && S.launchFullscreen( video )
					})
					$closeBtn.on('tap',function(){
						video.pause()
						$videoContainer.removeClass('is-play')
					})
					if (spice.iPhone) {
						video.onpause = function(){
							$videoContainer.removeClass('is-play')
						}
					}
				}
			}

		}
	});

	SlotStandard.prototype.constructor = SlotStandard;

	module.exports = SlotStandard;

