	SlotFn.prototype = $.extend(true, {}, new slot(), {
		config : {}
		, init: function(elem){
			this.drawEvents(elem);
		}
		, drawEvents: function( elem ){
	        if( $('[spice-data-image-src]', elem).length > 0 ){
				new autoReplaceImageSrc( '[spice-data-image-src]' )
			}

			var checkoutMedia = function(){
				return $(window).width() <= 767 ? 'mobile' : 'pc'
			}

			var $carousel = $('.spice-carousel-wrapper', elem)
				, $warp = $('.spice-carousel-wrap', $carousel)
				, $ul = $('ul', $warp)
				, $li = $('li', $ul)
				, $dots = $('.spice-carousel-dots>a', $carousel)
				, $dotsGroup = $('.spice-carousel-dots', $carousel)
				, IScrollData

			// $li.length == 2 && $('.spice-giant-look-slot', elem).addClass('vignette-two')

			$(window).on('resize.vignette-scroll', function(){
				var media = checkoutMedia()
					, preMedia = $carousel.data('media')
				if( media != preMedia ){
				
					if ( $li.length > 2 && media == 'mobile' ) {
						$carousel.addClass('vignette-scroll')

						IScrollData = new IScroll( $warp[0] , {
							scrollX: true
							, scrollY: true
							, preventDefault: false
							, snap: 'li'
							, preventDefaultException: {
								tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A|IMG|I|SPAN|DIV|LI|P)$/
							}
						})
						

						IScrollData.on('scrollEnd', function(e,o){
							if ( this.x == 0 ) {
								$dots.removeClass('spice-active').eq(0).addClass('spice-active')
							} else{
								$dots.removeClass('spice-active').eq(1).addClass('spice-active')
							}
						})

						$dotsGroup.addClass('dots_active')
						$dots.on('tap', function(){
							var $this = $(this)
								, index = $dots.index( $this )

							$dots.removeClass('spice-active')
							$this.addClass('spice-active')
							IScrollData.scrollToElement( IScrollData.options.snap[index*2], 600 );
						})

					} else{
						$carousel.removeClass('vignette-scroll')
						$dotsGroup.removeClass('dots_active')
						IScrollData && IScrollData.destroy()
						$ul.removeAttr('style')
					}

					if ( $('li', $carousel).length == 1 && media == 'mobile' ) {
						$carousel.addClass('vignette-single')
					}

					// if ( media == 'mobile' ) {
					// 	$('.e-vignette-overlay', elem).off('tap')
					// }

					if ( media == 'mobile' ) {
						var maxHeight = 0
							, $copy = $( 'p.copy' , $li)
						$copy.each(function(i,j){
							var $this = $(j)
								, h = $this.height()
							if ( h > maxHeight ) {
								maxHeight = h
							}
						})
						$copy.css({'height': maxHeight})
					} else {
						$( 'p.copy' , $li).removeAttr('style')
					}
				}

				$carousel.data('media', media)

			})
		}
	});

	SlotFn.prototype.constructor = SlotFn;

	module.exports = SlotFn;


