/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(328)
		, slot = __webpack_require__(171)
		, tinyscrollbar = __webpack_require__(46)
		, dropdown = __webpack_require__(36);

	__webpack_require__(329);

	function Simulation(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	Simulation.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function( elem ){
			this.drawModule( elem );
			this.drawEvents( elem );
		}
		, drawModule: function( elem ){
			var liHtml = ['<ul>']
				, selectValue = this.config.selectValue;
			$.each(this.config.content, function(i, v){
				liHtml.push('<li spice-data-value="' + v.value + '"' + ( (selectValue ? v.value == selectValue : i == 0) ? ' class="spice-current"' : '' ) + '><a><img/>' + v.words + '</a></li>');
			});
			liHtml.push('</ul>');

			new tinyscrollbar($('.spice-dropdown-menu:last', elem), {
				template: liHtml.join('')
			});

		}
		, drawEvents: function( elem ){
			var dropdownObj = new dropdown($('.spice-dropdown:last', elem), this.data.dropdownConfig);

			dropdownObj.elem.on('spice.dropdown-menu-tap', function(e, o){
				
				var $self = o.self;
				$($self.opt.dropElem, $self.elem).find('.spice-dropdown-text').html( $.trim(o.curMenu.text()) );
				setTimeout(function(){
					dropdownObj.elem.trigger('spice.dropdown-change', dropdownObj.elem)
				}, 0)

			}).trigger('spice.dropdown-menu-tap', {curMenu:$('.spice-current', dropdownObj.elem), self:dropdownObj.elem.data('spice.dropdown')});

			dropdownObj.elem.on('spice.dropdown-show', function(e, o){
				$(window).trigger( 'resize.Tinyscrollbar' );
			});
		}
	});

	Simulation.prototype.constructor = Simulation;

	module.exports = Simulation;


