/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
	    , slotFn = __webpack_require__(455)
	    , spiceModules = __webpack_require__(62)
	    , template = __webpack_require__(460)

	__webpack_require__(461);

	var CategoryGrid = function(element, config){
		this.element = $(element);
	    this.config = this.extend(config);
	    this.init();
	}

	CategoryGrid.prototype = $.extend(true, {}, new spiceModules(), {
		config: {
			CategoryGridConfig: {

			}
		}
		, drawModule: function( json ){
			var self = this
				, $elem = $(self.element)
				, config = self.config
				, data = json || config.defaultDataObj || self.cacheData
				, slot = self.config.slot
				, lazyImg = config.lazyImg || ""

			if( !data || !data.content ) return false

			$('.spice-cms-lazy-img', $elem).remove()
		
			var oData = data.content
			$elem.html('').append( template(oData) )

			if ( oData.grids ) {
