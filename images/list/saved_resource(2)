/* 396 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(397)
		, slot = __webpack_require__(394)
		, spice = __webpack_require__(2)

	__webpack_require__(398);

	function Simulation(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	Simulation.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function( elem ){
			this.drawEvents(elem)
		}
		, drawModule: function( elem ){}
		, drawEvents: function( elem ){
			var $self = $(elem)
				, $window = $(window)
				, $videoContainer = $('.spice-brand-panel-slot', $self)
				, $videoBtn = $('.e-homepage-video', $self)
				, $closeBtn = $('.e-video-close', $self)
				, $loopVideoContent = $('.homepage-video-content-loop', $self)
				, $videoContent = $('.homepage-video-content', $self)
				, data = this.data

			if ( data.hasFlash ) {
				var videoTimer = null
					, setVideoEvents = function(){
						var $div = $('.homepage-video-content>div', $self)
							, $object =  $('.homepage-video-content object', $self)
						if($object.length > 0 ){
							var player = getSWF( $object.attr('id') )
							$videoBtn.on('tap', function(){
								$videoContainer.removeClass('is-loop').addClass('is-play')
								$loopVideoContent.remove()
								player.start()
								player.setVolume(0.2)
							})
							$closeBtn.on('tap',function(){
								$videoContainer.removeClass('is-play')
								player.pause()
							})
						} else{
							clearTimeout(videoTimer)
							videoTimer = setTimeout(setVideoEvents, 400)
						}
					}
					, loopVideoTimer = null
					, setLoopVideoEvents = function(){
						var $div = $('.homepage-video-content-loop>div', $self)
							, $object =  $('.homepage-video-content-loop object', $self)
						if( $object.length > 0 ){
							var player = getSWF( $object.attr('id') )
								, flag = $loopVideoContent.data('flag')
							$window.on('scroll.brandPanel',function(){
								spice.debounce(function(){
									var $cover = $self
										, lock = false
										, scrollStart = $cover.offset().top - $window.height()
						    			, scrollEnd = scrollStart + $cover.innerHeight() + $window.height()
						    			, scrollNow = $window.scrollTop()
						    			, scrollH = scrollEnd - scrollStart

						    		if ( scrollNow > scrollStart && scrollNow <= scrollEnd ) {
						    			if ( player.start && !$videoContainer.hasClass('is-loop') ) {
						    				if (!flag) {
						    					var eW = $videoContainer.width()
													, eH = $videoContainer.height()
													, loopHeight = Math.ceil(eW * parseInt(data.loopVideoData.height) / parseInt(data.loopVideoData.width) ) / eH * 100
												$loopVideoContent.css({
													'height': loopHeight + '%'
													, 'top': '-' + (loopHeight-100)/2 + '%'
												})
												setTimeout(function(){
													player.start()
													player.setVolume(0)
													flag = false;
												}, 0)
											}
										}
						    		}

								},400,"immediate")();
							});
						} else{
							clearTimeout(loopVideoTimer)
							loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
						}
					}
				if ( $videoContent.length > 0 ) {
					videoTimer = setTimeout(setVideoEvents, 400)
				}

				if ( $loopVideoContent.length > 0 ) {
					if ( spice.hasTouch ) {
						$loopVideoContent.remove()
					} else{
						loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
					}
				}

				$('.homepage-video-content', $self).data('setting', {
					config: {
						rightmenu_enable: 0
						, tip_enable: 0
						, recommend_enable: 0
						, rightmenu_enable: 0
						, bigbutton_enable: 0
					}
				})
				$loopVideoContent.data('setting', {
					config: {
						control_enable: 0
						, loading_enable: 0
						, alert_enable: 0
						, bigbutton_enable: 0
						, rightmenu_enable: 0
						, tip_enable: 0
						, recommend_enable: 0
						, rightmenu_enable: 0
					}
					, startFn: function(){
						$videoContainer.addClass('is-loop')
						$loopVideoContent.data('flag', true)
						return;
					}
					, stopFn: function(){
						var $div = $('>div', $loopVideoContent)
							, $object =  $('object', $loopVideoContent)
							, player = getSWF( $object.attr('id') )
							, count = $loopVideoContent.data('count')
							, maxCount = $loopVideoContent.attr('spice-data-max-count')
						if ( maxCount ) {
							count = count ? count : 0;
							if ( count >= maxCount ) {
								$videoContainer.removeClass('is-loop')
								return
							} else{
								$loopVideoContent.data('count', count + 1)
								player.start()
							}
						} else{
							setTimeout(function(){
								player.start()
							},0)
						}
						return;
					}
				})

				function getSWF(objectId) { 
					if (window.document[ objectId ]) {
					    return window.document[ objectId ];
					} else if (navigator.appName.indexOf("Microsoft") == -1) {
					    if (document.embeds && document.embeds[ objectId ]) {
					    	return document.embeds[ objectId ];
					    }
					} else {
						return document.getElementById( objectId );
					}
				}

			}else{
				var $divLoop = $('.homepage-video-content-loop>div', $self)
					, $video =  $('.homepage-video-content video', $self)
					, video = $video[0]
					, $videoLoop =  $('.homepage-video-content-loop video', $self)
					, videoLoop = $videoLoop[0]

				if ( videoLoop ) {
					videoLoop.onplay = function(){
						$videoContainer.addClass('is-loop')
					}
					videoLoop.onended = function(){
						var count = $loopVideoContent.data('count')
							, maxCount = $loopVideoContent.attr('spice-data-max-count')
						if ( maxCount ) {
							count = count ? count : 0;
							if ( count >= maxCount ) {
								$videoContainer.removeClass('is-loop')
								return
							} else{
								$loopVideoContent.data('count', count + 1)
								videoLoop.play()
							}
						} else{
							videoLoop.play()
						}
						return;
					}
					videoLoop.oncanplay = function(){
						var flag = true
							, eW = $self.width()
							, eH = $self.height()
							, loopHeight = Math.ceil(eW * parseInt(data.loopVideoData.height) / parseInt(data.loopVideoData.width) ) / eH * 100
						$loopVideoContent.css({
							'height': loopHeight + '%'
							, 'top': '-' + (loopHeight-100)/2 + '%'
						})

						$window.on('scroll.brandPanel',function(){
							spice.debounce(function(){
								var $cover = $self
									, lock = false
									, scrollStart = $cover.offset().top - $window.height()
					    			, scrollEnd = scrollStart + $cover.innerHeight() + $window.height()
					    			, scrollNow = $window.scrollTop()
					    			, scrollH = scrollEnd - scrollStart

					    		if ( scrollNow > scrollStart && scrollNow <= scrollEnd ) {
					    			if ( !$videoContainer.hasClass('is-loop') ) {
					    				if (flag) {
											setTimeout(function(){
												videoLoop.volume = 0
												videoLoop.play()
												flag = false;
											}, 0)
										}
									}
					    		}

							},400,"immediate")();
						}).trigger('scroll.brandPanel');
					}
				}

				if (video) {
					var isMob = false
					if ( $(window).width() <= 767 ) {
						isMob = true
					}
					
					if ( isMob ) {
						video.controls = false
						S.setFullscreenEvents( video, function(e) {
							if ( $videoContainer.hasClass('is-play') ) {
								$videoContainer.removeClass('is-play')
								setTimeout(function(){
									video.pause()
								},0)
							}else{
								$videoContainer.removeClass('is-loop').addClass('is-play')
								$('.homepage-video-content-loop', $self).remove()
								setTimeout(function(){
									video.play()
								},0)
							}
						})
					} else{
						video.controls = true;
						video.onplay = function(){
							$videoContainer.removeClass('is-loop').addClass('is-play')
							$('.homepage-video-content-loop', $self).remove()
							
						}
						video.onended = function(){
							$videoContainer.removeClass('is-play')
						}
					}

					$videoBtn.on('tap', function(){
						video.play()
						isMob && S.launchFullscreen( video )
					})
					$closeBtn.on('tap',function(){
						video.pause()
						$videoContainer.removeClass('is-play')
					})
					if (spice.iPhone) {
