/* 506 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
	    , slotFn = __webpack_require__(507)
	    , spiceModules = __webpack_require__(62)

	// require('./vrvideo')
	__webpack_require__(512);

	var ProductPush = function(element, config){
		this.element = $(element);
	    this.config = this.extend(config);
	    this.init();
	}

	ProductPush.prototype = $.extend(true, {}, new spiceModules(), {
		config: {
			ProductPushConfig: {

			}
		}
		, drawModule: function( json ){
			var self = this
				, $elem = $(self.element)
				, config = self.config
				, data = json || config.defaultDataObj || self.cacheData
				, slot = self.config.slot
				, lazyImg = config.lazyImg || ""
	console.log(9999, data)
			if( !data || !data.content || data.content.length <= 0 ) return false

			$('.spice-cms-lazy-img', $elem).remove()
		
			var oData = data.content

			// if ( !oData.lazyImgSrc ) {
			// 	oData.lazyImgSrc = lazyImg
			// }

			new slotFn( $elem, {
				data: oData
				, sourceReq: config.sourceReq
			})

			//定义几个方法
			var browser={
			    versions:function(){
			        var u = navigator.userAgent, app = navigator.appVersion;
			        return {
			            trident: u.indexOf('Trident') > -1, //IE内核
			            presto: u.indexOf('Presto') > -1, //opera内核
			            webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
			            gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1,//火狐内核
			            mobile: !!u.match(/AppleWebKit.*Mobile.*/), //是否为移动终端
			            ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
			            android: u.indexOf('Android') > -1 || u.indexOf('Adr') > -1, //android终端
			            iPhone: u.indexOf('iPhone') > -1 , //是否为iPhone或者QQHD浏览器
			            iPad: u.indexOf('iPad') > -1, //是否iPad
			            webApp: u.indexOf('Safari') == -1, //是否web应该程序，没有头部与底部
			            weixin: u.indexOf('MicroMessenger') > -1, //是否微信 （2015-01-22新增）
			            qq: u.match(/\sQQ/i) == " qq" //是否QQ
			        };
			    }(),
			    language:(navigator.browserLanguage || navigator.language).toLowerCase()
			}

			function getIEVersion() {
			  var userAgent = navigator.userAgent; //取得浏览器的userAgent字符串  
			  var isIE = userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1; //判断是否IE<11浏览器  
			  var isEdge = userAgent.indexOf("Edge") > -1 && !isIE; //判断是否IE的Edge浏览器  
			  var isIE11 = userAgent.indexOf('Trident') > -1 && userAgent.indexOf("rv:11.0") > -1;
			  if(isIE) {
			      var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
			      reIE.test(userAgent);
			      var fIEVersion = parseFloat(RegExp["$1"]);
			      if(fIEVersion == 7) {
			          return 7;
			      } else if(fIEVersion == 8) {
			          return 8;
			      } else if(fIEVersion == 9) {
			          return 9;
			      } else if(fIEVersion == 10) {
			          return 10;
			      } else {
			          return 6;//IE版本<=7
			      }   
			  } else if(isEdge) {
			      return 'edge';//edge
			  } else if(isIE11) {
			      return 11; //IE11  
			  }else{
			      return -1;//不是ie浏览器
			  }
			}

			function isIOSDevice(){
				if(browser.versions.ios){
					return true
				}else{
					return false;
				}
			}

			function isIE(){
				if(browser.versions.trident){
					return true
				}else{
					return false;
				}
			}


			// define(['jquery', 'eventDispatcher', 'os','viewport'], function($, eventDispatcher, os, viewport) {

			    
			//     * This module handles communication between iframes and windows
			//     * In order to pass data and events we use postMessage function checking the origin
			//     * These changes are mostly for iOS since accelerometer data are not automatically passed
			//     * 


			    //The iframe that contains the 3d video
			    var $iframes = $('.iframe-vr',$elem);

			    var $vrButtonContainers = $('.play-vr-container',$elem);
			    var $vrButton = $('.iframe-btn-icon',$elem);
			    //The close button
			    var $vrClose = $('.video-vr',$elem).find('.iframe-btn-close');
			    var $vrTextContent = $('.iframe-btn-text',$elem);
			    //Get IE version
			    var ieVersion = getIEVersion();
			    var $header = $('header');


			    //Handles communication between windows and iframes checking the origin for security reasons
			    function receiveMessage(event) {
			        var origin = event.origin || event.originalEvent.origin;
			        if (origin !=='https://www.gucciconnect.com/' || origin !=='http://www.gucciconnect.com/' || origin !=='https://www.gucciconnect.com' || origin !=='http://www.gucciconnect.com')
			            return;
			        if (event.data == "iOSfullscreenON" && isIOSDevice() && window.screen.width < 768) {
			            $('[data-iframe-src]').parents('.video-vr').addClass('iOS-fullscreen');
			            $('body').addClass('body-fullscreen');
			        }

			        if (event.data == "iOSfullscreenOFF" && isIOSDevice() && window.screen.width < 768) {
			            $('[data-iframe-src]').parents('.video-vr').removeClass('iOS-fullscreen');
			            $('body').removeClass('body-fullscreen');
			        }
			    }

			    //Handles onScreenOrientationChangeEvent event
			    function onScreenOrientationChangeEvent(event){
			        var message = {action:'onScreenOrientationChangeEvent', orientation : (window.orientation || 0)};
			        sendMessageToIframe(JSON.stringify(message));
			        event.stopPropagation();
			    }
			    //Handles onDeviceOrientationChangeEvent event
			    function onDeviceOrientationChangeEvent(event){
			        var message = {action:'onDeviceOrientationChangeEvent',alpha: event.alpha, beta:event.beta, gamma: event.gamma};
			        sendMessageToIframe(JSON.stringify(message));
			        event.stopPropagation();
			    }

			    //Set event listeners and behaviors for iframe
			    /*
			    * This is specifically needed to relate
			    * device orientation and  motion with 3d video camera
			    * */

			    function on_load(iframe){

			        //Collects device motion data (from the accelerometer) and send it to the iframe
			        function setDeviceMotion(event) {
			            var message = {action:'setDeviceMotion',alpha: event.rotationRate.alpha, beta:event.rotationRate.beta, gamma: event.rotationRate.gamma};
			            sendMessageToIframe(JSON.stringify(message));
			            event.stopPropagation();
			            window.removeEventListener('devicemotion', setDeviceMotion, true);
			            window.addEventListener('deviceorientation', setOrientationControls, true);
			        }

			        //Collects device orientation data (from the accelerometer) and send it to the iframe
			        function setOrientationControls(event) {
			            var message = {action:'setOrientationControls',alpha: event.alpha, beta:event.beta, gamma: event.gamma};
			            sendMessageToIframe(JSON.stringify(message));
			            event.stopPropagation();
			            var message = {action:'onScreenOrientationChangeEvent', orientation : (window.orientation || 0)};
			            sendMessageToIframe(JSON.stringify(message));
			            window.removeEventListener('deviceorientation', setOrientationControls, true);
			            window.addEventListener( 'orientationchange', onScreenOrientationChangeEvent, false );

			            window.addEventListener( 'deviceorientation', onDeviceOrientationChangeEvent, false );
			            window.addEventListener('orientationchange', function(e) {
			                var message = {action:'orientationchange'};
			                sendMessageToIframe(JSON.stringify(message));
			                event.stopPropagation();

			            });
			        }

			        window.addEventListener('devicemotion', setDeviceMotion, true);
			    }

			    //Sends message using the postMessage
			    function sendMessageToIframe(message) {
			        var target = window.frames['iframe'];
			        if(typeof target != "undefined"){

			            target.postMessage(message, "*");
			        }
			    }


			    //Handles iframe resize
			    function resizeIframe(iframe) {
			        var $iframe = iframe;
			        var $iframeContainer = $iframe.parents('.video-vr');
			        var iframeContainerHeight = $iframeContainer.height();
			        if (($iframe.attr('src')!='')) {
			            $iframe.css('height',iframeContainerHeight+'px');
			        }
			    }

			    function resizeAllIframes() {
			        var $iframes = $('.iframe-vr');
			        $iframes.each(function(){
			            resizeIframe($(this));
			        });
			    }


			    //Takes the iframe URL from a data attribute and populates the src of the iframe tag
			    function activateIframe(button) {
			        var $button = $(button);
			        var $buttonContainer = $button.parents('.video-vr');
			        var $iframe = $buttonContainer.find('.iframe-vr');
			        var $close = $buttonContainer.find('.iframe-btn-close');

			        var $heroDiamond = $('.hero-diamond');


			        var iframeUrl = $iframe.attr('data-iframe-src');
			        iframeUrl += (iframeUrl.indexOf('?') === -1 ? '?' : '&') + 'lang=' + $('html').attr('lang');

			        var $vrContent = $buttonContainer.find('.vr-content');

			        var $dialogPanel = $(".spice-flex-standard-slot-promotion");

			        // Check if iframe is already visible
			        if ($iframe.attr('src')=='') {
			            //Step1
			            $iframe.attr('src',iframeUrl);
			            //Step2 order matters
			            setTimeout(function(){
			                resizeIframe($iframe);
			            });

			            $heroDiamond.addClass('opacity0');
			            setTimeout(function() {
			                $vrContent.addClass('opacity0');
			                $dialogPanel.addClass('opacity0');

			                $close.show();
			                $close.css('display','block');
			                if (isIOSDevice() && window.screen.width < 768) {
			                    $buttonContainer.addClass('iOS-fullscreen');
			                    $('body').addClass('body-fullscreen');
			                    resizeIframe($iframe);
			                }
			            }, 200);
			        }

			    }

			    //Closes the iframe removing the src value from the iframe tag.
			    // Sends an event through post message to handle video stop in order to avoid crash on iOS
			    function showContent(closeButton){

			        var $close = $(closeButton);
			        var $iframeContainer = $close.parents('.video-vr');
			        var $iframe = $iframeContainer.find('.iframe-vr');
			        var $vrContent = $iframeContainer.find('.vr-content');
			        var $dialogPanel = $(".spice-flex-standard-slot-promotion");
			        var $heroDiamond = $('.hero-diamond');
			        var message = {action:'onClose'};

			        // Check if iframe is already visible
			        if ($iframe.attr('src')!='') {
			            sendMessageToIframe(JSON.stringify(message));

			            $iframe.css('height','0');
			            $vrContent.show();
			            setTimeout(function(){
			                $vrContent.removeClass('opacity0');
			                $dialogPanel.removeClass('opacity0');
			                $heroDiamond.removeClass('opacity0');
			                $close.hide();
			            },0);

			            setTimeout(function() {
			                $iframe.attr('src','');
			            }, 10);
			        }
			    }

			    function handleTransition(selector){
			        if ($(selector).hasClass('opacity0')) {
			            $(selector).hide();
			        }
			    }

			    function remove360() {
			        $iframes.remove();
			        $vrButtonContainers.remove();
			    }

			    function bindEvents() {

			        //Third check is for edge browser.
			        // TODO: add it to os.js asap to improve getIeVersion function

			        if (ieVersion==11 || !isIE()) {
			            //handle iframe height on resize with debounce
			            // $(window).on('resize', function () {
			            //     viewport.debounce(resizeAllIframes());
			            // });

			            //Activates iframe adding the src attribute
			            $vrButton.on('click', function (e) {
			                activateIframe(e.currentTarget);

			            });

			            $('.iframe-vr').on('load', function(event){
			                on_load(event.currentTarget);
			            });

			            $vrClose.on('click', function (e) {
			                showContent(e.currentTarget);
			                $('[data-iframe-src]').parents('.video-vr').removeClass('iOS-fullscreen');
			                $('body').removeClass('body-fullscreen');
			            });

			            $vrTextContent.on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', function (e) {
			                handleTransition(e.currentTarget);
			            });

			            //handle iframe height
			            //resizeAllIframes($iframes);

			            // handle height for hero in home
			            // since it changes after loading
			            // with an animation
			            $('.video-vr').on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', function () {
			                setTimeout(resizeIframe($('.video-vr').find('.iframe-vr')), 100);
			            });

			            //ascolta e passa eventi solo su ios
			            if (isIOSDevice()) {
			                if (window.addEventListener) {
			                    window.addEventListener("message", receiveMessage, false);
			                } else {
			                    window.attachEvent("onmessage", receiveMessage);
			                }
			            }

			        } else {
			            remove360();
			        }
			    }

			    // return {
			    //     load: function() {

			            bindEvents();
			            //Module activation handling
			//             function activateThisModule() {
			//                 //console.log('video LOADED NEW WAY');
			//             }

			//             //Notify and Listen to module loading
			//             eventDispatcher.dispatchAndListen('vrvideo', activateThisModule);
			//         }

			//     };

			// });


			// $(".e-scan-360",$elem).on('tap', function() {
			// 	var _this = $(this)
			// 		,iframeBtnGroup = _this.parents(".iframe-btn-grounp")
			// 		,iframeClose = iframeBtnGroup.siblings(".iframe-btn-close")
			// 		,$iframe = _this.parents(".agenda-media-wrapper").find("iframe")
			// 	iframeBtnGroup.hide();
			// 	setTimeout(function(){
			// 		$iframe.attr("src",oData.iframeUrl)
			// 	},10);
			// 	setTimeout(function(){
			// 		iframeClose.show();
			// 	},500);
				
				
			// });

			// $(".e-iframe-close",$elem).on('tap', function() {
			// 	var _this = $(this)
			// 		,iframeBtnGroup = _this.siblings(".iframe-btn-grounp")
			// 		,$iframe = _this.parents(".agenda-media-wrapper").find("iframe")
				
