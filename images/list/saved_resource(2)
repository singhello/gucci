/* 296 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
	    , spice = __webpack_require__(2);

	var defaultOptions = {
	        defaultText: ['省份/直辖市', '市', '县/区']
	        , defaultDataObj: window.districtJson
	        , defaultDataStart: '1'
	        , btnClass: '.spice-dropdown-btn'
	        , btnAddClass: 'spice-active'
	        , contentClass: '.spice-overview ul'
	        , defaultError: true
	        , dropdownHtml: null
	        , noSelectedClass:"spice-error"
	        , show: {
	            easing: 'slideDown'
	            , speed: 'fast'
	        }
	        , hide: {
	            easing: 'slideUp'
	            , speed: 'fast'
	        }
	    };

	var defaultFunc = {
	    getPCDObj: function(val, opt){
	        return val && !$.isEmptyObject(opt.defaultDataObj[val]) ? opt.defaultDataObj[val] : {};
	    }
	    , getDefaultSelectVal: function(elem){
	        return $.map($('select', elem), function(n){
	            var $slc = $('option:selected', n);
	            return $slc.attr('value') + '-' + $slc.text();
	        });
	    }
	    , defaultSelect: function($elem, self){
	        var opt = self.opt, elem = self.elem,noSelectedClass = opt.noSelectedClass;

	        $elem
	            .on('tap', function(e){
	                if( e.target.tagName == 'OPTION' ) return false;
	                var $self = $(this)
	                    , btn = $self.prev(opt.btnClass);
	                //如果选择框为半模拟的
	                if(btn.length != 0){
	                    btn.parent().addClass( opt.btnAddClass );
	                }else{
	                    $self.addClass( opt.btnAddClass );
	                }
	            })
	            .on('blur', function(e){
	                if( e.target.tagName == 'OPTION' ) return false;
	                var $self = $(this)
	                    , btn = $self.prev(opt.btnClass);
	                //如果选择框为半模拟的
	                if(btn.length != 0){
	                    btn.parent().removeClass( opt.btnAddClass );
	                }else{
	                    $self.removeClass( opt.btnAddClass );
	                }
	            })
	            .on('change', function(ev, whe){
	                var $self = $(this)
	                    , btn = $self.prev(opt.btnClass)
	                    , index = +$self.attr('spice-data-index')
	                    , val = $('option:selected', $self).attr('value')
	                    , text = $('option:selected', $self).text();
	                //删除默认选中的值，防止影响之后的选择
	                $self.removeAttr('spice-data-curval');
	                //如果选择框为半模拟的，则动态赋值
	                if(btn.length != 0){
	                    $('.spice-dropdown-text', btn).html( text );
	                    btn.parent().removeClass( opt.btnAddClass );
	                }else{
	                    $self.removeClass( opt.btnAddClass );
	                }

	                if( opt.defaultError && !whe ){
	                    if( !val ){
	                        (btn.length != 0 ? btn.closest('.spice-dropdown') : $self)
	                            .addClass(noSelectedClass)
	                            .removeClass('spice-success');
	                    }else{
	                        (btn.length != 0 ? btn.closest('.spice-dropdown') : $self)
	                            .removeClass(noSelectedClass)
	                            .addClass('spice-success');
	                    }
	                    self.elem[$.inArray(true, self.isError()) == -1 ? 'removeClass' : 'addClass' ](noSelectedClass);
	                    self.elem[$.inArray(true, self.isError()) == -1 ? 'addClass' : 'removeClass' ]('spice-success');
	                }

	                var pcdObj = defaultFunc.getPCDObj(val, opt);

	                //当前select change完成后，需要刷新相邻的select的数据
	                var $nextSelect = $('[spice-data-index='+(++index)+']', elem)
	                    , curval = $nextSelect.attr('spice-data-curval');
	                if($nextSelect.length == 0){
	                    elem.trigger('spice.citySelect-selectEnd', {value: defaultFunc.getDefaultSelectVal(elem), elem: elem, whe: whe});
	                    return false;
	                }
	                $nextSelect
	                    .removeAttr('spice-data-curval')
	                    .html( defaultFunc.getOptionHtml(index, pcdObj, curval, self) ).trigger('change', [whe]);

	            });
	    }
	    , hide: function(btnClass, opt, fn){
	        var $subMenu = $('.spice-dropdown-menu', '.spice-dropdown');
	        $subMenu[opt.hide.easing](opt.hide.speed, function(){
	           $subMenu.parent().removeClass(btnClass);
	           $subMenu.parents('.spice-city-select-group').trigger('spice.citySelectMenuHide')
	           fn && fn()
	        });
	    }
	    , getSimulationSelectVal: function(opt, elem){
	        return $.map($(opt.contentClass, elem), function(n){
	            var $slc = $('li.spice-current', n);
	            return $slc.attr('spice-data-value') + '-' + $slc.text();
	        });
	    }
	    , simulationSelect: function($elem, self){
	        var opt = self.opt
	            , elem = self.elem
	            , index = +$elem.attr('spice-data-index')
	            , $btn = self.pcdBtn.eq(index)
	            , noSelectedClass = opt.noSelectedClass;
	        $btn.on('tap', function(ev){
	            ev.preventDefault();
	            var $subMenu = $elem.closest('.spice-dropdown-menu');
	            if($subMenu.is(':visible')){
	                defaultFunc.hide(opt.btnAddClass, opt);
	                return false;
	            }
	            defaultFunc.hide(opt.btnAddClass, opt, function(){
	                $btn.parent().addClass(opt.btnAddClass);
	            });
	            $subMenu.stop(true, true)[opt.show.easing](opt.show.speed, function(){
	                elem.trigger('spice.citySelect-show', self);
	            });
	        });
	        $elem.on('tap', 'li', function(ev, whe){
	            ev.preventDefault();
	            var $self = $(this)
	                , $parent = $self.parent()
	                , index = +$parent.attr('spice-data-index')
	                , val = $self.attr('spice-data-value');

	            $self.addClass('spice-current').siblings().removeClass('spice-current');
	            $parent.removeAttr('spice-data-curval');

	            //赋值并隐藏弹出层
	            var $span = $('.spice-dropdown-text', $self.closest('.spice-dropdown-menu').prev());
	            //如果选择的值与当前的值一样，则不触发 change 事件
	            if($span.html() == val){
	                defaultFunc.hide(opt.btnAddClass, opt);
	                return false;
	            }
	            if( opt.defaultError && !whe ){
	                if( !val ){
	                    $self.closest('.spice-dropdown')
	                        .addClass(noSelectedClass)
	                        .removeClass('spice-success');
	                }else{
	                    $self.closest('.spice-dropdown')
	                        .removeClass(noSelectedClass)
	                        .addClass('spice-success');
	                }
	                self.elem[$.inArray(true, self.isError()) == -1 ? 'removeClass' : 'addClass' ](noSelectedClass);
	                self.elem[$.inArray(true, self.isError()) == -1 ? 'addClass' : 'removeClass' ]('spice-success');
	            }

	            $span.html( $self.text() );
	            defaultFunc.hide(opt.btnAddClass, opt);

	            var pcdObj = defaultFunc.getPCDObj(val, opt);

	            //当前select change完成后，需要刷新相邻的select的数据
	            var $nextSelect = $('[spice-data-index='+(++index)+']', elem)
	                , curval = $nextSelect.attr('spice-data-curval');
	            if($nextSelect.length == 0){
	                elem.trigger('spice.citySelect-selectEnd', {value: defaultFunc.getSimulationSelectVal(opt, elem), elem: elem, whe: whe});
	                return;
	            }
	            $nextSelect
	                .html( defaultFunc.getOptionHtml(index, pcdObj, curval, self) );
	            var $li = $('li.spice-current', $nextSelect);
	            if($li.length == 0){
	                $li = $('li:eq(0)', $nextSelect);
	            }
	            $li.trigger('tap', [whe]);
	        });
	    }
	    , activate: function($elem, self){
	        if(self.isDefault){
	            defaultFunc.defaultSelect($elem, self);
	        }else{
	            defaultFunc.simulationSelect($elem, self);
	        }
	    }
	    , getOptionHtml: function(index, pcdObj, curval, self){
	        var dropdownHtml = self.opt.dropdownHtml
	            , options = '';
	        if( dropdownHtml && $.isFunction(dropdownHtml) ){
	            options = dropdownHtml(index, pcdObj, curval);
	        }else{
	            if(self.isDefault){
	                options = self.opt.defaultText ? '<option value="">' + (self.opt.defaultText[index] || '') + '</option>' : '';
	                $.each(pcdObj, function(i, o){
	                    options += '<option value="' + i + '" ' + ((i == curval || o == curval) ? 'selected' : '') + ' >' + o + '</option>';
	                });
	            }else{
	                options = self.opt.defaultText ? '<li spice-data-value=""><a>' + (self.opt.defaultText[index] || '') + '</a></li>' : '';
	                $.each(pcdObj, function(i, o){
	                    options += '<li' + ((i == curval || o == curval) ? ' class="spice-current"' : '') + ' spice-data-value="' + i + '" ><a>' + o + '</a></li>';
	                });
	            }
	        }

	        return options;
	    }
	}

	function CitySelect(self, option){
	    var $self  = this;
	    $self.opt = $.extend({}, defaultOptions, option);
	    $self.elem = self;
	    $self.init();
	}

	CitySelect.prototype = {
	    init: function(){
	        var self = this
	            , opt = self.opt;
	        //判断数据是否存在，不存在不往下执行
	        if(!opt.defaultDataObj || $.isEmptyObject(opt.defaultDataObj)) return false;

	        var isDefault = self.isDefault = $('select', self.elem).length > 0;
	        if(isDefault){
	            self.select = $('select', self.elem);
	        }else{
	            self.select = $(opt.contentClass, self.elem);
	            self.pcdBtn = $(opt.btnClass, self.elem);
	        }

	        //如果页面中没有找到元素，直接退出以下操作
	        if(self.select.length == 0) return false;

	        $.each(self.select, function(index, elem){
	            var $elem = $(elem)
	                , curval = $elem.attr('spice-data-curval');
	            //设置默认参数，方便之后取值
	            $elem.attr({'spice-data-index': index}).html( defaultFunc.getOptionHtml(index, (index == 0 ? opt.defaultDataObj[opt.defaultDataStart] : {}), curval, self) );
	            //绑定事件
	            defaultFunc.activate($elem, self);
	        });
	        if(isDefault){
	            self.select.eq(0).trigger('change', ['default']);
	        }else{
	            var $li = $('li.spice-current', self.select.eq(0));
	            if($li.length == 0){
	                $li = $('li:eq(0)', self.select.eq(0));
	            }
	            $li.trigger('tap', ['default']);

	            $('html').on('tap', function(e){
	                var $elem = $(e.target);
	                if(!($elem.hasClass('spice-dropdown') || $elem.parents('.spice-dropdown').length != 0)){
	                    defaultFunc.hide(opt.btnAddClass, opt);

	                }
	            });

	        }
	    }
	    , getSelectVal: function(){
	        return (this.isDefault ? defaultFunc.getDefaultSelectVal(this.elem) : defaultFunc.getSimulationSelectVal(this.opt, this.elem));
	    }
	    , setSelectVal: function(city){
	        if(!$.isArray(city) || city.length == 0) return false;
	        if(this.select){
	            this.select.attr('spice-data-curval', function(){
	                return city[$(this).attr('spice-data-index')];
	            });

	            var $select = null;
	            if(this.isDefault){
	                $('option', this.select.eq(0)).removeAttr('selected');
	                $select = $('option:contains('+city[0]+')', this.select.eq(0));
	                if($select.length === 0){
	                    $select = $('option[value='+city[0]+']', this.select.eq(0));
	                }
	                $select.attr('selected', 'selected');
	                this.select.eq(0).trigger('change', ['default']);
	            }else{
	                $('li', this.select.eq(0)).removeAttr('class');
	                $select = $('li:contains('+city[0]+')', this.select.eq(0));
	                if($select.length === 0){
	                    $select = $('li[spice-data-value='+city[0]+']', this.select.eq(0));
	                }
	                $select.addClass('spice-current').trigger('tap', ['default']);
	            }
	        }
	    }
	    , isError: function(){
	        var error = [];
	        $.each(this.getSelectVal(), function(i, v){
	            error.push( !v.split('-')[0] ? true : false );
	        });
	        return error;
	    }
	}

	function Plugin(selector, option) {
	    if( !(selector && !$.isPlainObject(selector)) ) return false;
	    var rtn = {};
	    $(selector).each(function(index, self){
	        var $self = $(self)
	            , data  = $self.data('spice.citySelect');
	        if (!data) $self.data('spice.citySelect', (data = new CitySelect($self, option)));
	        rtn[index] = data;
	    });
	    if( $(selector).length == 1 ){
	        rtn = rtn[0];
	    }
	    return rtn;
	}

	module.exports = Plugin;


/***/ }),
