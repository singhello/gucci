]);
function handleVideoUrl(data, flag) {
	// 不支持flash时，调用
	if (!flag) {
		var videoId = "";
		if ("" != data.videoId && undefined != data.videoId && "undefined" != data.videoId) {
			videoId = data.videoId;
			var url = base + "/cc/getUrl";
			var result = loxia.syncXhrPost(url, {
				videoId : videoId
			});
			if(result){
				data.videoUrlHigh = result.highUrl;
				data.videoUrlLow = result.lowUrl;
				return data;
			}else{
				return data
			}
		} else if ("" != data.loopVideoId && undefined != data.loopVideoId && "undefined" != data.loopVideoId) {
			videoId = data.loopVideoId;
			var url = base + "/cc/getUrl";
			var result = loxia.syncXhrPost(url, {
				videoId : videoId
			});
			if(result){
				data.loopVideoUrlHigh = result.highUrl;
				data.loopVideoUrlLow = result.lowUrl;
				return data;
			}else{
				return data
			}
		}else{
			return data;
		}
	}else{
		return data;
	}
}

$(function() {
    try {
		// 定义全局变量
		S = window.S || {}
		S.base = window.base || "";
        // 获取页面文字
        S.setMultiLanguage([ 'common' ]);
        //ajax方法
        //异步
        S.asyncXhrGet = function(url, data, fn){
            spice.asyncXhrGet( S.base + url , data, {
                success: function(data, textStatus){
                    fn(data, textStatus)
                }
                , error: function(XMLHttpRequest, textStatus, errorThrown){
                    fn(XMLHttpRequest, textStatus, errorThrown)
                }
                , dataType: 'json'
            })
        }
//        try {
	        // 模板调用
	        var data = $("#jsonData").attr("json");
	    	var jsonData = eval("(" + data.replace(/singlequote/g,"'") + ")");
	        
	        //调用分区一数据
	        for ( var key in jsonData["1"]) {
	            // 调用hero组件
	            if (jsonData["1"][key].code == 'heroLooksGrid') {
	                var heroContainment = $("<article class='spice-article e-spice-hero'><img class='spice-cms-lazy-img' src='"+staticbase+"/images/autoReplaceImageSrc/hero-lazy.png' alt='' /></article>");
	                if (key == "0") {
	                    $(heroContainment).insertBefore($(".spice-gutter"));
	                } else {
	                    $(heroContainment).appendTo($(".spice-gutter"));
	                }
	
	                var heroFun = new window.moduleFn.hero($(heroContainment));
	                var content = new Object();
	                content.content = handleVideoUrl(jsonData["1"][key].data, S.hasFlash);
	                heroFun.drawModule(content);
	                
	                
	                //加载面包屑
	                var breadcrumb = $('#look-breadcrumb').html();
	                $("article.e-spice-hero div.spice-flex-standard-slot-label").append(breadcrumb);
	                var navigationLength = $('#navigationLength').val();
	                if(navigationLength!=1){
	                	$('.e-spice-hero .spice-flex-filter-menu>a').on('tap', function(){
	                		$(this).closest('.spice-flex-filter-menu').toggleClass('is-open')
	                	})
	                }
	                $("article.e-spice-hero div.spice-flex-standard-slot-label").find('div.spice-flex-filter-menu').find("a[name='spice-look-hero-filter']").on('tap',function(){
	                    window.location = $(this).attr("spice-hero-filter-value");
	                });
	
	            }
	        }
	        
	        //调用分区二数据
	        for ( var key in jsonData["2"]) {
	            // 调用blockQuote组件
	            if (jsonData["2"][key].code == 'blockQuote') {
	                var heroContainment = $("<article class='spice-article e-spice-block-quote'><img class='spice-cms-lazy-img' src='"+staticbase+"/images/autoReplaceImageSrc/hero-lazy.png' alt='' /></article>");
	                $(heroContainment).insertAfter($(".e-spice-looks-grid"));
	                var heroFun = new window.moduleFn.blockQuote($(heroContainment));
	                var content = new Object();
	                content.content = jsonData["2"][key].data;
	                heroFun.drawModule(content);
	            }
	            // 调用editorialVideo组件
	            if (jsonData["2"][key].code == 'editorialVideo') {
	                var heroContainment = $("<article class='spice-article e-spice-editorial-video'><img class='spice-cms-lazy-img' src='"+staticbase+"/images/autoReplaceImageSrc/hero-lazy.png' alt='' /></article>");
	                if($(".e-spice-block-quote").length > 0){
	                    $(heroContainment).insertAfter($(".e-spice-block-quote"));
	                }else{
	                    $(heroContainment).insertAfter($(".e-spice-looks-grid"));
	                }
	                var heroFun = new window.moduleFn.editorialVideo($(heroContainment));
	                var content = new Object();
	                content.content = handleVideoUrl(jsonData["2"][key].data, S.hasFlash);
	                heroFun.drawModule(content);
	            }
	        }
			
//        } catch (err) {
//        	// 页面渲染出错时
//            console.log(err);
////            $("section.spice-container").empty();
////            $("<div style='display: table;width: 100%;height: 100%;'><div style='width: 100%;height: 700px;text-align: center;position: relative;display: table-cell;vertical-align: middle;'><img src='"+staticbase+"/images/loading.gif'></div></div>").appendTo(
////                    $("section.spice-container"));
//        }
        
        // looksGrid
        var timestamp = Date.parse(new Date());
        var serial = $("#runwaySerial").val();
        var looksGridData = null;
        $.ajax({  
            type : "GET",  
            url : base + "/data/runway/"+serial+"?time="+timestamp,  
            success : function(result) {
                console.log(result);
                if(result!=null && result!=""){
                	looksGridData = result;
                	var heroContainment = $(".e-spice-looks-grid");
//              $(heroContainment).appendTo($(".spice-gutter"));
                	var heroFun = new window.moduleFn.looksGrid($(heroContainment));
                	var content = new Object();
                	content.content = looksGridData.content;
                	heroFun.drawModule(content);
                	$(window).trigger('resize.spice-autoReplaceImageSrc');
                	
                	
                	// 加入删除心愿单
                	$('.spice-item-hover .e-spice-wishlist-add').on('tap',function(){
                		var _this = $(this);
                		var looksCode = _this.attr('spice-wishlist-code');
                		if($(this).hasClass('spice-btn-isWishList')){
                			var url = base + "/wish/remove";
                			var data = loxia.syncXhrPost(url, {
                				looksCode : looksCode
                			});
                			if (data.result) {
                				window.loadingMiniWashCout();
                				_this.removeClass('spice-btn-isWishList');
                			}
                		} else {
                			var url = base + "/wish/add";
                			var data = loxia.syncXhrPost(url, {
                				looksCode : looksCode
                			});
                			if (data.result) {
                				window.loadingMiniWashCout();
                				_this.addClass('spice-btn-isWishList');
                				//window.showHeaderMiniWash()
                			}
                		}
                	});
                }
            }  
        });  

        $(window).trigger('resize.spice-autoReplaceImageSrc');
        $(".spice-module-dami").remove();
    } catch (err) {
        // 页面渲染出错时
        console.log(err);
//        $("section.spice-container").empty();
//        $("<div style='display: table;width: 100%;height: 100%;'><div style='width: 100%;height: 700px;text-align: center;position: relative;display: table-cell;vertical-align: middle;'><img src='"+staticbase+"/images/loading.gif'></div></div>").appendTo(
//                $("section.spice-container"));
    }
    
    
    
});

$(function() {
    var $looksGrid = $('.e-spice-looks-grid')
     // 浏览更多按钮
     $looksGrid.on('spice.looks-grid-append', function(e,o){
        var postData = {}
            , inputVal = $('#runwaySerial').val()
            , pageNo = o.elem.attr('spice-data-pageno')

        postData.runwaySerial = inputVal
        postData.pageNo = pageNo

        //这里不加base，因为asyncXhrGet这个方法自动加了base
        S.asyncXhrGet('/pagedata/runway/'+inputVal+'/'+pageNo, postData, function(data, textStatus, errorThrown){
            console.log(data, textStatus, errorThrown)
            if ( textStatus == "success" ) {
                o.data.appendLi( S.parseJSON(data) )
                $(window).trigger('resize.spice-autoReplaceImageSrc')
                
                
             // 加入删除心愿单
            	$('.spice-item-hover .e-spice-wishlist-add').on('tap',function(){
            		var _this = $(this);
            		var looksCode = _this.attr('spice-wishlist-code');
            		if($(this).hasClass('spice-btn-isWishList')){
            			var url = base + "/wish/remove";
            			var data = loxia.syncXhrPost(url, {
            				looksCode : looksCode
            			});
            			if (data.result) {
            				window.loadingMiniWashCout();
            				_this.removeClass('spice-btn-isWishList');
            			}
            		} else {
            			var url = base + "/wish/add";
            			var data = loxia.syncXhrPost(url, {
            				looksCode : looksCode
            			});
            			if (data.result) {
            				window.loadingMiniWashCout();
            				_this.addClass('spice-btn-isWishList');
            			}
            		}
            	});
            }
        })
        
        
        

     })
     
    
});
