			var liHtml = ['<ul>']
				, selectValue = this.config.selectValue;
			$.each(this.config.content, function(i, v){
				liHtml.push('<li spice-data-value="' + v.value + '"' + ( (selectValue ? v.value == selectValue.value : i == 0) ? ' class="spice-current"' : '' ));
