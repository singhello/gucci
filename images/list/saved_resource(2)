/* 216 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(217)
		, slot = __webpack_require__(164)
		, dropdownFn = __webpack_require__(189)

	__webpack_require__(218)

	function SlotSelect(config){
		this.config = $.extend(true, {}, this.config, config)
		this.template = template
		this.data = this.config
	}

	SlotSelect.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function(elem){
			this.drawEvents(elem)
		}
		, drawEvents: function( elem ){
			var self = this
				, config = self.config
				, dropdownConfig = config.dropdownConfig
				, $dropdown = $('.e-dropdown', elem)
				, dropdown = new dropdownFn($dropdown, dropdownConfig)
				, data = {
					content: []
				}

			$.each(config.content, function(k, v){
				var obj = {}
				if( dropdownConfig.slot ) obj.slot = dropdownConfig.slot
				obj.content = v.options
				data.content.push( obj )
			})

			dropdown.drawModule( data )

			$.each($('.spice-dropdown', dropdown.element), function(i, elem){
				var content = config.content[i]
					, name = content.name
					, dataTerm = content.dataTerm
					, term = []

				$.each( dataTerm, function(key, value){
					term.push( '"' + key + '":' + '"' + value + '"' )
				})

				$(elem).attr({
					name: name
					, 'spice-data-term': '{' + term.join(',') + '}'
				})
			})
			
		}
	})

	SlotSelect.prototype.constructor = SlotSelect

	module.exports = SlotSelect

/***/ }),
