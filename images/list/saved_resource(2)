/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(348)
		, slot = __webpack_require__(150)
		, spice = __webpack_require__(2)
		, dropdown = __webpack_require__(189)

	__webpack_require__(349);

	var Standard = function(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	Standard.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function(elem){
			this.drawEvents(elem);
		}
		, drawEvents: function( elem ){
			var self = this
				, config = self.config

			var  dropdownSourceReq = __webpack_require__(305)
				, dropdownConfig = $.extend({},{
					sourceReq:dropdownSourceReq
				}, config.dropdownConfig)
				, $colorDropdown = $('.e-spice-dialog-edit-color-dropdown', elem)
				, $sizeDropdown = $('.e-spice-dialog-edit-size-dropdown', elem)

			if ( $colorDropdown.length > 0 ) {
				var colorDropdownObj = new dropdown( $colorDropdown, dropdownConfig )
					, colorDropdownSlot = spice.hasTouch ? "slot-extend-simulation-color-noscroll" : "slot-extend-simulation-color-noscroll"
					, colorDropdownData = {
						"content": [
							{
								"slot": colorDropdownSlot
								, "content": config.allColor
								, "selectValue": config.selectedColor
								, "colorText": window.multiLanguage.page['shopping-cart-dialog-edit-color-text']
							}
						]
					}
				colorDropdownObj.drawModule( colorDropdownData )
			}

			if ( $sizeDropdown.length > 0 ) {
				var sizeDropdownObj = new dropdown( $sizeDropdown, dropdownConfig )
					, sizeDropdownSlot = spice.hasTouch ? "slot-rewrite-semi-simulation-size" : "slot-extend-simulation-size-noscroll"
					, arrSize = []
				arrSize.length = config.allSize.length
				for (var i = 0; i < config.allSize.length; i++) {
					arrSize[i] = {
						"words": config.allSize[i].sizeText
						, "value": config.allSize[i].value
						, "sizeText": config.allSize[i].sizeText
						, "stock": config.allSize[i].stock
					}
				}
				var sizeDropdownData = {
						"content": [
							{
								"slot": sizeDropdownSlot
								, "content": arrSize
								, "selectValue": config.selectedSize
								, "sizeText": window.multiLanguage.page['shopping-cart-dialog-edit-size-text']
								, "labelRight": window.multiLanguage.page['shopping-cart-dialog-edit-size-text']
							}
						]
					}
				sizeDropdownObj.drawModule( sizeDropdownData )
				var $sizeSelect = $('select', $sizeDropdown)
				if ( $sizeSelect.length > 0 ) {
					$sizeSelect.find('option').each(function(i,j){
						var $this = $(j)
							, stock = $this.attr('stock')
							, multiLanguage = window.multiLanguage
						if ( multiLanguage && stock == 'outOfStock' ) {
							$this.html( $this.html() + multiLanguage.common['common-dropdown-outofstock'] )
						}
					})
					$sizeSelect.on('spice.dropdown-change', function(e,o){
						elem.trigger('spice.slot-dropdown-select-change', o)
					})
				}
			}
			
		}
	});

	Standard.prototype.constructor = Standard;

	module.exports = Standard;

