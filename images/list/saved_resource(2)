/* 362 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3),
	    spice = __webpack_require__(2);
	function Flex(selector,options){
	    if(!(this instanceof arguments.callee)){
	        return new Flex(selector,options);
	    };
	    this.selector = selector;
	    this.elem = $(selector);
	    this.defineOption = options;
	    this.ratio = 1;
	    if(this.defineOption.useDefaultConfig === false){
	        this.options = $.extend(false,{},this.options,options);
	    }else{
	        this.options = $.extend(true,{},this.options,options);
	    }
	    this.init();
	};
	Flex.prototype = $.extend(true,{},{
	    options:{
	        config:spice.config,
	        open:false,
	        small:{
	            settedMaxHeight:520,
	            minHeight:300,
	            maxHeight:900,
	            offsetHeightBtm:0,
	            offsetHeightTop:50,
	            ratio:3,
	            opacityLevel:10
	        },
	        medium:{
	            settedMaxHeight:520,
	            minHeight:300,
	            maxHeight:520,
	            offsetHeightBtm:30,
	            offsetHeightTop:80,
	            ratio:3,
	            opacityLevel:10
	        },
	        standard:{
	            settedMaxHeight:520,
	            minHeight:400,
	            maxHeight:520,
	            offsetHeightBtm:120,
	            offsetHeightTop:80,
	            ratio:3,//高/宽
	            opacityLevel:50
	        },
	        fn:function(debounceObj,debounce){

	        },
	        useDefaultConfig:true,
	        checkRetina:false
	    },
	    init:function(){
	        this.drawEvents();
	        this.elem.data('spice-flex',this);
	    },
	    drawEvents:function(){
	        var self = this,
	            $elem = self.elem,
	            $fixedDom  = $elem.find('.spice-flex-content'),
	            $maskDom = $fixedDom.find('.spice-flex-mask'),
	            height = $fixedDom.height(),
	            $fixedImgs = $fixedDom.find('img'),
	            length = $fixedImgs.length,
	            $fixedVideo = $fixedDom.find('video'),
	            videoDomLength = $fixedVideo.length,
	            timer,
	            initFlag = true,
	            initFlagTimer,
	            maxHeight,
	            debounce,
	            debounceMaxValue = self.computeDebounceMaxValue(),
	            //用来缓存边界值
	            debounceObj = {},
	            winHeight = window.innerHeight,
	            lastWinHeight = winHeight,
	            winWidth = window.innerWidth,
	            lastWinWidth = winWidth,
	            open = self.options.open || false,
	            // spice.config.debounceTime = 0
	            $nextDom = $elem.next();
	            // 用来保证下级dom元素能正常覆盖该插件
	            if($nextDom.css('position') !== 'relative'){
	                $nextDom.css('position','relative');
	            }
	            $nextDom.css({
	                    'zIndex':parseInt($('.spice-flex-content').css('zIndex'))+1,
	                });
	            if(!$elem.hasClass('spice-flex-wrapper')){
	                $elem.addClass('spice-flex-wrapper');
	            }
	        var resizeScreenDebounce = spice.resizeScreenDebounce({
	            checkRetina: self.options.checkRetina,
	            debounceTime: 0,
	            callBack: function(media){
	                var key;
	                debounce = media;
	                winHeight = window.innerHeight;
	                winWidth  = window.innerWidth;
	                if(!open && !initFlag)return;
	                if(initFlag){
	                    for(key in self.options[media]){
	                        if(key !== 'settedMaxHeight'){
	                            $elem.css(key,self.options[media][key]+'px');
	                        }
	                    }
	                }else{
	                    if( lastWinHeight != winHeight ){
	                        debounceObj = {};
	                        lastWinHeight = winHeight;
	                        // lastWinWidth = winWidth;
	                    }

	                    setMaxHeight(self.ratio);
	                }
	                if(length === 0){
	                    height = $fixedDom.height();
	                    $elem.height(height);
	                    if(!self.options[debounce]['ratio']){
	                        console.error('请传入一个高/宽的比例值');
	                        return;
	                    };
	                    setMaxHeight(self.options[debounce]['ratio']);
	                }
	            }
	        });
	        $(window)
	            .off('resize.flex')
	            .on('resize.flex',resizeScreenDebounce)
	            .trigger('resize.flex');
	        if(length>0){
	            $fixedImgs.on('load',function(e){
	                var $dom = $(this),
	                    domHeight = $dom.height(),
	                    ratio = parseInt($dom[0].height)/parseInt($dom[0].width),
	                    $size =$elem.find('.spice-flex-size'),
	                    $sizeImg = $size.find('img');
	                    self.ratio = ratio;
	                if( videoDomLength === 0 ){
	                    maskFade();
	                }
	                if($sizeImg.height()!== domHeight){
	                    $sizeImg.attr('src',$dom.attr('src'));
	                    if(!initFlag){
	                        if(!open)return;
	                        setTimeout(function(){
	                            setMaxHeight(ratio);
	                        },200);
	                    }
	                }
	            })
	        }
	        if( videoDomLength>0 ){
	            // console.log($fixedVideo)
	            $fixedVideo.on('loadstart',function(){
	                maskFade();
	            })
	        }

	        function maskFade(){
	            var opacityLevel = self.elem.find('.spice-flex-mask').data('flex-mask-opacity')|| self.options[debounce].opacityLevel || 0,
	                $size =$elem.find('.spice-flex-size'),
	                $sizeImg = $size.find('img');
	                opacityLevel /= 100;
	            if(initFlag){
	                !!timer && clearTimeout(timer);
	                timer = setTimeout(function(){
	                    if($sizeImg[0].complete){
	                        $maskDom.css('opacity',opacityLevel);
	                        if(!open)return;
	                        setMaxHeight(self.ratio);
	                    }else{
	                        $sizeImg
	                            .off('load')
	                            .on('load',function(){
	                                $maskDom.css('opacity',opacityLevel);
	                                if(!open)return;
	                                setMaxHeight(self.ratio);
	                            })
	                    }
	                },200);
	                initFlag = false;
	            };
	        }


	        function setMaxHeight(ratio){
	            // if(!self.options.open)return;
	            var maxHeight,pos;
	            var offsetHeightBtm = !!self.options[debounce] ? self.options[debounce].offsetHeightBtm : 0,
	                offsetHeightTop = !!self.options[debounce] ? self.options[debounce].offsetHeightTop : 0;
	            // if(!debounceObj[debounce]){
	                if(!!self.options[debounce] && !!(maxHeight = self.options[debounce].settedMaxHeight)){

	                }else{
	                    if(!!debounceMaxValue[debounce]){
	                        maxHeight = debounceMaxValue[debounce]*ratio;
	                    }else{
	                        maxHeight = debounceMaxValue[(pos = debounce.toLowerCase().indexOf('retina'))>0?debounce.slice(0,pos):debounce]*ratio;
	                        if(!maxHeight && isNaN(maxHeight)){console.error('请将config参数配置正确')};
	                    }
	                    if(maxHeight > (winHeight-offsetHeightBtm-offsetHeightTop)){
	                        maxHeight = winHeight-offsetHeightBtm-offsetHeightTop;
	                    }
	                }
	                debounceObj[debounce] = maxHeight;
	            // }
	            self.ratio = ratio;
	            $elem.css('maxHeight',debounceObj[debounce]+'px');
	            if(!!self.options.fn){
	                self.options.fn(self, debounceObj,debounce);
	            }
	        }
	    },
	    computeDebounceMaxValue: function(){
	        var maxDebounce = {},
	            win_w = window.screen.availWidth,
	            resize = this.options.config.resize,
	            resize_fb = $.extend({},resize),
	            key,
	            i,
	            tempAry = [],
	            obj = {},
	            ary = [],
	            tempObj={};
	        for(key in resize_fb){
	            tempAry.push(resize_fb[key]);
	            obj[resize_fb[key]] = key;
	        }
	        for(key in obj){
	            ary.push(key);
	        }
	        tempAry.push(win_w);
	        tempAry.sort(sort);
	        ary.sort(sort);
	        for(i=0,len = ary.length;i<len;i++){
	            ary[i] = obj[ary[i]];
	        }
	        var tempValue = tempAry.shift();
	        for(i = 0,len = tempAry.length ; i < len ; i++ ){
	            if(i == 0){
	                maxDebounce[ary[i]] = tempAry[i];
	            }else{
	                maxDebounce[ary[i]] = +tempAry[i]+1;
	            }
	        };

	        return maxDebounce;
	        function sort(a,b){
	            return a-b;
	        };
	    }
	})
	module.exports = Flex;


