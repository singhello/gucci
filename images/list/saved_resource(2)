/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
	    , slotFn = __webpack_require__(492)
	    , spiceModules = __webpack_require__(62)
	    , template = __webpack_require__(497)
	    , spice = __webpack_require__(2)

	__webpack_require__(498);

	var Capsule = function(element, config){
		this.element = $(element);
	    this.config = this.extend(config);
	    this.init();
	}

	Capsule.prototype = $.extend(true, {}, new spiceModules(), {
		config: {
			CapsuleConfig: {

			}
		}
		, drawModule: function( json ){
			var self = this
				, $elem = $(self.element)
				, config = self.config
				, data = json || config.defaultDataObj || self.cacheData
				, slot = self.config.slot
				, lazyImg = config.lazyImg || ""

			if( !data || !data.content ) return false

			$('.spice-cms-lazy-img', $elem).remove()

			var oData = data.content

			if ( oData.grids ) {
				for (var i = 0; i < oData.grids.length; i++) {
					if ( !oData.grids[i].lazyImgSrc ) {
						oData.grids[i].lazyImgSrc = lazyImg
					}
				};
			}

			var filterArr = [{ id: 'all'}]
				, grids = oData.grids
				, filterObj = {}
				, k
				, media
				, wW = $(window).width()
			if ( oData.hasLabels ) {
				filterArr = oData.labels || [{ id: 'all'}]
			}

			// spice.resizeScreenDebounce({
			// 	checkRetina: false
	  //           , callBack: function(media){
	  //           	console.log(media)
	  //               k = oData.isLooks ? parseInt( oData.looksLimit[media] ) : parseInt( oData.itemsLimit[media] )
	  //           }
	  //       })()
			if ( wW <= 767 ) {
				media = 'small'
			} else if( wW <= 1023 ){
				media = 'medium'
			} else{
				media = 'standard'
			}
			k = oData.isLooks ? parseInt( oData.looksLimit[media] ) : parseInt( oData.itemsLimit[media] )

	        self.limit = k

			if ( grids.length <= k ) { oData.noMorePage = true }
			$elem.html('').append( template(oData) )

			for (var i = 0; i < filterArr.length; i++) {
				filterObj[filterArr[i].id] = {}

				if ( filterArr[i].id == 'all' ) {
					filterObj[filterArr[i].id].standard = grids.slice( 0 , k )
					filterObj[filterArr[i].id].viewAll = grids.slice( k )
				} else{
					var a = []
					for (var l = 0; l < grids.length; l++) {
						if ( grids[l].filterId == filterArr[i].id ) {
							a.push( grids[l] )
						}
					}
					filterObj[filterArr[i].id].standard = a.slice( 0 , k )
					filterObj[filterArr[i].id].viewAll = a.slice( k )
				}
				
			}
			self.filterObj = filterObj
			var o = oData
			o.grids = filterObj.all.standard

			var $ul = $('ul',$elem)
			$ul.on('spice.capsule-li-complete', function(e,o){
				$elem.trigger('spice.capsule-load-li-complete', o)
			})
			new slotFn( $ul, {
				data: o
				, sourceReq: config.sourceReq
			})


			var $appendBtn = $('.e-looks-grid-append', $elem)
			if ( $appendBtn.length > 0 ) {
				$appendBtn.attr('spice-looks-grid-id', 'all')
			}

			self._drawEvents($elem);
		}
		, _drawEvents: function( elem ){
			var self = this

			$('.e-looks-grid-append',elem).on('tap',function(){
				var $this = $(this)
					, id = $this.attr('spice-looks-grid-id')
					, filterObj = self.filterObj

				var json = {
						'noMorePage': true
						, 'grids': filterObj[id].viewAll
					}
				
				self.appendLi(json)
			})
			var $filterBtn = $('.e-looks-grid-refresh',elem)
			$filterBtn.on('tap',function(){
				var $this = $(this)
					, id = $this.attr('spice-looks-grid-id')
					, filterObj = self.filterObj

				if ( $this.hasClass('is-active') ) { return false }

				$filterBtn.removeClass('is-active')
				$this.addClass('is-active')

				var noMorePage = (filterObj[id].standard.length + filterObj[id].viewAll.length) > self.limit ? false : true
					, json = {
						'noMorePage': noMorePage
						, 'filterId': id
						, 'grids': filterObj[id].standard
					}

				$('ul', elem).html('')
				self.appendLi(json)
			})
		}
		, appendLi: function( json ){
			var self = this
				, $elem = $(self.element)
				, config = self.config
				, lazyImg = config.lazyImg || ""

			if( !json ) return false

			var oData = json
			oData.slot = "slot-standard"

			new slotFn( $('ul',$elem), {
				data: oData
				, sourceReq: config.sourceReq
			})

			$(window).trigger('resize.spice-autoReplaceImageSrc')

			var $appendBtn = $('.e-looks-grid-append', $elem)
			if ( oData.noMorePage ) {
				$appendBtn.addClass('spice-none')
			} else{
				$appendBtn.removeClass('spice-none')
			}
			if ( oData.filterId ) {
				$appendBtn.attr('spice-looks-grid-id', oData.filterId)
			}
		}
	});

	Capsule.prototype.constructor = Capsule;

