/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, spice= __webpack_require__(2)
		, baseLibPath = '../../../../../libs'
		, baseModulesPath = '../../../..'
		, template = __webpack_require__(303)
		, slot = __webpack_require__(276)
		, dropdownFn = __webpack_require__(189)

	__webpack_require__(6)
	__webpack_require__(304)

	var sourceReqDropdown = __webpack_require__(305)

	function SlotFn(config){
		this.config = $.extend(true, {}, this.config, config)
		this.template = template
		this.data = this.config
	}

	SlotFn.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function(elem){
			this.drawEvents(elem)
		}
		, drawEvents: function( elem ){
			var $dropDownDom = $('.e-dropdown',elem),
					url = $dropDownDom.attr('spice-data-url'),
					slot = spice.hasTouch?'slot-rewrite-semi-simulation':'slot-extend-simulation-noscroll'
				
					if(!!url){
						var data = window.multiLanguage.page['checkout-'+url]
						if(!data){
							data = window.multiLanguage.common['common-'+url]
						}
						$dropDownDom.attr('spice-data-url','');
						if(!data){
							url = url.replace('zh',S.language);
							data = spice.syncXhr(url);
						}
					var dropDownInstance = new dropdownFn($dropDownDom,{
							sourceReq:sourceReqDropdown
						});
						data.content[0].slot = slot;
						dropDownInstance.drawModule(data);
						$dropDownDom.data('spice.dropdown',dropDownInstance);
					}
		}
	})

	SlotFn.prototype.constructor = SlotFn

	module.exports = SlotFn


