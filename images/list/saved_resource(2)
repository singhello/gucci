/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, spice = __webpack_require__(2)
		, Swiper =__webpack_require__(463)
	    , slotFn = __webpack_require__(464)
	    , spiceModules = __webpack_require__(62)
	    , gridTemple = __webpack_require__(109)

	__webpack_require__(469)

	function looksCollection(element, config){
		this.config = this.extend(config)
		this.element = $(element)    
	    this.init()
	}

	looksCollection.prototype = $.extend(true, {}, new spiceModules(), {
		config: {
			
		}
		, drawModule: function( json ){
			var self = this
				, data = json || self.cacheData
				, $elem = $(this.element)
				, lazyImg = self.config.lazyImg
				, slot = self.config.slot

			// if( !data ) return false

			data.content.itemGroupSlot = (data.content.itemGroupSlot==""||data.content.itemGroupSlot=="undefined")?"slot-looks-collection":data.content.itemGroupSlot

			if ( data.content ) {
				
				$('.spice-cms-lazy-img', $elem).remove()

				new slotFn( $elem, {
					data: data.content
					, sourceReq: self.config.sourceReq
				})

				var gridTempleConfig = data.content.itemGroup
				for (var i = 0; i < gridTempleConfig.length; i++) {
					gridTempleConfig[i].slot = data.content.itemGroupSlot
				};
				var gridTempleData = {
					"content": gridTempleConfig
				}

				var swiperItem = $('.swiper-slide',$elem);
				var swiperContainer = $('.swiper-container', $elem);

				new gridTemple( swiperItem, {
					defaultDataObj: gridTempleData
					, sourceReq: self.config.sourceReq
				});


				function swiperUnit(){
					var mySwiper = new Swiper(swiperContainer, {
						pagination: '.swiper-pagination',
						observer: true,
						observeParents: true,
						centeredSlides: true,
						grabCursor: true,
						slideToClickedSlide: true,
						paginationClickable: true,
						slidesPerView: "auto",
						initialSlide: 0,
						autoplayDisableOnInteraction: false
					})
					swiperContainer.data('swiper', mySwiper)		
				}


				function swiperFun(){
					var windowW =  window.innerWidth;
					if (windowW >= 768) {
						
							if (swiperContainer.data('swiper')){
								swiperContainer.data('swiper').destroy(true,true)
								swiperContainer.removeData('swiper')
							}
							
							// $('.looks-collection-look-wrapper').addClass('no-swiper');
						
						
					}else {
						if (swiperContainer.data('swiper')){
							swiperContainer.data('swiper').destroy(true,true)
							swiperContainer.removeData('swiper')
						}

							// $('.looks-collection-look-wrapper').removeClass('no-swiper');
						swiperUnit();
					}
				}

				swiperFun();



				var media = null,
					checkMedia = function(){
				        var media = null
				            , w =  window.innerWidth
				        if ( w <= 767 ) {
				            media = 'mobile'
				        } else if( w <= 1023 ){
				            media = 'pad'
				        } else{
				            media = 'pc'
				        }
				        return media
				    }
				

				$(window).on('resize.swiper', function() {
					var isMobile =  window.innerWidth <= 767
				 		,mediaNow = checkMedia()

				 	//刚切换至mb
				 	if ( mediaNow != media && isMobile) {
				 		// console.log("to mb");
				 		swiperFun();
				 	}//pc
				 	if(!isMobile){

				 	}
				 	//刚切换至pc
				 	if( mediaNow != media && !isMobile ){
				 		// console.log("to pc");
				 		swiperFun();
				 	}		
				 	media = mediaNow;
				}).trigger('resize.swiper')
		
			}


			$elem.trigger('spice.looksCollection-completed', {
					data: self
					
				})

		}
		, drawEvents: function(elem){
			
		}
	})

	looksCollection.prototype.constructor = looksCollection

	module.exports = looksCollection

