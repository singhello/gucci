/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, slotFn = __webpack_require__(170)
		, dropdown = __webpack_require__(189)
		, citySelectFn = __webpack_require__(28)
		, mobIscrollFn = __webpack_require__(191)

	__webpack_require__(199)

	function CitySelect(element, config){
		this.config = this.extend(config)
		this.element = $(element)
		var widgetConfig = $.parseJSON( this.element.attr('spice-data-widget-config') )
			, citySelectConfig = this.config.citySelectConfig
		if( widgetConfig && $.isPlainObject(widgetConfig) && !$.isEmptyObject(widgetConfig) ){
			this.config.citySelectConfig = $.extend(true, {}, citySelectConfig, widgetConfig)
		}
	    this.init()
	}

	CitySelect.prototype = $.extend(true, {}, new dropdown(), {
		config:{
			// 联动数据
			defaultDataObj: null
			// 默认选中文字
			, defaultSelectVal: []
			// 模板名，也可直接从数据中获取
			, slot: 'slot-select'
			, labelText:['省','市','区']
			, citySelectConfig: {
				// 默认文字
				defaultText: ['省', '市', '区']
				// 开始地方
				, defaultDataStart: '1'
				, btnClass: '.spice-dropdown-btn'
				, contentClass: '.spice-overview ul'
			}
			, mobIscroll: true
			, mobIscrollText: '请选择省/市/区'
			, mobIscrollClass: 'spice-none spice-block-xs'
			, dropdownClass: 'spice-block spice-none-xs'
			, mobIscrollConfig: {
				wheelsStart: '1'
				, joinResult: '/'
				, preset: 'citySelect'
			}
		}
		, drawModule: function( json ){
			var self = this
				, $elem = self.element
				, config = self.config
				, mobIscroll = config.mobIscroll
				, mobIscrollText = config.mobIscrollText
				, mobIscrollClass = config.mobIscrollClass
				, dropdownClass = config.dropdownClass
				, data = json || config.defaultDataObj || self.cacheData

			self.cacheData = data

			if( !self.cacheData.slot ){
				self.cacheData.slot = config.slot
			}

			var editedData = $.extend(true, {}, data)
				, appendHtml = ['<div class="spice-city-select-group">']

			if( mobIscroll ){
				appendHtml.push('<div class="spice-dropdown spice-dropdown-mobiscroll' + ( mobIscrollClass ? ' ' + mobIscrollClass : '' ) + '">\
								    <a class="spice-dropdown-btn">\
								    	<span class="spice-dropdown-text" data-city-default-text="' + mobIscrollText + '">' + mobIscrollText + '</span>\
								    	<span class="spice-dropdown-icon">\
								    		<i></i>\
								    	</span>\
								    </a>\
								</div>')
			}

			appendHtml.push('<div class="spice-dropdown spice-dropdown-city-select' + ( mobIscroll && dropdownClass ? ' ' + dropdownClass : '' ) + '">\
					<div class="spice-dropdown-city-select-content"></div>\
							</div>')

			appendHtml.push( '</div>' )

			$elem.append( appendHtml.join('') )

			$.each(config.citySelectConfig.defaultText, function(i, o){
				// 初始化Slot
				new slotFn($('.spice-dropdown-city-select-content', $elem), {
					data: editedData
					, sourceReq: config.sourceReq
					, defaultDataStart: config.citySelectConfig.defaultDataStart
					, index: i
					, defaultValue: o
					, defaultSelectVal: config.defaultSelectVal[i]
					, labelText:config.labelText[i]
				})
			})
		}
		, drawEvents: function(elem){
			var self = this
				, $elem = elem
				, config = self.config
				, $citySelect = $('.spice-dropdown-city-select-content', $elem)
				, $mobSelect = $('.spice-dropdown-mobiscroll', $elem)
				, $mobText = $('.spice-dropdown-text', $mobSelect)
				, isSelect = true
				, joinResult = config.mobIscrollConfig.joinResult
				, getValList = function(val){
					var c = [], v = [];
					$.each(val, function(i, va){
						c.push( va.split('-')[0] );
						v.push( va.split('-')[1] );
					});
					return {
						c: c
						, v: v
					}
				}

			// citySelect
			$citySelect.on('spice.citySelect-selectEnd', function(e, o){
				var l = getValList( o.value )
					, c = l.c
					, v = l.v

				if(isSelect){
					$mobText.html( (
									$.inArray('', c) >= 0 ? $mobText.attr('data-city-default-text') : v.join( joinResult )
								) );
					$mobSelect.attr('data-city-selector-value', c.join( joinResult ));
				}

			})

			var citySelect = new citySelectFn($citySelect, $.extend(true, {}, {
				defaultDataObj: self.cacheData
				, dropdownHtml: function(index, pcdObj, curval){
					var slot = new slotFn('dropdownHtml', {
								sourceReq: config.sourceReq
							})
					return slot._drawSlotModuleEvents({
						data: self.cacheData
						, defaultData: pcdObj
						, curval: curval
						, defaultValue: config.citySelectConfig.defaultText[index]
					})
				}
			}, config.citySelectConfig))
			self.citySelectInstance = citySelect;
			citySelect.elem.on('spice.citySelect-show', function(){
				$(window).trigger('resize.Tinyscrollbar')
			})

			// mobIscroll
			config.mobIscrollConfig.preset = 'citySelect'

			config.mobIscrollConfig.onChange = function(v, inst){
								isSelect = false
							}

			config.mobIscrollConfig.onSelect = function(valueText, inst){
								isSelect = true;
								citySelect.elem.data('spice.citySelect').setSelectVal(
													valueText.split( joinResult )
												)
								$mobText.html( valueText )
							}

			new mobIscrollFn($mobSelect, $.extend(true, {}, {
				customWheels: self.cacheData
			}, config.mobIscrollConfig))

			$(window).off('resize.spice-city-select-resize').on('resize.spice-city-select-resize', function(){
				$.each($elem, function(i, elem){
					var $elem = $(elem)
						, $citySelect = $('.spice-dropdown-city-select-content', $elem)
						, $mobSelect = $('.spice-dropdown-mobiscroll', $elem);

					$mobSelect.data('spice.mobIscroll').cancel()

				});
			});

		}
	})

	CitySelect.prototype.constructor = CitySelect

	module.exports = CitySelect


