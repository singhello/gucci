/* 246 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(247)
		, slot = __webpack_require__(164)
		, spice = __webpack_require__(2)

	__webpack_require__(248)

	function SlotRewriteInputText(config){
		this.config = $.extend(true, {}, this.config, config)
		this.template = template
		this.data = this.config
	}

	SlotRewriteInputText.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function(elem){
			this.drawEvents(elem)
		}
		, drawEvents: function( elem ){
			$(':checkbox', elem).on('spice.validator-error', function(){
				$(this).closest('label').removeClass('spice-checked')
			})
			.on('spice.validator-success', function(){
				var $self = $(this)
					, $label = $self.closest('label')
				if( $self.closest('.spice-form-group').hasClass('spice-success') ){
					$label.addClass('spice-checked')
				}else{
					$label.removeClass('spice-checked')
				}
			})
			if( spice.boolean(this.data.isDefault) ){
				$(':checkbox', elem).closest('.spice-form-group').hasClass('spice-success')
				$(':checkbox', elem).closest('label').addClass('spice-checked')
			}
		}
	})

	SlotRewriteInputText.prototype.constructor = SlotRewriteInputText

	module.exports = SlotRewriteInputText

