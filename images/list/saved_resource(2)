/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
	    , dialogFn = __webpack_require__(29)
	    , slotFn = __webpack_require__(150)
	    , spiceModules = __webpack_require__(62)

	__webpack_require__(155)

	var Dialog = function(element, config){
		if( $.isPlainObject(element) ){
			this.element = null
	    	this.config = this.extend(element)
		}else{
			this.element = $(element)
	    	this.config = this.extend(config)
		}
		var widgetConfig = $.parseJSON( $(this.element).attr('spice-data-widget-config') )
			, dialogConfig = this.config.dialogConfig
		if( widgetConfig && $.isPlainObject(widgetConfig) && !$.isEmptyObject(widgetConfig) ){
			this.config.dialogConfig = $.extend(true, {}, dialogConfig, widgetConfig)
		}
	    this.init()
	}

	Dialog.prototype = $.extend(true, {}, new spiceModules(), {
		config: {
			data: ''
			, dialogConfig:{}
			, url: ''
		}
		, init: function(){
			var self = this
			if( $(self.element).length != 0 || (self.config.url && !self.config.data) ){
				self.cacheData = self.loadJSON()	
			}
			self.drawModule()
			self.drawEvents( self.element )
		}
		, drawModule: function( json ){
			var self = this
				, config = self.config
				, data = json || config.data || self.cacheData.content
				, contents = config.contents
				, ctaButtons = config.ctaButtons
				, dialogConfig = config.dialogConfig
			// 如果同一个弹出层是多次调用
			// 则在初始化前先做删除操作
			self.dialog && self.dialog.destroy()
			// 初始化弹出层
			self.dialog = new dialogFn( dialogConfig )
			// 初始化弹出层Slot
			new slotFn($('.spice-dialog-content', self.dialog.elem), {
				data: data
				, sourceReq: self.config.sourceReq
			})
		}
		, drawEvents: function( elem ){
			var self = this
			// 按钮绑定点击事件
			// 点击显示弹出层		
			$(elem || self.element)
				.data('spice.dialog', self.dialog)
				.on('tap', function(){
					self.dialog.show()
				})
		}
	})

	Dialog.prototype.constructor = Dialog

	module.exports = Dialog

