/* 315 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(316)
		, slot = __webpack_require__(171)

	__webpack_require__(317);

	function SemiSimulation(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	SemiSimulation.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function( elem ){
			this.drawEvents( elem );
		}
		, drawModule: function( elem ){}
		, drawEvents: function( elem ){
			$('select:last', elem).on('change', function(){
				var $self = $(this)
					, $cur = $('option:selected', this)
					, optionValue = $cur.attr('value')
					, optionText = $cur.attr('text')
					, stock = $cur.attr('stock') || ""
				if( stock == 'outOfStock' ){
					optionText = '<del>' + optionText + '</del>'
				}
				$('.spice-dropdown-text', $(this).siblings('.spice-dropdown-btn')).html( optionText );
				setTimeout(function(){
					$self.trigger('spice.dropdown-change', {
						elem: $self
						, value: optionValue
						, text: optionText
						, stock: stock
					})
				}, 0)
			})
			// .trigger('change');
		}
	});

	SemiSimulation.prototype.constructor = SemiSimulation;

	module.exports = SemiSimulation;

