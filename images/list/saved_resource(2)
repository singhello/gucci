/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(119)
		, slot = __webpack_require__(110)
		, spice = __webpack_require__(2)

	__webpack_require__(120);

	function Simulation(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	Simulation.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function( elem ){
			this.drawEvents(elem)
		}
		, drawModule: function( elem ){}
		, drawEvents: function( elem ){
			var $grid = $('.spice-item-hover', elem)
			if ( !spice.hasTouch ) {
				$grid.on( 'mouseenter' , function(){
					$(this).addClass('spice-item-hover-active');
				})
