/* 221 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, slotFn = __webpack_require__(222)
		, dropdown = __webpack_require__(189)
		, ymdSelectFn = __webpack_require__(44);

	function YmdSelect(element, config){
		this.config = this.extend(config);
		this.element = $(element);
		var widgetConfig = $.parseJSON( this.element.attr('spice-data-widget-config') )
			, ymdSelectConfig = this.config.ymdSelectConfig
		if( widgetConfig && $.isPlainObject(widgetConfig) && !$.isEmptyObject(widgetConfig) ){
			this.config.ymdSelectConfig = $.extend(true, {}, ymdSelectConfig, widgetConfig)
		}
	    this.init();
	}

	YmdSelect.prototype = $.extend(true, {}, new dropdown(), {
		config:{
			// 默认选中文字
			defaultSelectVal: []
			// 模板名
			, slot: 'slot-select'
			, ymdSelectConfig: {
				// 默认文字
				defaultText: ['年', '月', '日']
				// 年开始结束时间
				, year: [1900, 2016]
			}
		}
		, drawModule: function( json ){
			var self = this
				, $elem = self.element
				, config = self.config
				, data = {
					slot: config.slot
				};

			$.each(config.ymdSelectConfig.defaultText, function(i, o){
				var $_elem = $elem;
				// 如果 $elem 的长度大于 1
				// 就往不同的 elem 里面增加元素
				if( $elem.length > 1 ){
					$_elem = $elem.eq(i);
				}
				// 初始化Slot
				new slotFn($_elem, {
					data: data
					, sourceReq: config.sourceReq
					, defaultValue: o
					, defaultSelectVal: config.defaultSelectVal[i]
				});
			});

		}
		, drawEvents: function(elem){
			var self = this
				, $elem = elem
				, config = self.config;
			if( $elem.length > 1 ){
				$elem = $elem.end();
			}

			var ymdSelect = new ymdSelectFn($elem, $.extend(true, {}, {
				dropdownHtml: function(text, curval, isFirst){
					var slot = new slotFn('dropdownHtml', {
										sourceReq: config.sourceReq
									});
					return slot._drawSlotModuleEvents({
								data: {
									slot: config.slot
								}
								, text: text
								, curval: curval
								, isFirst: isFirst
							});
				}
			}, config.ymdSelectConfig));
			
			ymdSelect.elem.on('spice.ymdSelect-show', function(){
				$(window).trigger('resize.Tinyscrollbar')
			});
		}
	});

	YmdSelect.prototype.constructor = YmdSelect;

	module.exports = YmdSelect;

