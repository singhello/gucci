/***/ }),
/* 487 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, template = __webpack_require__(488)
		, slot = __webpack_require__(485)
		, spice = __webpack_require__(2)
		, dialogeffect = __webpack_require__(29)
	__webpack_require__(489);

	function Simulation(config){
		this.config = $.extend(true, {}, this.config, config);
		this.template = template;
		this.data = this.config;
	}

	Simulation.prototype = $.extend(true, {}, new slot(), {
		config:{}
		, init: function( elem ){
			this.drawEvents(elem)
		}
		, drawModule: function( elem ){}
		, drawEvents: function( elem ){
			var $self = $(elem)
				, $window = $(window)
				, $videoContainer = $('.spice-editorial-video-content', $self)
				, $videoBtn = $('.e-homepage-video', $self)
				, $loopVideoContent = $('.homepage-video-content-loop', $self)
				, data = this.data

			if (data.hasFlash) {
				var loopVideoTimer = null
					, setLoopVideoEvents = function(){
						var $div = $('.homepage-video-content-loop>div', $self)
							, $object =  $('.homepage-video-content-loop object', $self)
						if( $object.length > 0 ){
							var player = getSWF( $object.attr('id') )
								, flag = $loopVideoContent.data('flag')
							$window.on('scroll.brandPanel',function(){
								spice.debounce(function(){
									var $cover = $self
										, lock = false
										, scrollStart = $cover.offset().top - $window.height()
						    			, scrollEnd = scrollStart + $cover.innerHeight() + $window.height()
						    			, scrollNow = $window.scrollTop()
						    			, scrollH = scrollEnd - scrollStart

						    		if ( scrollNow > scrollStart && scrollNow <= scrollEnd ) {
						    			if ( player.start && !$videoContainer.hasClass('is-loop') ) {
						    				if (!flag) {
						    					var eW = $videoContainer.width()
													, eH = $videoContainer.height()
													, loopHeight = Math.ceil(eW * parseInt(data.loopVideoData.height) / parseInt(data.loopVideoData.width) ) / eH * 100
												
												$loopVideoContent.css({
													'height': loopHeight + '%'
													, 'top': '-' + (loopHeight-100)/2 + '%'
												})
												setTimeout(function(){
													player.start()
													player.setVolume(0)
													flag = false;
												}, 0)
											}
										}
						    		}

								},400,"immediate")();
							});
						} else{
							clearTimeout(loopVideoTimer)
							loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
						}
					}

				if ( $loopVideoContent.length > 0 ) {
					if ( spice.hasTouch ) {
						$loopVideoContent.remove()
					} else{
						loopVideoTimer = setTimeout(setLoopVideoEvents, 400)
					}
				}

				$loopVideoContent.data('setting', {
					config: {
						control_enable: 0
						, loading_enable: 0
						, alert_enable: 0
						, bigbutton_enable: 0
						, rightmenu_enable: 0
						, tip_enable: 0
						, recommend_enable: 0
						, rightmenu_enable: 0
					}
					, startFn: function(){
						$videoContainer.addClass('is-loop')
						$loopVideoContent.data('flag', true)
						return;
					}
					, stopFn: function(){
						var $div = $('>div', $loopVideoContent)
							, $object =  $('object', $loopVideoContent)
							, player = getSWF( $object.attr('id') )
							, count = $loopVideoContent.data('count')
							, maxCount = $loopVideoContent.attr('spice-data-max-count')
						if ( maxCount ) {
							count = count ? count : 0;
							if ( count >= maxCount ) {
								$videoContainer.removeClass('is-loop')
								return
							} else{
								$loopVideoContent.data('count', count + 1)
								player.start()
							}
						} else{
							player.start()
						}
						return;
					}
				})

				function getSWF(objectId) { 
					if (window.document[ objectId ]) {
					    return window.document[ objectId ];
					} else if (navigator.appName.indexOf("Microsoft") == -1) {
					    if (document.embeds && document.embeds[ objectId ]) {
					    	return document.embeds[ objectId ];
					    }
					} else {
						return document.getElementById( objectId );
					}
				}


				// 视频弹层
				if ( data.flashUrl ) {
					var videoDialogTimer = null
						, flashUrl = data.flashUrl.replace('false', 'true')

					var videoDialogHtml  = '<div class="slot-extend-video-standard-slot" spice-data-video-type="brandPanel_'+ data.moduleIndex +'">'
						videoDialogHtml += '<div>'
						videoDialogHtml += '<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,0,0" id="brand_panel_normalVideo_'+ data.moduleIndex +'">'
						videoDialogHtml += '<param name="movie" value="' + flashUrl + '" />'
						videoDialogHtml += '<param name="allowFullScreen" value="true" />'
						videoDialogHtml += '<param name="allowScriptAccess" value="always" />'
						videoDialogHtml += '<param value="transparent" name="wmode" />'
						videoDialogHtml += '<embed src="' + flashUrl + '" width="100%" height="100%" name="brand_panel_normalVideo_'+ data.moduleIndex +'" allowFullScreen="true" wmode="transparent" allowScriptAccess="always" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash"/>'
						videoDialogHtml += '</object>'
						videoDialogHtml += '</div>'
						videoDialogHtml += '</div>'
						videoDialogHtml += '<img class="spice-stories-video-dialog-bg" src="'+ S.staticbase +'/images/autoReplaceImageSrc/stories-video-dialog-bg.png" />'

					var videoDialog = new dialogeffect({
							template: videoDialogHtml
							, dialogClass: 'slot-extend-video-standard'
							, callback: function (e) {
								$('.slot-extend-video-standard-slot', e.elem).data('setting', {
									config: {
										rightmenu_enable: 0
										, tip_enable: 0
										, recommend_enable: 0
										, rightmenu_enable: 0
									}
								})
								var setVideoDialogEvents = function(){
									var $div = $('.slot-extend-video-standard-slot', e.elem)
										, $object =  $('.slot-extend-video-standard-slot object', e.elem)
									if($object.length > 0 ){
										var player = getSWF( $object.attr('id') )

										e.elem.on('spice.dialog-show-before', function(e, o){
											$loopVideoContent.remove()
											var setPlayTimer = null
												, setPlay = function(){
													if ( player.start ) {
														player.start()
														player.setVolume(0.2)
													} else{
														clearTimeout(setPlayTimer)
														setPlayTimer = setTimeout(setPlay, 600)
													}
												}
											setPlayTimer = setTimeout(setPlay, 600)
										});

										e.elem.on('spice.dialog-hide', function(e, o){
											player.pause()
										});
									} else{
										clearTimeout(videoDialogTimer)
										videoDialogTimer = setTimeout(setVideoDialogEvents, 400)
									}
								}
								videoDialogTimer = setTimeout(setVideoDialogEvents, 400)
							}
						});

					$videoBtn.on('tap', function(){
						videoDialog.show()
					})
				}
			} else{
				var $divLoop = $('.homepage-video-content-loop>div', $self)
					, $videoLoop =  $('.homepage-video-content-loop video', $self)
					, videoLoop = $videoLoop[0]

				if ( videoLoop ) {
					videoLoop.onplay = function(){
						$videoContainer.addClass('is-loop')
					}
					videoLoop.onended = function(){
						var count = $loopVideoContent.data('count')
							, maxCount = $loopVideoContent.attr('spice-data-max-count')
						if ( maxCount ) {
							count = count ? count : 0;
							if ( count >= maxCount ) {
								$videoContainer.removeClass('is-loop')
								return
							} else{
								$loopVideoContent.data('count', count + 1)
								videoLoop.play()
							}
						} else{
							videoLoop.play()
						}
						return;
					}
					videoLoop.oncanplay = function(){
						var flag = true
							, eW = $self.width()
							, eH = $self.height()
							, loopHeight = Math.ceil(eW * parseInt(data.loopVideoData.height) / parseInt(data.loopVideoData.width) ) / eH * 100
						$loopVideoContent.css({
							'height': loopHeight + '%'
							, 'top': '-' + (loopHeight-100)/2 + '%'
						})

						$window.on('scroll.brandPanel',function(){
							spice.debounce(function(){
								var $cover = $self
									, lock = false
									, scrollStart = $cover.offset().top - $window.height()
					    			, scrollEnd = scrollStart + $cover.innerHeight() + $window.height()
					    			, scrollNow = $window.scrollTop()
					    			, scrollH = scrollEnd - scrollStart

					    		if ( scrollNow > scrollStart && scrollNow <= scrollEnd ) {
					    			if ( !$videoContainer.hasClass('is-loop') ) {
					    				if (flag) {
											setTimeout(function(){
												videoLoop.volume = 0
												videoLoop.play()
												flag = false;
											}, 0)
										}
									}
					    		}

							},400,"immediate")();
						}).trigger('scroll.brandPanel');
					}
				}

				// 视频弹层
				var isMob = false
				if ( $(window).width() <= 767 ) {
					isMob = true
				}
				var videoUrl = data.isPC ? data.videoUrlHigh : data.videoUrlLow
				if ( videoUrl ) {
					if (isMob) {
						var videoDialogHtml  = '<div class="homepage-video-content" spice-data-video-type="brandPanel_'+ data.moduleIndex +'">'
							videoDialogHtml += '<div>'
							videoDialogHtml += '<video src="'+ videoUrl +'" preload="none"></video>'
							videoDialogHtml += '</div>'
							videoDialogHtml += '</div>'
						$('.spice-editorial-video-content', $self).append( $(videoDialogHtml) )

						var $video =  $('.homepage-video-content video', $self)
							, video = $video[0]
						if (video) {
							video.controls = false
							S.setFullscreenEvents( video, function(e) {
								if ( $videoContainer.hasClass('is-play') ) {
									$videoContainer.removeClass('is-play')
									setTimeout(function(){
										video.pause()
									},0)
								}else{
									$videoContainer.removeClass('is-loop').addClass('is-play')
									$('.homepage-video-content-loop', $self).remove()
									setTimeout(function(){
										video.play()
									},0)
								}
							})
							// video.onplay = function(){
							// 	$videoContainer.removeClass('is-loop').addClass('is-play')
							// 	$('.homepage-video-content-loop', $self).remove()
							// }
							// video.onended = function(){
							// 	$videoContainer.removeClass('is-play')
							// }
							// video.onpause = function(){
							// 	$videoContainer.removeClass('is-play')
							// }
							$videoBtn.on('tap', function(){
								video.play()
								isMob && S.launchFullscreen( video )
							})
							if (spice.iPhone) {
								video.onpause = function(){
									$videoContainer.removeClass('is-play')
								}
							}
						}
					} else{
						var videoDialogHtml  = '<div class="slot-extend-video-standard-slot" spice-data-video-type="brandPanel_'+ data.moduleIndex +'">'
							videoDialogHtml += '<div>'
							videoDialogHtml += '<video src="'+ videoUrl +'"></video>'
							videoDialogHtml += '</div>'
							videoDialogHtml += '</div>'
							videoDialogHtml += '<img class="spice-stories-video-dialog-bg" src="'+ S.staticbase +'/images/autoReplaceImageSrc/stories-video-dialog-bg.png" />'

						var videoDialog = new dialogeffect({
								template: videoDialogHtml
								, dialogClass: 'slot-extend-video-standard'
								, callback: function (e) {
									var $video =  $('video', e.elem)
										, video = $video[0]
										, $slot = $('.slot-extend-video-standard-slot', e.elem)

									if (video) {
										video.controls = true;

										e.elem.on('spice.dialog-show-after', function(e, o){
											$videoContainer.removeClass('is-loop')
											$('.homepage-video-content-loop', $self).remove()
											$video.width( $slot.width() )
											$video.height( $slot.height() )
											$video.fadeIn(function(){
												video.play()
											})
										});

										e.elem.on('spice.dialog-hide', function(e, o){
											video.pause()
										});

									}
									
								}
							});

						$videoBtn.on('tap', function(){
							videoDialog.show()
						})
					}
				}
			}

			// 下划线
			if ( $('.cta-sub-title', $self).length > 0 || $('.cta-copy', $self).length > 0 ) {
				$('.cta-title', $self).after('<span class="cta-underline"></span>')
			}
		}
		, _drawModule: function(){}
	});

	Simulation.prototype.constructor = Simulation;

	module.exports = Simulation;

