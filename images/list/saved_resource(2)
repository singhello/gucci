				data : data.content
				, sourceReq: self.config.sourceReq
			})

			if ( $(window).width() > 1024 ) {
				var slotSelector = $('.spice-offset-content',$elem)
					, offsetInstance = new offsetFn(slotSelector,self.config)

				self.instance = offsetInstance;
			}

			var vignettesData = data.content.vignettes
			if ( vignettesData ) {
