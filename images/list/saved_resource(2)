/* 386 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3),
	    spice = __webpack_require__(2);

	var tempAry = [];

	var animationFrame =  (function(window){
	                        　　return window.requestAnimationFrame ||    //IE10以及以上版本，以及最新谷歌，火狐版本
	                        　　　　　　window.webkitRequestAnimationFrame ||   //谷歌老版本
	                        　　　　　　window.mozRequestAnimationFrame ||   //火狐老版本
	                    　　　　　　　　function(callback){    //IE9以及以下版本
	                    　　　　　　　　　　window.setTimeout(callback , 1000/60);  //这里强制让动画一秒刷新60次，这里之所以设置为16.7毫秒刷新一次，是因为requestAnimationFrame默认也是16.7毫秒刷新一次。
	                    　　　　　　　　}
	                        })(window);
	var cancelAnimationFrame = (function(window){
	        return window.cancelAnimationFrame || window.mozCancelAnimationFrame || window.webkitCancelAnimationFrame || function(id){
	            clearTimeout(id);
	        };
	    })(window);

	function isIE(){
	    // return !-[1,];
	    return !!window.ActiveXObject || "ActiveXObject" in window;
	}

	function Offset(selector,options){
	    if(!(this instanceof Offset)) return new Offset(selector,options);
	    this.selector = selector;
	    this.elem = typeof this.selector === 'string' ? $(selector) : this.selector;
	    this.defineOption = options;
	    this.options = $.extend(true,{},this.options,options);
	    this.top =  0;
	    this.debounceOpt ={};
	    this.instanceNum = tempAry.push({selector:this.selector,content:this,handle:''});
	    this.init();
	};
	Offset.prototype = $.extend(true,{},{
	    options:{
	        config : spice.config,
	        standard: {
	            open:true,
	            offsetBottomStart: 200,
	            offsetBottomEnd: 200,
	            offset: {
	                top: 0,
	                bottom: 0,
	            },
	            checkRetina:false,
	            fn:''
	        },
	        standardRetina: {
	            open:true,
	            offsetBottomStart: 200,
	            offsetBottomEnd: 200,
	            offset: {
	                top: 0,
	                bottom: 0,
	            },
	            checkRetina:false,
	            fn:''
	        },
	        medium:{
	            open:false,
	            offsetBottomStart: 0,
	            offsetBottomEnd: 0,
	            offset: {
	                top: 0,
	                bottom: 0,
	            },
	            checkRetina:false,
	            fn:''
	        },
	        small:{
	            open:false,
	            offsetBottomStart: 0,
	            offsetBottomEnd: 0,
	            offset: {
	                top: 0,
	                bottom: 0,
	            },
	            checkRetina:false,
	            fn:''
	        }
	    },
	    animationFrame : function(fn){
	        return animationFrame(fn);
	    },
	    cancelAnimationFrame:function(id){
	        cancelAnimationFrame(id);
	    },
	    initFlag:true,
	    init:function(){
	        var self = this;
	        self.isIE = isIE();
	        this.loadMedia(function(){
	            self.computeDebounce();
	        });
	        if(document.body.style.transform!==undefined){
	          this.supportTransform = true;
	        }else{
	          this.supportTransform = false;
	        }
	    },
	    instanceAry : function(){
	        return tempAry;
	    },
	    loadMedia:function(fn){
	      var self = this,$imgDom;
	      if(($imgDom=self.elem.find('img')).length>0){
	        $imgDom
	        .off('load')
	        .on('load',function(){
	         fn();
	        })
	    }else{
	        fn();
	    }
	    },
	    computeDebounce:function(){
	        var self = this, options = self.options, lastMedia = '', content;
	        var debounce = spice.resizeScreenDebounce({
	        	checkRetina: options.checkRetina || false,
	        	callBack: function(media){
	                if( lastMedia !== media){
	                    self.debounceOpt = options[media];
	                    if( self.debounceOpt && self.debounceOpt.open ){
	                        self.addScrollEvent();
	                    }else{
	                        self.destory();
	                    }
	                }
	                lastMedia = media;
	            }
	        });
	        $(window)
	            .off('resize.move'+this.instanceNum)
	            .on('resize.move'+this.instanceNum,function(){
	                debounce();
	            })
	            .trigger('resize.move'+this.instanceNum);
	    },
	    addScrollEvent:function(){
	        var self = this,
	        timeFlag = true,obj,
	        handle = self.handle(),
	        flag = true,
	        count = 0,
	        timer,
	        lentionPosYStart = self.elem.offset().top-window.innerHeight,
	        lentionPosYEnd = lentionPosYStart + self.elem.outerHeight(true)+window.innerHeight,
	        timeOutTimer;
	        function animate (){
	            timeFlag = true;
	            obj = {};
	            obj.scrollTop = $(document).scrollTop();
	            obj.scrollLeft = $(document).scrollLeft();
	            handle(obj);
	            if(self.isIE){
	                timer = self.animationFrame(animate);
	            }
	        };
	        console.log(lentionPosYStart,lentionPosYEnd,lentionPosYEnd-lentionPosYStart)
	        $(window)
	            .off('scroll.move'+self.instanceNum)
	            .on('scroll.move'+self.instanceNum,function(e){

	                var tempScrollTop = $(document).scrollTop();
	                if(tempScrollTop >= lentionPosYStart && tempScrollTop <= lentionPosYEnd && flag){
	                    if(self.isIE){
	                        flag = false;
	                    }
	                    animate ();
	                }
	                if(self.isIE){
	                    if((tempScrollTop>lentionPosYEnd ||tempScrollTop < lentionPosYStart) && !flag ){
	                        flag = true;
	                        self.cancelAnimationFrame(timer);
	                    }
	                    if(timeOutTimer)clearTimeout(timeOutTimer);
	                    timeOutTimer = setTimeout(function(){
	                        flag = true;
	                        self.cancelAnimationFrame(timer);
	                    },100);
	                }

	            })
	            .trigger('scroll.move'+self.instanceNum);

	            // if (isIE()) {
				// 	$(document).off('mousewheel').on('mousewheel', function () {
				// 		var wheelDelta = event.wheelDelta,
				// 			currentScrollPosition = window.pageYOffset;
	            //
				// 		window.scrollTo(0, currentScrollPosition - wheelDelta);
	            //
				// 		//event.preventDefault();
				// 	});
				// }
	    },
	    handle:function(){
	        var self = this,
	            $elem = self.elem,
	            $parent = $elem.parent(),
	            $Pparent = $parent.parent(),
	            pHeight = parseInt($parent.height()),
	            pOffset = $parent.offset(),
	            sHeight = parseInt($elem.height()),
	            sOffset = $elem.offset(),
	            options = self.debounceOpt,
	            winHeight = window.innerHeight,
	            fn = $.isFunction( options.fn ) ? options.fn : '',
	            currScrollTop = 0,
	            lastTop = 0,
	            pOffsetTop    = pOffset.top,
	            scrollStartY  = pOffsetTop >= winHeight ?
	                            pOffsetTop -  winHeight + options.offsetBottomStart :
	                            pOffsetTop >= options.offsetBottomStart ?
	                            0 :
	                            options.offsetBottomStart -( winHeight - pOffsetTop ) ,
	            scrollEndY =    pOffsetTop >= winHeight ?
	                            pOffsetTop + pHeight + options.offsetBottomEnd - winHeight :
	                            pOffsetTop+pHeight < winHeight ?
	                            scrollStartY + pHeight :
	                            pHeight + pOffsetTop - winHeight + options.offsetBottomEnd,
	            parentSlideHeight = scrollEndY - scrollStartY,
	            selfSlideHeight = pHeight - sHeight,
	            ratio = selfSlideHeight/parentSlideHeight,
	            endTop = pHeight - options.offset.bottom -sHeight;
	            if(!($elem.hasClass('spice-offset-wrapper'))){
	                $elem.addClass('spice-offset-wrapper');
	            }

	            var points = [],startPointTop = options.offset.top,timeNum = 0;

	            function log10(val) {
	               if (log10 in Math) return Math.log10(val);
	                return Math.log(val)/Math.LN10;
	            };
	            var moduleHeight = $('.e-spice-giant-look').height()-250,
	                moduleScrollPos = $('.e-spice-giant-look').position().top - 50,
	                exp = log10((moduleHeight-$elem.height()+400)/2)/log10(50),
	                contentBlockPosTop = $('.giant-look-content-block').position().top - sHeight - 50;
	            // 设置初始偏移高度
	            self.setCss( options.offset.top);
	            function handleScrollAction(obj){
	                var tempTop, tempLeft,start = 50,target = 0,step = 2,current = 0,id,absValue,currTop,step = 5,num = 1,finallStep=0;
	                currScrollTop = obj.scrollTop || $(document).scrollTop();
	                // console.log(self.isIE);
	                if(self.isIE){
	                    self.top = tempTop = (currScrollTop-scrollStartY)*ratio;
	                    self.top =  currScrollTop > scrollStartY && currScrollTop < scrollEndY ?
	                                tempTop > options.offset.top && tempTop < endTop  ?
	                                tempTop :
	                                tempTop <= options.offset.top ?
	                                options.offset.top :
	                                endTop  :
	                                currScrollTop <= scrollStartY ?
	                                options.offset.top :
	                                endTop;
	                }else{
	                    var percent = (obj.scrollTop - moduleScrollPos)/moduleHeight*70;
	                    // console.log(percent);
	                    // percent = percent < 0 ? 0 : percent > 100 ? 0 :  percent > 50 ? (100-percent) : percent;
	                    self.top = Math.pow(percent,exp);
	                }
	                if(self.top > contentBlockPosTop){
	                    self.top = contentBlockPosTop ;
	                }
	                if(lastTop === self.top)return;
	                !!fn && fn(self.top);
	                self.setCss(self.top);
	                lastTop = self.top;
	            }
	            return handleScrollAction;
	    },
	    setCss:function(y){
	      var self = this;
	      if(self.supportTransform){
	        self.elem.css({
	          transform:'translate3D(0,'+y+'px,0)'
	        })
	      }else{
	        self.elem.css({
	          top:y
	        })
	      }
	    },
	    destory:function(){
	        var style,self = this ,$dom;
	        // 卸载滚动监听事件
	        $(window).off('scroll.move'+self.instanceNum);
	        // 元素transform清零
	        $dom = $(tempAry[self.instanceNum-1])['selector'];
	        // $dom.css({
	        //     transform:'translate3D(0px,0,0)'
	        // });
	        self.setCss(0)
	        tempAry[self.instanceNum-1].debounceOpt = {};
	    }
	})
	Offset.contructor = Offset;
	module.exports = Offset;


