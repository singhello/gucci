					for (var i = 0; i < vignettesData.length; i++) {
						if ( !vignettesData[i].slot ) {
							vignettesData[i].slot = 'slot-standard'
						}
					}
				} else{
					if ( !vignettesData.slot ) {
						vignettesData.slot = 'slot-standard'
					}
				}
				new vignetteFn( $('.spice-carousel-wrapper li',$elem), {
					defaultDataObj: vignettesData
					, sourceReq: self.config.sourceReq
				});

				$(window).trigger('resize.vignette-scroll')
			}
		}

	} )
