/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, slot = __webpack_require__(171);

	function Slot( element, config ){
		this.config = this.extend(config);
		this.element = element;
		this.init();
	}

	Slot.prototype = $.extend(true, {}, new slot(), {
		reqSpiceReq: __webpack_require__(288)
		, drawModule: function(){
			var self = this
				, defaultData = self.config.data
				, slot = defaultData['slot'];

			if( self.element != 'dropdownHtml' ){
				var $element = $(self.element)
					, content = self.config.index == 0 ? defaultData[self.config.defaultDataStart] : {}
					, defaultValue = self.config.defaultValue
					, defaultSelectVal = self.config.defaultSelectVal
					, data = {};
				data['slot'] = slot;
				data['content'] = content;
				data['defaultValue'] = defaultValue;
				data['defaultSelectVal'] = defaultSelectVal;
				data['labelText'] = self.config.labelText;
				data['dataTerm'] = self.config.dataTerm;
				data['name'] = self.config.name;
				data['index'] = self.config.index;
				data['errorMsg'] = self.config.errorMsg;

				if( $element.length > 0 && data ){
					self.drawSlotModuleEvents( $element, data, 'append' );
				}
			}
		}
		, _drawSlotModuleEvents: function( cfg ){
			var slot = cfg.data.slot
				, slotModule = this.getSlotModule(slot)
				, data = {
					content: cfg.defaultData
					, defaultValue: cfg.defaultValue
					, curval: cfg.curval
				}
				, module = '';
			if( slotModule ){
				var slotObj = new slotModule( data );
				module = slotObj._drawModule();
			}
			return module;
		}
	});

	Slot.prototype.constructor = Slot;

	module.exports = Slot;


