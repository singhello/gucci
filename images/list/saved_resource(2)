	module.exports = function(src, lazySrc){
	    var imgSrc = ''
	    	// , domain_image = window.S ? window.S.staticbase : ''
	    	, domain_image = ''
		if( $.isPlainObject(src) ){
			var srcList = []
			$.each( src, function(key, value){
				srcList.push( '"' + key + '":' + '"' + domain_image + value + '"' )
			})
		    imgSrc = ' spice-data-image-src=\'{' + srcList.join(',') + '}\'' + ( lazySrc ? ' src="' + lazySrc + '"' : '' )
		}else{
			imgSrc = (lazySrc ? ' spice-data-lazy-src="' + domain_image + src + '" src="' + domain_image + lazySrc + '"' : ' src="' + domain_image + src + '"')
		}
		return imgSrc
	}

