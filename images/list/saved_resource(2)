/* 189 */
/***/ (function(module, exports, __webpack_require__) {

	var $ = __webpack_require__(3)
		, slotFn = __webpack_require__(171)
		, spiceModules = __webpack_require__(62);

	__webpack_require__(190);

	function Dropdown(element, config){
		this.config = this.extend(config);
		this.element = $(element); 
		var widgetConfig = $.parseJSON( this.element.attr('spice-data-widget-config') )
			, dropdownConfig = this.config.dropdownConfig
		if( widgetConfig && $.isPlainObject(widgetConfig) && !$.isEmptyObject(widgetConfig) ){
			this.config.dropdownConfig = $.extend(true, {}, dropdownConfig, widgetConfig)
		}   
	    this.init();
	}

	Dropdown.prototype = $.extend(true, {}, new spiceModules(), {
		config:{
			// 模板名，也可直接从数据中获取
			slot: 'slot-select'
			, dropdownConfig: {}
		}
		, drawModule: function( json ){
			var self = this
				, $elem = $(self.element)
				, data = json || self.cacheData;

			if( !data.slot ){
				data.slot = self.config.slot
			}

			if( data ){
				$.each(data.content, function(i, o){
					var $_elem = $elem;
					// 如果 $elem 的长度大于 1
					// 就往不同的 elem 里面增加元素
					if( $elem.length > 1 ){
						$_elem = $elem.eq(i);
					}

					o.dropdownConfig = self.config.dropdownConfig

					// 初始化Slot
					new slotFn($_elem, {
						data: o
						, sourceReq: self.config.sourceReq
					});
				});	
			}
		}
	});

	Dropdown.prototype.constructor = Dropdown;

	module.exports = Dropdown;

/***/ }),
